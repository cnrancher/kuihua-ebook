,CAsDD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '工具介绍'\n};\nexport const metadata = {\n  \"id\": \"rancher2/project-admin/tools/_index\",\n  \"title\": \"工具介绍\",\n  \"description\": \"Rancher 集成了很多 Kubernetes 缺少的监控和运维工具，可以辅助您进行 DevOps 开发工作。Rancher 可以跟其他服务集成，帮助您的集群更高效地运行。工具分为以下几个种类：\",\n  \"source\": \"@site/docs/rancher2/project-admin/tools/_index.md\",\n  \"permalink\": \"/docs/rancher2/project-admin/tools/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/project-admin/tools/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '通知和告警',\n  id: '通知和告警',\n  children: []\n}, {\n  value: '日志',\n  id: '日志',\n  children: []\n}, {\n  value: '监控',\n  id: '监控',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p>{`Rancher 集成了很多 Kubernetes 缺少的监控和运维工具，可以辅助您进行 DevOps 开发工作。Rancher 可以跟其他服务集成，帮助您的集群更高效地运行。工具分为以下几个种类：`}</p>\n    {\n      /* TOC */\n    }\n    <ul>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"#%E9%80%9A%E7%9F%A5%E5%92%8C%E5%91%8A%E8%AD%A6\"\n        }}>{`通知和告警`}</a></li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"#%E6%97%A5%E5%BF%97\"\n        }}>{`日志`}</a></li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"#%E7%9B%91%E6%8E%A7\"\n        }}>{`监控`}</a></li>\n    </ul>\n    {\n      /* /TOC */\n    }\n    <h2 {...{\n      \"id\": \"通知和告警\"\n    }}>{`通知和告警`}</h2>\n    <p>{`通知和告警需要互相配合，才能发挥作用。它们把 Rancher 系统中发生的事件以 Slack、电子邮件、PagerDuty、微信或 webhook 的形式告知用户。`}</p>\n    <p><a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/monitoring-alerting/2.0-2.4/notifiers/_index\"\n      }}>{`通知`}</a>{`的功能是告知用户 Rancher 中正在发生的事件。您可以配置告警，把通知发送给最适合解决这个问题的员工，让他采取行动。Rancher 已经和 Slack、电子邮件、PagerDuty、微信和 webhook 对接，您可以使用这些应用发送通知。`}</p>\n    <p><a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/monitoring-alerting/2.0-2.4/cluster-alerts/_index\"\n      }}>{`告警`}</a>{`是触发 Rancher 发送通知的规则。只有完成了告警的配置，您才会收到通知。您可以设置集群层级的告警或项目层级的告警。`}</p>\n    <h2 {...{\n      \"id\": \"日志\"\n    }}>{`日志`}</h2>\n    <p>{`日志对 Rancher 的日常运维提供了帮助，因为它允许您进行以下操作：`}</p>\n    <ul>\n      <li parentName=\"ul\">{`捕获和分析您的集群状态。`}</li>\n      <li parentName=\"ul\">{`寻找环境的变化趋势。`}</li>\n      <li parentName=\"ul\">{`在集群以外的位置保存日志。`}</li>\n      <li parentName=\"ul\">{`告知下游集群或项目中的异常情况，如容器崩溃、Pod 驱逐、节点死亡等。`}</li>\n      <li parentName=\"ul\">{`让系统调试和问题定位变得更加简单。`}</li>\n    </ul>\n    <p>{`Rancher 可以和 Elasticsearch、 splunk、 kafka、 syslog、 fluentd 等工具集成。详情请参考`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/logging/2.0.x-2.4.x/project-logging/_index\"\n      }}>{`日志`}</a>{`章节。`}</p>\n    <h2 {...{\n      \"id\": \"监控\"\n    }}>{`监控`}</h2>\n    <p><em parentName=\"p\">{`v2.2.0 或更新版可用`}</em></p>\n    <p>{`您可以使用 Rancher 对接`}<a parentName=\"p\" {...{\n        \"href\": \"https://prometheus.io/\"\n      }}>{`Prometheus`}</a>{`，监控集群节点的状态和进程，详细信息参考`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/tools/monitoring/_index\"\n      }}>{`监控`}</a>{`章节。`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}