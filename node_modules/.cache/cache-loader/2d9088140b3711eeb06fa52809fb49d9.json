AhCK,CAAP,CAoCD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '基于角色的访问控制',\n\tdescription: '在 Rancher 中，每个人都以一个用户身份进行身份验证，它是授予您访问 Rancher 权限的登录名。如身份验证中所述，用户可以是本地用户，也可以是外部用户。配置外部身份验证后，在用户页面上显示的用户会发生变化。如果您以本地用户身份登录，则仅显示本地用户。如果您以外部用户身份登录，则同时显示外部用户和本地用户。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'系统管理员指南',\n\t\t'基于角色的访问控制'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/admin-settings/rbac/_index\",\n  \"title\": \"基于角色的访问控制\",\n  \"description\": \"在 Rancher 中，每个人都以一个用户身份进行身份验证，它是授予您访问 Rancher 权限的登录名。如身份验证中所述，用户可以是本地用户，也可以是外部用户。配置外部身份验证后，在用户页面上显示的用户会发生变化。如果您以本地用户身份登录，则仅显示本地用户。如果您以外部用户身份登录，则同时显示外部用户和本地用户。\",\n  \"source\": \"@site/docs/rancher2/admin-settings/rbac/_index.md\",\n  \"permalink\": \"/docs/rancher2/admin-settings/rbac/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/admin-settings/rbac/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar\": \"rancher2\",\n  \"previous\": {\n    \"title\": \"对接 Google OAuth\",\n    \"permalink\": \"/docs/rancher2/admin-settings/authentication/google/_index\"\n  },\n  \"next\": {\n    \"title\": \"全局权限\",\n    \"permalink\": \"/docs/rancher2/admin-settings/rbac/global-permissions/_index\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '用户和角色',\n  id: '用户和角色',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p>{`在 Rancher 中，每个人都以一个`}<strong parentName=\"p\">{`用户`}</strong>{`身份进行身份验证，它是授予您访问 Rancher 权限的登录名。如`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/admin-settings/authentication/_index\"\n      }}>{`身份验证`}</a>{`中所述，用户可以是本地用户，也可以是外部用户。`}</p>\n    <p>{`配置外部身份验证后，在`}<strong parentName=\"p\">{`用户`}</strong>{`页面上显示的用户会发生变化。`}</p>\n    <ul>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`如果您以本地用户身份登录，则仅显示本地用户。`}</p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`如果您以外部用户身份登录，则同时显示外部用户和本地用户。`}</p>\n      </li>\n    </ul>\n    <h2 {...{\n      \"id\": \"用户和角色\"\n    }}>{`用户和角色`}</h2>\n    <p>{`一旦用户登录到 Rancher 中，他们的`}<strong parentName=\"p\">{`授权`}</strong>{`，也就是用户在系统中的访问权限，将由`}<strong parentName=\"p\">{`全局权限`}</strong>{`，`}<strong parentName=\"p\">{`集群角色`}</strong>{`和`}<strong parentName=\"p\">{`项目角色`}</strong>{`决定。`}</p>\n    <ul>\n      <li parentName=\"ul\">\n        <p parentName=\"li\"><a parentName=\"p\" {...{\n            \"href\": \"/docs/rancher2/admin-settings/rbac/global-permissions/_index\"\n          }}>{`全局权限`}</a>{`：`}</p>\n        <p parentName=\"li\">{`定义用户在任何特定集群之外的授权。`}</p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\"><a parentName=\"p\" {...{\n            \"href\": \"/docs/rancher2/admin-settings/rbac/cluster-project-roles/_index\"\n          }}>{`集群和项目角色`}</a>{`：`}</p>\n        <p parentName=\"li\">{`定义用户在分配了角色的集群和项目中的授权。`}</p>\n      </li>\n    </ul>\n    <p>{`全局权限以及集群和项目角色是基于 `}<a parentName=\"p\" {...{\n        \"href\": \"https://kubernetes.io/docs/reference/access-authn-authz/rbac/\"\n      }}>{`Kubernetes RBAC`}</a>{`实现的。因此，权限和角色的底层实现是由 Kubernetes 完成的。`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}