port { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: 'NFS 存储',\n\tdescription: '您必须先配置 NFS 服务器，然后才能将 NFS 存储卷插件用于 Rancher 部署。如果您已经拥有 NFS 共享，则无需配置新的 NFS 服务器即可使用 Rancher 中的 NFS 卷插件。同时，请跳过本章节的其余部分并进行存储添加。虽然您可以将本章节的说明用在其他 Linux 发行版（例如 Debian，RHEL，Arch Linux 等）上，但是本章节的所有内容都是基于 Ubuntu 来演示如何设置 NFS 服务器。有关如何使用其他 Linux 发行版创建 NFS 服务器的官方说明，请查阅发行版的文档。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'集群管理员指南',\n\t\t'存储卷和存储类',\n\t\t'创建存储示例',\n\t\t'NFS 存储'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/cluster-admin/volumes-and-storage/examples/nfs/_index\",\n  \"title\": \"NFS 存储\",\n  \"description\": \"您必须先配置 NFS 服务器，然后才能将 NFS 存储卷插件用于 Rancher 部署。如果您已经拥有 NFS 共享，则无需配置新的 NFS 服务器即可使用 Rancher 中的 NFS 卷插件。同时，请跳过本章节的其余部分并进行存储添加。虽然您可以将本章节的说明用在其他 Linux 发行版（例如 Debian，RHEL，Arch Linux 等）上，但是本章节的所有内容都是基于 Ubuntu 来演示如何设置 NFS 服务器。有关如何使用其他 Linux 发行版创建 NFS 服务器的官方说明，请查阅发行版的文档。\",\n  \"source\": \"@site/docs/rancher2/cluster-admin/volumes-and-storage/examples/nfs/_index.md\",\n  \"permalink\": \"/docs/rancher2/cluster-admin/volumes-and-storage/examples/nfs/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/cluster-admin/volumes-and-storage/examples/nfs/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '配置 NFS 存储',\n  id: '配置-nfs-存储',\n  children: []\n}, {\n  value: '后续操作',\n  id: '后续操作',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p>{`您必须先配置 NFS 服务器，然后才能将 NFS 存储卷插件用于 Rancher 部署。`}</p>\n    <blockquote>\n      <p parentName=\"blockquote\"><strong parentName=\"p\">{`注意：`}</strong></p>\n      <ul parentName=\"blockquote\">\n        <li parentName=\"ul\">\n          <p parentName=\"li\">{`如果您已经拥有 NFS 共享，则无需配置新的 NFS 服务器即可使用 Rancher 中的 NFS 卷插件。同时，请跳过本章节的其余部分并进行`}<a parentName=\"p\" {...{\n              \"href\": \"/docs/rancher2/cluster-admin/volumes-and-storage/_index\"\n            }}>{`存储添加`}</a>{`。`}</p>\n        </li>\n        <li parentName=\"ul\">\n          <p parentName=\"li\">{`虽然您可以将本章节的说明用在其他 Linux 发行版（例如 Debian，RHEL，Arch Linux 等）上，但是本章节的所有内容都是基于 Ubuntu 来演示如何设置 NFS 服务器。有关如何使用其他 Linux 发行版创建 NFS 服务器的官方说明，请查阅发行版的文档。`}</p>\n        </li>\n      </ul>\n      <p parentName=\"blockquote\"><strong parentName=\"p\">{`推荐：`}</strong>{` 为了简化管理防火墙规则的过程，请使用 NFSv4。`}</p>\n    </blockquote>\n    <h2 {...{\n      \"id\": \"配置-nfs-存储\"\n    }}>{`配置 NFS 存储`}</h2>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`使用远程终端连接，登录要用于 NFS 存储的 Ubuntu 服务器。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`输入以下命令：`}</p>\n        <pre parentName=\"li\"><code parentName=\"pre\" {...{\n            \"className\": \"language-bash\"\n          }}>{`sudo apt-get install nfs-kernel-server\n`}</code></pre>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`输入以下命令，该命令用于设置存储的目录以及用户访问权限。 如果要将存储保留在其他目录中，请修改命令。`}</p>\n        <pre parentName=\"li\"><code parentName=\"pre\" {...{\n            \"className\": \"language-bash\"\n          }}>{`mkdir -p /nfs && chown nobody:nogroup /nfs\n`}</code></pre>\n        <ul parentName=\"li\">\n          <li parentName=\"ul\"><inlineCode parentName=\"li\">{`-p /nfs`}</inlineCode>{` 在根目录创建了一个名为 `}<inlineCode parentName=\"li\">{`nfs`}</inlineCode>{` 的子目录。`}</li>\n          <li parentName=\"ul\"><inlineCode parentName=\"li\">{`chown nobody:nogroup /nfs`}</inlineCode>{` 允许所有人可以访问存储目录 `}<inlineCode parentName=\"li\">{`/nfs`}</inlineCode>{`。`}</li>\n        </ul>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`创建一个 NFS 导出表。该表设置了 NFS 服务器上暴露给将使用该服务器进行存储的节点的目录路径。`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">\n            <p parentName=\"li\">{`使用文本编辑器打开 `}<inlineCode parentName=\"p\">{`/etc/exports`}</inlineCode>{`。`}</p>\n          </li>\n          <li parentName=\"ol\">\n            <p parentName=\"li\">{`添加在步骤 3 中创建的 `}<inlineCode parentName=\"p\">{`/nfs`}</inlineCode>{` 文件夹的路径，以及集群节点的 IP 地址。为集群中的每个 IP 地址添加一个条目。在每个地址及其伴随的参数后面加上一个空格，该空格作为定界符。`}</p>\n            <pre parentName=\"li\"><code parentName=\"pre\" {...{\n                \"className\": \"language-bash\"\n              }}>{`/nfs <IP_ADDRESS1>(rw,sync,no_subtree_check) <IP_ADDRESS2>(rw,sync,no_subtree_check) <IP_ADDRESS3>(rw,sync,no_subtree_check)\n`}</code></pre>\n            <p parentName=\"li\"><strong parentName=\"p\">{`提示：`}</strong>{` 您可以用子网替代 IP 地址，例如：`}<inlineCode parentName=\"p\">{`10.212.50.12/24`}</inlineCode>{`。`}</p>\n          </li>\n          <li parentName=\"ol\">\n            <p parentName=\"li\">{`使用以下命令更新 NFS 导出表：`}</p>\n            <pre parentName=\"li\"><code parentName=\"pre\" {...{\n                \"className\": \"language-bash\"\n              }}>{`exportfs -ra\n`}</code></pre>\n          </li>\n        </ol>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`打开 NFS 的端口。`}</p>\n        <ol parentName=\"li\">\n          <li parentName=\"ol\">\n            <p parentName=\"li\">{`可以使用以下命令找出 NFS 正在使用的端口：`}</p>\n            <pre parentName=\"li\"><code parentName=\"pre\" {...{\n                \"className\": \"language-bash\"\n              }}>{`rpcinfo -p | grep nfs\n`}</code></pre>\n          </li>\n          <li parentName=\"ol\">\n            <p parentName=\"li\"><a parentName=\"p\" {...{\n                \"href\": \"https://help.ubuntu.com/lts/serverguide/firewall.html.en\"\n              }}>{`打开`}</a>{`上面命令输出的端口，比如使用下面的命令来打开端口 `}<inlineCode parentName=\"p\">{`2049`}</inlineCode>{`：`}</p>\n            <pre parentName=\"li\"><code parentName=\"pre\" {...{\n                \"className\": \"language-bash\"\n              }}>{`sudo ufw allow 2049\n`}</code></pre>\n          </li>\n        </ol>\n      </li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{` 您的 NFS 服务器已经设置好，可用于 Rancher 作存储服务。`}</p>\n    <h2 {...{\n      \"id\": \"后续操作\"\n    }}>{`后续操作`}</h2>\n    <p>{`在 Rancher 中，将 NFS 服务器添加为`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/volumes-and-storage/_index\"\n      }}>{`存储卷`}</a>{`又或者`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/volumes-and-storage/_index\"\n      }}>{`存储类`}</a>{`。添加服务器后，可以将其用于部署存储。`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}