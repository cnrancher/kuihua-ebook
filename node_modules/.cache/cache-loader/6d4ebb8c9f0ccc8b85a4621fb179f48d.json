AAI,UAAU,CAAC,IAAf,EAAwB,CACtB,QAAS,MADa,CAAxB,EAEK,IAFL,CAJF,CADF,CADF,CAWE,aAAO,UAAU,CAAC,OAAlB,EACE,UAAI,UAAU,CAAC,OAAf,EACE,mBAAI,UAAU,CAAC,IAAf,EAAwB,CACtB,QAAS,MADa,CAAxB,EAEG,kBAAY,UAAU,CAAC,IAAvB,EAA8B,gBAA9B,CAFH,CADF,CAIE,mBAAI,UAAU,CAAC,IAAf,EAAwB,CACtB,QAAS,MADa,CAAxB,EAEK,KAFL,CAEU,kBAAY,UAAU,CAAC,IAAvB,EAA8B,gBAA9B,CAFV,CAEuE,4BAFvE,CAJF,CADF,CAXF,CAzIK,CAAP,CAgKD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '恢复配置',\n\tdescription: 'Restore Create 页面提供要还原的备份的详细信息。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'备份与恢复',\n\t\t'rancher2.5',\n\t\t'恢复配置'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/backups/2.5/configuration/restore-config/_index\",\n  \"title\": \"恢复配置\",\n  \"description\": \"Restore Create 页面提供要还原的备份的详细信息。\",\n  \"source\": \"@site/docs/rancher2/backups/2.5/configuration/restore-config/_index.md\",\n  \"permalink\": \"/docs/rancher2/backups/2.5/configuration/restore-config/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/backups/2.5/configuration/restore-config/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '备份源',\n  id: '备份源',\n  children: [{\n    value: '现有的备份配置',\n    id: '现有的备份配置',\n    children: []\n  }, {\n    value: '默认存储目标',\n    id: '默认存储目标',\n    children: []\n  }, {\n    value: 'S3 兼容的对象存储',\n    id: 's3-兼容的对象存储',\n    children: []\n  }]\n}, {\n  value: '加密',\n  id: '加密',\n  children: []\n}, {\n  value: 'Prune During Restore',\n  id: 'prune-during-restore',\n  children: []\n}, {\n  value: '从 S3 获取备份文件名',\n  id: '从-s3-获取备份文件名',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p>{`Restore Create 页面提供要还原的备份的详细信息。`}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/backup_restore/restore/restore.png\",\n        \"alt\": \"restore\"\n      }}></img></p>\n    <h2 {...{\n      \"id\": \"备份源\"\n    }}>{`备份源`}</h2>\n    <p>{`提供备份文件和备份文件存储位置的详细信息，operator 使用这个文件执行还原。从以下选项中选择这些详细信息。`}</p>\n    <h3 {...{\n      \"id\": \"现有的备份配置\"\n    }}>{`现有的备份配置`}</h3>\n    <p>{`选择该选项将在`}<strong parentName=\"p\">{`目标备份`}</strong>{`下拉菜单中填充该集群中可用的备份。从下拉菜单中选择备份，自动为您填写`}<strong parentName=\"p\">{`备份文件名`}</strong>{`字段，还将把所选 Backup 的备份源信息传递给 operator。`}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/backup_restore/restore/existing.png\",\n        \"alt\": \"existing\"\n      }}></img></p>\n    <p>{`如果 Backup 自定义资源在集群中不存在，则需要获取准确的文件名，并提供备份源详细信息，并提供默认的存储目标或与 S3 兼容的对象存储。`}</p>\n    <h3 {...{\n      \"id\": \"默认存储目标\"\n    }}>{`默认存储目标`}</h3>\n    <p>{`如果您要从 operator 级别配置的默认存储位置的备份文件中恢复，请选择此选项。operator 级别的配置是指安装或升级 `}<inlineCode parentName=\"p\">{`rancher-backup`}</inlineCode>{` operator 时配置的存储位置。在`}<strong parentName=\"p\">{`备份文件名`}</strong>{`字段中提供准确的文件名。`}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/backup_restore/restore/default.png\",\n        \"alt\": \"default\"\n      }}></img></p>\n    <h3 {...{\n      \"id\": \"s3-兼容的对象存储\"\n    }}>{`S3 兼容的对象存储`}</h3>\n    <p>{`如果在 operator 级别没有配置默认存储位置，或者如果备份文件与配置为默认存储位置不同的 S3 桶中，请选择此选项。在`}<strong parentName=\"p\">{`备份文件名`}</strong>{`字段中提供准确的文件名。请参阅`}<a parentName=\"p\" {...{\n        \"href\": \"#%E4%BB%8Es3%E8%8E%B7%E5%8F%96%E5%A4%87%E4%BB%BD%E6%96%87%E4%BB%B6%E5%90%8D\"\n      }}>{`本节`}</a>{`了解从 S3 获取备份文件名的具体步骤。填写 S3 兼容对象存储的所有细节。它的字段与`}<a parentName=\"p\" {...{\n        \"href\": \"./../back-up-config/_index#%E5%AD%98%E5%82%A8%E4%BD%8D%E7%BD%AE\"\n      }}>{`备份自定义资源`}</a>{`中`}<inlineCode parentName=\"p\">{`backup.StorageLocation`}</inlineCode>{`配置的字段完全相同。`}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/backup_restore/restore/s3store.png\",\n        \"alt\": \"s3store\"\n      }}></img></p>\n    <h2 {...{\n      \"id\": \"加密\"\n    }}>{`加密`}</h2>\n    <p>{`如果备份是在启用加密的情况下创建的，其文件后缀为`}<inlineCode parentName=\"p\">{`.enc`}</inlineCode>{`。选择这样的备份，或提供后缀为`}<inlineCode parentName=\"p\">{`.enc`}</inlineCode>{`的备份文件名，将显示另一个名为`}<strong parentName=\"p\">{`加密配置 Secret`}</strong>{`的下拉菜单。`}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/backup_restore/restore/encryption.png\",\n        \"alt\": \"encryption\"\n      }}></img></p>\n    <p>{`从该下拉菜单中选择的 Secret 必须与执行备份时用于 Backup 自定义资源的 Secret 内容相同。如果加密配置不匹配，还原将失败。`}</p>\n    <p><inlineCode parentName=\"p\">{`加密配置Secret`}</inlineCode>{`下拉菜单将过滤出并仅列出那些拥有此密钥的 Secret。`}</p>\n    <table>\n      <thead parentName=\"table\">\n        <tr parentName=\"thead\">\n          <th parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`YAML 指令名称`}</th>\n          <th parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`说明`}</th>\n        </tr>\n      </thead>\n      <tbody parentName=\"table\">\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}><inlineCode parentName=\"td\">{`encryptionConfigSecretName`}</inlineCode></td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`提供 `}<inlineCode parentName=\"td\">{`cattle-resources-system`}</inlineCode>{` 命名空间中包含加密配置文件的 Secret 名称。`}</td>\n        </tr>\n      </tbody>\n    </table>\n    <div {...{\n      \"className\": \"admonition admonition-important alert alert--info\"\n    }}><div parentName=\"div\" {...{\n        \"className\": \"admonition-heading\"\n      }}><h5 parentName=\"div\"><span parentName=\"h5\" {...{\n            \"className\": \"admonition-icon\"\n          }}><svg parentName=\"span\" {...{\n              \"xmlns\": \"http://www.w3.org/2000/svg\",\n              \"width\": \"14\",\n              \"height\": \"16\",\n              \"viewBox\": \"0 0 14 16\"\n            }}><path parentName=\"svg\" {...{\n                \"fillRule\": \"evenodd\",\n                \"d\": \"M7 2.3c3.14 0 5.7 2.56 5.7 5.7s-2.56 5.7-5.7 5.7A5.71 5.71 0 0 1 1.3 8c0-3.14 2.56-5.7 5.7-5.7zM7 1C3.14 1 0 4.14 0 8s3.14 7 7 7 7-3.14 7-7-3.14-7-7-7zm1 3H6v5h2V4zm0 6H6v2h2v-2z\"\n              }}></path></svg></span>{`重要`}</h5></div><div parentName=\"div\" {...{\n        \"className\": \"admonition-content\"\n      }}><p parentName=\"div\">{`此字段仅在备份创建时启用了加密功能时才应设置。提供错误的加密配置将导致还原失败。`}</p></div></div>\n    <h2 {...{\n      \"id\": \"prune-during-restore\"\n    }}>{`Prune During Restore`}</h2>\n    <ul>\n      <li parentName=\"ul\"><strong parentName=\"li\">{`Prune`}</strong>{`：为了从备份中完全恢复 Rancher，并回到备份时的确切状态，我们需要删除 Rancher 在备份后创建的任何额外资源。如果`}<strong parentName=\"li\">{`Prune`}</strong>{`标志被启用，operator 就会这样做。Prune 默认是启用的，建议保持启用。`}</li>\n      <li parentName=\"ul\"><strong parentName=\"li\">{`删除超时`}</strong>{`：这是 operator 在删除资源时等待的时间。`}</li>\n    </ul>\n    <table>\n      <thead parentName=\"table\">\n        <tr parentName=\"thead\">\n          <th parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`YAML 指令名称`}</th>\n          <th parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`说明`}</th>\n        </tr>\n      </thead>\n      <tbody parentName=\"table\">\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}><inlineCode parentName=\"td\">{`prune`}</inlineCode></td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`删除备份中不存在的由 Rancher 管理的资源（推荐）。`}</td>\n        </tr>\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}><inlineCode parentName=\"td\">{`deleteTimeoutSeconds`}</inlineCode></td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`operator 在删除资源时等待的时间。`}</td>\n        </tr>\n      </tbody>\n    </table>\n    <h2 {...{\n      \"id\": \"从-s3-获取备份文件名\"\n    }}>{`从 S3 获取备份文件名`}</h2>\n    <p>{`这是 `}<inlineCode parentName=\"p\">{`rancher-backup`}</inlineCode>{` operator 用来执行还原的备份文件的名称。`}</p>\n    <p>{`要从 S3 获取这个文件名，请进入你的 S3 桶（如果在执行备份时指定了文件夹）。`}</p>\n    <p>{`复制文件名并将其存储在你的 Restore 自定义资源中。假设你的备份文件的名字是`}<inlineCode parentName=\"p\">{`backupfile`}</inlineCode>{`：`}</p>\n    <ul>\n      <li parentName=\"ul\">{`如果你的桶名称是`}<inlineCode parentName=\"li\">{`s3bucket`}</inlineCode>{`而没有指定文件夹，那么要使用的`}<inlineCode parentName=\"li\">{`备份文件名称`}</inlineCode>{`将是`}<inlineCode parentName=\"li\">{`backupfile`}</inlineCode>{`。`}</li>\n      <li parentName=\"ul\">{`如果你的桶名称是`}<inlineCode parentName=\"li\">{`s3bucket`}</inlineCode>{`，基础文件夹是`}<inlineCode parentName=\"li\">{`s3folder`}</inlineCode>{`，那么要使用的`}<inlineCode parentName=\"li\">{`备份文件名称`}</inlineCode>{`只有`}<inlineCode parentName=\"li\">{`backupfile`}</inlineCode>{`。`}</li>\n      <li parentName=\"ul\">{`如果在`}<inlineCode parentName=\"li\">{`s3Folder`}</inlineCode>{`内有一个名为`}<inlineCode parentName=\"li\">{`s3sub`}</inlineCode>{`的子文件夹，里面有你的备份文件，那么要使用的`}<inlineCode parentName=\"li\">{`备份文件名称`}</inlineCode>{`就是`}<inlineCode parentName=\"li\">{`s3sub/backupfile`}</inlineCode>{`。`}</li>\n    </ul>\n    <table>\n      <thead parentName=\"table\">\n        <tr parentName=\"thead\">\n          <th parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`YAML 指令名称`}</th>\n          <th parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`说明`}</th>\n        </tr>\n      </thead>\n      <tbody parentName=\"table\">\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}><inlineCode parentName=\"td\">{`backupFilename`}</inlineCode></td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`这是 `}<inlineCode parentName=\"td\">{`rancher-backup`}</inlineCode>{` operator 将用来执行还原的备份文件的名称。`}</td>\n        </tr>\n      </tbody>\n    </table>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}