AhF,CAA2F,kBAAY,UAAU,CAAC,IAAvB,EAA8B,MAA9B,CAA3F,CAA8I,GAA9I,CAHF,CA/BK,CAoCL,aAAK,2CAAL,CAAgD,kBAAG,UAAU,CAAC,GAAd,EAAsB,CAClE,OAAQ,0DAD0D,CAAtB,EAEzC,0BAFyC,CAAhD,CAEuC,GAFvC,CApCK,CAuCL,eAAK,qBAAM,UAAU,CAAC,KAAjB,EAA2B,CAC5B,YAAa,eADe,CAA3B,EAEE;;;;;;;;;;;;;;;;;;;;;;;;;;CAFF,CAAL,CAvCK,CAAP,CAqED,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '启用 API 审计日志',\n\tdescription: '如果使用 RKE 安装 Rancher，则可以使用指令为 Rancher 安装启用 API 审计日志。您可以知道容器中发生了什么事件，何时发生，事件发起人是谁以及它影响了哪些集群。API 审计日志记录了与 Rancher API 之间的所有请求和响应，包括对 Rancher UI 的使用以及通过程序使用对 Rancher API 的任何其他使用。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'安装指南',\n\t\t'资料、参考和高级选项',\n\t\t'RKE Add-on 安装',\n\t\t'启用 API 审计日志'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/installation/options/rke-add-on/api-auditing/_index\",\n  \"title\": \"启用 API 审计日志\",\n  \"description\": \"如果使用 RKE 安装 Rancher，则可以使用指令为 Rancher 安装启用 API 审计日志。您可以知道容器中发生了什么事件，何时发生，事件发起人是谁以及它影响了哪些集群。API 审计日志记录了与 Rancher API 之间的所有请求和响应，包括对 Rancher UI 的使用以及通过程序使用对 Rancher API 的任何其他使用。\",\n  \"source\": \"@site/docs/rancher2/installation/options/rke-add-on/api-auditing/_index.md\",\n  \"permalink\": \"/docs/rancher2/installation/options/rke-add-on/api-auditing/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/installation/options/rke-add-on/api-auditing/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '内联参数',\n  id: '内联参数',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <div {...{\n      \"className\": \"admonition admonition-important alert alert--info\"\n    }}><div parentName=\"div\" {...{\n        \"className\": \"admonition-heading\"\n      }}><h5 parentName=\"div\"><span parentName=\"h5\" {...{\n            \"className\": \"admonition-icon\"\n          }}><svg parentName=\"span\" {...{\n              \"xmlns\": \"http://www.w3.org/2000/svg\",\n              \"width\": \"14\",\n              \"height\": \"16\",\n              \"viewBox\": \"0 0 14 16\"\n            }}><path parentName=\"svg\" {...{\n                \"fillRule\": \"evenodd\",\n                \"d\": \"M7 2.3c3.14 0 5.7 2.56 5.7 5.7s-2.56 5.7-5.7 5.7A5.71 5.71 0 0 1 1.3 8c0-3.14 2.56-5.7 5.7-5.7zM7 1C3.14 1 0 4.14 0 8s3.14 7 7 7 7-3.14 7-7-3.14-7-7-7zm1 3H6v5h2V4zm0 6H6v2h2v-2z\"\n              }}></path></svg></span>{`重要提示`}</h5></div><div parentName=\"div\" {...{\n        \"className\": \"admonition-content\"\n      }}><p parentName=\"div\">{`RKE add-on 安装仅支持 Rancher v2.0.8 之前的版本。\n请使用 Rancher helm chart 将 Rancher 安装在 Kubernetes 集群上。有关详细信息，请参见`}<a parentName=\"p\" {...{\n            \"href\": \"/docs/rancher2/installation/k8s-install/_index\"\n          }}>{`Rancher 高可用安装`}</a>{`。\n如果您当前正在使用 RKE add-on 安装方法，参见`}<a parentName=\"p\" {...{\n            \"href\": \"/docs/rancher2/upgrades/upgrades/migrating-from-rke-add-on/_index\"\n          }}>{`将 RKE add-on 安装的 Rancher 迁移到 Helm 安装`}</a>{`，获取有关如何使用 Helm chart 的详细信息。`}</p></div></div>\n    <p>{`如果使用 RKE 安装 Rancher，则可以使用指令为 Rancher 安装启用 API 审计日志。您可以知道容器中发生了什么事件，何时发生，事件发起人是谁以及它影响了哪些集群。API 审计日志记录了与 Rancher API 之间的所有请求和响应，包括对 Rancher UI 的使用以及通过程序使用对 Rancher API 的任何其他使用。`}</p>\n    <h2 {...{\n      \"id\": \"内联参数\"\n    }}>{`内联参数`}</h2>\n    <p>{`通过向 Rancher 容器添加参数来使用 RKE 启用 API 审计日志。`}</p>\n    <p>{`启用 API 审计日志的的先决条件如下：`}</p>\n    <ul>\n      <li parentName=\"ul\">{`将 API 审计日志 参数 (`}<inlineCode parentName=\"li\">{`args`}</inlineCode>{`) 添加到 Rancher 容器中。`}</li>\n      <li parentName=\"ul\">{`在容器的 `}<inlineCode parentName=\"li\">{`volumeMounts`}</inlineCode>{` 指令中声明一个 `}<inlineCode parentName=\"li\">{`mountPath`}</inlineCode>{`。`}</li>\n      <li parentName=\"ul\">{`在 `}<inlineCode parentName=\"li\">{`volumes`}</inlineCode>{` 指令中声明一个 `}<inlineCode parentName=\"li\">{`path`}</inlineCode>{`。`}</li>\n    </ul>\n    <p>{`有关每个参数的说明、使用参数的语法以及如何查看 API 审计日志的更多信息，请参阅`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/installation/options/api-audit-log/_index\"\n      }}>{`Rancher v2.0 文档：API 审计日志`}</a>{`。`}</p>\n    <pre><code parentName=\"pre\" {...{\n        \"className\": \"language-yaml\"\n      }}>{`...\ncontainers:\n        - image: rancher/rancher:latest\n          imagePullPolicy: Always\n          name: cattle-server\n          args: [\"--audit-log-path\", \"/var/log/auditlog/rancher-api-audit.log\", \"--audit-log-maxbackup\", \"5\", \"--audit-log-maxsize\", \"50\", \"--audit-level\", \"2\"]\n          ports:\n          - containerPort: 80\n            protocol: TCP\n          - containerPort: 443\n            protocol: TCP\n          volumeMounts:\n          - mountPath: /etc/rancher/ssl\n            name: cattle-keys-volume\n            readOnly: true\n          - mountPath: /var/log/auditlog\n            name: audit-log-dir\n        volumes:\n        - name: cattle-keys-volume\n          secret:\n            defaultMode: 420\n            secretName: cattle-keys-server\n        - name: audit-log-dir\n          hostPath:\n            path: /var/log/rancher/auditlog\n            type: Directory\n`}</code></pre>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}