onst frontMatter = {\n\ttitle: '简介',\n\tdescription: '本文提供了使用 AWS EC2 自动缩放组在 Rancher 自定义集群上安装和使用Kubernetes cluster-autoscaler的操作指导。集群自动缩放器是一个工具，当集群满足以下条件之中的任意一条时，Rancher 会自动调整 Kubernetes 集群的大小：集群中存在因资源不足而无法运行的 pod，或集群中存在长时间未被充分利用的节点，其 pod 可以放在其他现有节点上。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'集群管理员指南',\n\t\t'集群弹性伸缩',\n\t\t'简介'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/cluster-admin/cluster-autoscaler/_index\",\n  \"title\": \"简介\",\n  \"description\": \"本文提供了使用 AWS EC2 自动缩放组在 Rancher 自定义集群上安装和使用Kubernetes cluster-autoscaler的操作指导。集群自动缩放器是一个工具，当集群满足以下条件之中的任意一条时，Rancher 会自动调整 Kubernetes 集群的大小：集群中存在因资源不足而无法运行的 pod，或集群中存在长时间未被充分利用的节点，其 pod 可以放在其他现有节点上。\",\n  \"source\": \"@site/docs/rancher2/cluster-admin/cluster-autoscaler/_index.md\",\n  \"permalink\": \"/docs/rancher2/cluster-admin/cluster-autoscaler/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/cluster-admin/cluster-autoscaler/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar\": \"rancher2\",\n  \"previous\": {\n    \"title\": \"将用户添加到集群中\",\n    \"permalink\": \"/docs/rancher2/cluster-admin/cluster-access/cluster-members/_index\"\n  },\n  \"next\": {\n    \"title\": \"使用 AWS Auto Scaling 组进行集群弹性伸缩\",\n    \"permalink\": \"/docs/rancher2/cluster-admin/cluster-autoscaler/amazon/_index\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '概述',\n  id: '概述',\n  children: []\n}, {\n  value: '支持此功能的云供应商',\n  id: '支持此功能的云供应商',\n  children: [{\n    value: '在 AWS 上设置集群 autoscaler',\n    id: '在-aws-上设置集群-autoscaler',\n    children: []\n  }]\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <h2 {...{\n      \"id\": \"概述\"\n    }}>{`概述`}</h2>\n    <p>{`本文提供了使用 AWS EC2 自动缩放组在 Rancher 自定义集群上安装和使用`}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/kubernetes/autoscaler/blob/master/cluster-autoscaler/\"\n      }}>{`Kubernetes cluster-autoscaler`}</a>{`的操作指导。`}</p>\n    <p>{`集群自动缩放器是一个工具，当集群满足以下条件之中的任意一条时，Rancher 会自动调整 Kubernetes 集群的大小`}</p>\n    <ul>\n      <li parentName=\"ul\">{`集群中存在因资源不足而无法运行的 pod。`}</li>\n      <li parentName=\"ul\">{`集群中存在长时间未被充分利用的节点，其 pod 可以放在其他现有节点上。`}</li>\n    </ul>\n    <p>{`为了防止 pod 被驱逐，您需要在 pod 规范上设置`}<inlineCode parentName=\"p\">{`priorityClassName: system-cluster-critical`}</inlineCode>{`属性。`}</p>\n    <p>{`Cluster Autoscaler 在 Kubernetes 主节点上运行。它可以运行在`}<inlineCode parentName=\"p\">{`kube-system`}</inlineCode>{`命名空间中。Cluster Autoscaler 不会缩减在其上运行非镜像`}<inlineCode parentName=\"p\">{`kube-system`}</inlineCode>{`pod 的节点。`}</p>\n    <p>{`您可以在 worker 节点上运行 Cluster Autoscaler 的定制部署，但需要格外小心，以确保 Cluster Autoscaler 保持运行。`}</p>\n    <h2 {...{\n      \"id\": \"支持此功能的云供应商\"\n    }}>{`支持此功能的云供应商`}</h2>\n    <p>{`Cluster Autoscaler 提供对不同云提供商的支持。有关更多信息，请访问`}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/kubernetes/autoscaler/tree/master/cluster-autoscaler#deployment\"\n      }}>{`cluster-autoscaler 支持的云提供商。`}</a>{`。`}</p>\n    <h3 {...{\n      \"id\": \"在-aws-上设置集群-autoscaler\"\n    }}>{`在 AWS 上设置集群 autoscaler`}</h3>\n    <p>{`有关在 AWS 上运行集群弹性伸缩的详细操作指导，请参考`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/cluster-autoscaler/amazon/_index\"\n      }}>{`本页`}</a>{`。`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}