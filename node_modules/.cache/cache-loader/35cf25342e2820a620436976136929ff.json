K,CAsBL,aAAK,yCAAL,CAtBK,CAuBL,SAAQ,CACN,KAAM,uBADA,CAAR,CAEK,uBAFL,CAvBK,CA0BL,aAAK,yGAAL,CA1BK,CA2BL,SAAQ,CACN,KAAM,UADA,CAAR,CAEK,WAFL,CA3BK,CA8BL,SAAQ,CACN,KAAM,wBADA,CAAR,CAEK,wBAFL,CA9BK,CAiCL,aAAK,2FAAL,CAjCK,CAkCL,SAAQ,CACN,KAAM,iBADA,CAAR,CAEK,iBAFL,CAlCK,CAqCL,aAAK,qEAAL,CArCK,CAsCL,SAAQ,CACN,KAAM,UADA,CAAR,CAEK,WAFL,CAtCK,CAyCL,SAAQ,CACN,KAAM,qBADA,CAAR,CAEK,qBAFL,CAzCK,CA4CL,aAAK,kCAAL,CA5CK,CA6CL,SAAQ,CACN,KAAM,WADA,CAAR,CAEK,YAFL,CA7CK,CAgDL,SAAQ,CACN,KAAM,YADA,CAAR,CAEK,YAFL,CAhDK,CAmDL,aAAK,iEAAL,CAnDK,CAoDL,SAAQ,CACN,KAAM,WADA,CAAR,CAEK,WAFL,CApDK,CAuDL,aAAK,gCAAL,CAvDK,CAAP,CAyDD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: 'Rancher托管vSphere集群的最佳实践',\n\tdescription: 'description',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'最佳实践',\n\t\t'Rancher托管vSphere集群的最佳实践'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/best-practices/2.5/rancher-managed/managed-vsphere/_index\",\n  \"title\": \"Rancher托管vSphere集群的最佳实践\",\n  \"description\": \"description\",\n  \"source\": \"@site/docs/rancher2/best-practices/2.5/rancher-managed/managed-vsphere/_index.md\",\n  \"permalink\": \"/docs/rancher2/best-practices/2.5/rancher-managed/managed-vsphere/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/best-practices/2.5/rancher-managed/managed-vsphere/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar\": \"rancher2\",\n  \"previous\": {\n    \"title\": \"日志最佳实践\",\n    \"permalink\": \"/docs/rancher2/best-practices/2.5/rancher-managed/logging/_index\"\n  },\n  \"next\": {\n    \"title\": \"Rancher管理集群的最佳实践\",\n    \"permalink\": \"/docs/rancher2/best-practices/2.5/rancher-managed/_index\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '1. 虚拟机注意事项',\n  id: '1-虚拟机注意事项',\n  children: [{\n    value: '充分利用模板来构建环境',\n    id: '充分利用模板来构建环境',\n    children: []\n  }, {\n    value: '利用 DRS 反亲和规则（如果可能）在 ESXi 主机上分离下游集群节点',\n    id: '利用-drs-反亲和规则（如果可能）在-esxi-主机上分离下游集群节点',\n    children: []\n  }, {\n    value: '利用 DRS 反亲和规则（如果可能）在整个数据存储区中分离下游集群节点',\n    id: '利用-drs-反亲和规则（如果可能）在整个数据存储区中分离下游集群节点',\n    children: []\n  }]\n}, {\n  value: '2. 网络注意事项',\n  id: '2-网络注意事项',\n  children: [{\n    value: '为虚拟机提供固定的 IP 地址',\n    id: '为虚拟机提供固定的-ip-地址',\n    children: []\n  }]\n}, {\n  value: '3. 储存注意事项',\n  id: '3-储存注意事项',\n  children: []\n}, {\n  value: '4. 备份和灾难恢复',\n  id: '4-备份和灾难恢复',\n  children: [{\n    value: '定期执行下游集群备份',\n    id: '定期执行下游集群备份',\n    children: []\n  }, {\n    value: '备份下游节点虚拟机',\n    id: '备份下游节点虚拟机',\n    children: []\n  }]\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p>{`除了 VMware 记录的标准 vSphere 最佳实践外，本指南还概述了在 vSphere 环境中配置下游 Rancher 集群的参考架构。`}</p>\n    <p><strong parentName=\"p\">{`解决方案概述`}</strong></p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/solution_overview.drawio.svg\",\n        \"alt\": \"Solution Overview\"\n      }}></img></p>\n    <h2 {...{\n      \"id\": \"1-虚拟机注意事项\"\n    }}>{`1. 虚拟机注意事项`}</h2>\n    <h3 {...{\n      \"id\": \"充分利用模板来构建环境\"\n    }}>{`充分利用模板来构建环境`}</h3>\n    <p>{`为了促进整个环境中部署的虚拟机的一致性，可以考虑使用虚拟机模板形式的 \"Golden Images\"。可以使用 Packer 来实现这一点，增加更多的自定义选项。`}</p>\n    <h3 {...{\n      \"id\": \"利用-drs-反亲和规则（如果可能）在-esxi-主机上分离下游集群节点\"\n    }}>{`利用 DRS 反亲和规则（如果可能）在 ESXi 主机上分离下游集群节点`}</h3>\n    <p>{`这样做将确保节点虚拟机分布在多台 ESXi 主机上--防止主机级别的单点故障。`}</p>\n    <h3 {...{\n      \"id\": \"利用-drs-反亲和规则（如果可能）在整个数据存储区中分离下游集群节点\"\n    }}>{`利用 DRS 反亲和规则（如果可能）在整个数据存储区中分离下游集群节点`}</h3>\n    <p>{`这样做可以确保节点虚拟机分布在多个数据存储上，防止在数据存储层面出现单点故障。`}</p>\n    <h4 {...{\n      \"id\": \"为-kubernetes-配置合适的虚拟机\"\n    }}>{`为 Kubernetes 配置合适的虚拟机`}</h4>\n    <p>{`在部署节点时，遵循 K8s 和 etcd 的最佳实践是很重要的，包括禁用 swap，仔细检查你在集群中的所有机器之间有完好的网络连接，为每个节点使用唯一的主机名、MAC 地址和 product_uuids。`}</p>\n    <h2 {...{\n      \"id\": \"2-网络注意事项\"\n    }}>{`2. 网络注意事项`}</h2>\n    <h4 {...{\n      \"id\": \"利用-etcd-节点之间的低延迟、高带宽连接\"\n    }}>{`利用 ETCD 节点之间的低延迟、高带宽连接`}</h4>\n    <p>{`尽可能在单个数据中心内部署 etcd 成员，以避免延迟开销并减少网络分区的可能性。对于大多数设置，1Gb 连接就足够了。对于大型集群，10Gb 连接可以减少从备份恢复所需的时间。`}</p>\n    <h3 {...{\n      \"id\": \"为虚拟机提供固定的-ip-地址\"\n    }}>{`为虚拟机提供固定的 IP 地址`}</h3>\n    <p>{`使用的每个节点都应该配置一个静态 IP。在 DHCP 的情况下，每个节点应该有一个 DHCP 预留，以确保节点获得相同的 IP 分配。`}</p>\n    <h2 {...{\n      \"id\": \"3-储存注意事项\"\n    }}>{`3. 储存注意事项`}</h2>\n    <h4 {...{\n      \"id\": \"建议-etcd-节点使用-ssd-硬盘\"\n    }}>{`建议 ETCD 节点使用 SSD 硬盘`}</h4>\n    <p>{`ETCD 对写入延迟非常敏感。因此，尽可能地使用 SSD 磁盘。`}</p>\n    <h2 {...{\n      \"id\": \"4-备份和灾难恢复\"\n    }}>{`4. 备份和灾难恢复`}</h2>\n    <h3 {...{\n      \"id\": \"定期执行下游集群备份\"\n    }}>{`定期执行下游集群备份`}</h3>\n    <p>{`Kubernetes 使用 etcd 来存储其所有数据--从配置、状态和元数据。在灾难恢复的情况下，备份这些数据是至关重要的。`}</p>\n    <h3 {...{\n      \"id\": \"备份下游节点虚拟机\"\n    }}>{`备份下游节点虚拟机`}</h3>\n    <p>{`将 Rancher 下游节点虚拟机纳入标准虚拟机备份策略中。`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}