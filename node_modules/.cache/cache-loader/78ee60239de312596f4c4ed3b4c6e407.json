,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '在 vSphere 环境中安装 Rancher',\n\tdescription: '本指南概述了在 vSphere 环境中的 RKE Kubernetes 集群上安装 Rancher 的参考架构，以及 VMware 记录的标准 vSphere 最佳实践。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'最佳实践',\n\t\t'在 vSphere 环境中安装 Rancher'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/best-practices/2.5/rancher-server/rancher-in-vsphere/_index\",\n  \"title\": \"在 vSphere 环境中安装 Rancher\",\n  \"description\": \"本指南概述了在 vSphere 环境中的 RKE Kubernetes 集群上安装 Rancher 的参考架构，以及 VMware 记录的标准 vSphere 最佳实践。\",\n  \"source\": \"@site/docs/rancher2/best-practices/2.5/rancher-server/rancher-in-vsphere/_index.md\",\n  \"permalink\": \"/docs/rancher2/best-practices/2.5/rancher-server/rancher-in-vsphere/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/best-practices/2.5/rancher-server/rancher-in-vsphere/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar\": \"rancher2\",\n  \"previous\": {\n    \"title\": \"日志最佳实践\",\n    \"permalink\": \"/docs/rancher2/best-practices/2.5/rancher-managed/logging/_index\"\n  },\n  \"next\": {\n    \"title\": \"Rancher管理集群的最佳实践\",\n    \"permalink\": \"/docs/rancher2/best-practices/2.5/rancher-managed/_index\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '1. 负载均衡器的注意事项',\n  id: '1-负载均衡器的注意事项',\n  children: [{\n    value: '利用容错和高可用性',\n    id: '利用容错和高可用性',\n    children: []\n  }, {\n    value: '备份负载均衡器配置',\n    id: '备份负载均衡器配置',\n    children: []\n  }, {\n    value: '配置健康检查',\n    id: '配置健康检查',\n    children: []\n  }, {\n    value: '充分利用外部负载均衡器',\n    id: '充分利用外部负载均衡器',\n    children: []\n  }, {\n    value: '安全访问 Rancher',\n    id: '安全访问-rancher',\n    children: []\n  }]\n}, {\n  value: '2. VM 注意事项',\n  id: '2-vm-注意事项',\n  children: [{\n    value: '根据 Rancher 文档确定虚拟机的大小',\n    id: '根据-rancher-文档确定虚拟机的大小',\n    children: []\n  }, {\n    value: '充分利用虚拟机模板来构建环境',\n    id: '充分利用虚拟机模板来构建环境',\n    children: []\n  }, {\n    value: '利用 DRS 反亲和规则（如果可能）在 ESXi 主机上分离 Rancher 集群节点',\n    id: '利用-drs-反亲和规则（如果可能）在-esxi-主机上分离-rancher-集群节点',\n    children: []\n  }, {\n    value: '利用 DRS 反亲和规则（如果可能）在整个数据存储区中分离 Rancher 集群节点',\n    id: '利用-drs-反亲和规则（如果可能）在整个数据存储区中分离-rancher-集群节点',\n    children: []\n  }, {\n    value: '为 Kubernetes 配置合适的虚拟机',\n    id: '为-kubernetes-配置合适的虚拟机',\n    children: []\n  }]\n}, {\n  value: '3. 网络注意事项',\n  id: '3-网络注意事项',\n  children: [{\n    value: '利用 ETCD 节点之间的低延迟、高带宽连接',\n    id: '利用-etcd-节点之间的低延迟、高带宽连接',\n    children: []\n  }, {\n    value: '为虚拟机提供固定的 IP 地址',\n    id: '为虚拟机提供固定的-ip-地址',\n    children: []\n  }]\n}, {\n  value: '4. 储存注意事项',\n  id: '4-储存注意事项',\n  children: [{\n    value: '建议 ETCD 节点使用 SSD 硬盘',\n    id: '建议-etcd-节点使用-ssd-硬盘',\n    children: []\n  }]\n}, {\n  value: '5. 备份和灾难恢复',\n  id: '5-备份和灾难恢复',\n  children: [{\n    value: '定期执行管理集群备份',\n    id: '定期执行管理集群备份',\n    children: []\n  }, {\n    value: '备份 Rancher 集群节点虚拟机',\n    id: '备份-rancher-集群节点虚拟机',\n    children: []\n  }]\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p>{`本指南概述了在 vSphere 环境中的 RKE Kubernetes 集群上安装 Rancher 的参考架构，以及 VMware 记录的标准 vSphere 最佳实践。`}</p>\n    <p><strong parentName=\"p\">{`解决方案概述`}</strong></p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/rancher-on-prem-vsphere.svg\",\n        \"alt\": \"Solution Overview\"\n      }}></img></p>\n    <h2 {...{\n      \"id\": \"1-负载均衡器的注意事项\"\n    }}>{`1. 负载均衡器的注意事项`}</h2>\n    <p>{`您需要一个负载均衡器将流量引导到 RKE 节点上的 Rancher 工作负载。`}</p>\n    <h3 {...{\n      \"id\": \"利用容错和高可用性\"\n    }}>{`利用容错和高可用性`}</h3>\n    <p>{`请充分利用具有继承高可用功能的外部（硬件或软件）负载均衡器（如：F5、NSX-T、Keepalived 等）。`}</p>\n    <h3 {...{\n      \"id\": \"备份负载均衡器配置\"\n    }}>{`备份负载均衡器配置`}</h3>\n    <p>{`在发生灾难恢复时，负载均衡器配置的可用性将加快恢复过程。`}</p>\n    <h3 {...{\n      \"id\": \"配置健康检查\"\n    }}>{`配置健康检查`}</h3>\n    <p>{`配置负载均衡器在健康检查失败时自动将节点标记为不可用。例如，NGINX 可以通过以下配置来实现这一功能：`}</p>\n    <p><inlineCode parentName=\"p\">{`max_fails=3 fail_timeout=5s`}</inlineCode></p>\n    <h3 {...{\n      \"id\": \"充分利用外部负载均衡器\"\n    }}>{`充分利用外部负载均衡器`}</h3>\n    <p>{`避免在管理集群内实施软件负载均衡器。`}</p>\n    <h3 {...{\n      \"id\": \"安全访问-rancher\"\n    }}>{`安全访问 Rancher`}</h3>\n    <p>{`配置适当的防火墙/ACL 规则，只允许对 Rancher 的访问。`}</p>\n    <h2 {...{\n      \"id\": \"2-vm-注意事项\"\n    }}>{`2. VM 注意事项`}</h2>\n    <h3 {...{\n      \"id\": \"根据-rancher-文档确定虚拟机的大小\"\n    }}>{`根据 Rancher 文档确定虚拟机的大小`}</h3>\n    <p>{`请参考`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/installation/requirements/_index\"\n      }}>{`安装 Rancher Server 节点的软件，硬件和网络要求`}</a>{`，确定虚拟机的大小。`}</p>\n    <h3 {...{\n      \"id\": \"充分利用虚拟机模板来构建环境\"\n    }}>{`充分利用虚拟机模板来构建环境`}</h3>\n    <p>{`为了促进整个环境中部署的虚拟机的一致性，可以考虑使用虚拟机模板形式的 \"Golden Images\"。可以使用 Packer 来实现这一点，增加更多的自定义选项。`}</p>\n    <h3 {...{\n      \"id\": \"利用-drs-反亲和规则（如果可能）在-esxi-主机上分离-rancher-集群节点\"\n    }}>{`利用 DRS 反亲和规则（如果可能）在 ESXi 主机上分离 Rancher 集群节点`}</h3>\n    <p>{`这样做将确保节点虚拟机分布在多台 ESXi 主机上--防止主机级别的单点故障。`}</p>\n    <h3 {...{\n      \"id\": \"利用-drs-反亲和规则（如果可能）在整个数据存储区中分离-rancher-集群节点\"\n    }}>{`利用 DRS 反亲和规则（如果可能）在整个数据存储区中分离 Rancher 集群节点`}</h3>\n    <p>{`这样做可以确保节点虚拟机分布在多个数据存储上，防止在数据存储层面出现单点故障。`}</p>\n    <h3 {...{\n      \"id\": \"为-kubernetes-配置合适的虚拟机\"\n    }}>{`为 Kubernetes 配置合适的虚拟机`}</h3>\n    <p>{`在部署节点时，遵循 K8s 和 etcd 的最佳实践是很重要的，包括禁用 swap，仔细检查你在集群中的所有机器之间有完好的网络连接，为每个节点使用唯一的主机名、MAC 地址和 product_uuids。`}</p>\n    <h2 {...{\n      \"id\": \"3-网络注意事项\"\n    }}>{`3. 网络注意事项`}</h2>\n    <h3 {...{\n      \"id\": \"利用-etcd-节点之间的低延迟、高带宽连接\"\n    }}>{`利用 ETCD 节点之间的低延迟、高带宽连接`}</h3>\n    <p>{`尽可能在单个数据中心内部署 etcd 成员，以避免延迟开销并减少网络分区的可能性。对于大多数设置，1Gb 连接就足够了。对于大型集群，10Gb 连接可以减少从备份恢复所需的时间。`}</p>\n    <h3 {...{\n      \"id\": \"为虚拟机提供固定的-ip-地址\"\n    }}>{`为虚拟机提供固定的 IP 地址`}</h3>\n    <p>{`使用的每个节点都应该配置一个静态 IP。在 DHCP 的情况下，每个节点应该有一个 DHCP 预留，以确保节点获得相同的 IP 分配。`}</p>\n    <h2 {...{\n      \"id\": \"4-储存注意事项\"\n    }}>{`4. 储存注意事项`}</h2>\n    <h3 {...{\n      \"id\": \"建议-etcd-节点使用-ssd-硬盘\"\n    }}>{`建议 ETCD 节点使用 SSD 硬盘`}</h3>\n    <p>{`ETCD 对写入延迟非常敏感。因此，尽可能地使用 SSD 磁盘。`}</p>\n    <h2 {...{\n      \"id\": \"5-备份和灾难恢复\"\n    }}>{`5. 备份和灾难恢复`}</h2>\n    <h3 {...{\n      \"id\": \"定期执行管理集群备份\"\n    }}>{`定期执行管理集群备份`}</h3>\n    <p>{`Rancher 将其数据存储在其所在的 Kubernetes 集群的 ETCD 数据存储中。与任何 Kubernetes 集群一样，对该集群执行频繁且经过测试的备份。`}</p>\n    <h3 {...{\n      \"id\": \"备份-rancher-集群节点虚拟机\"\n    }}>{`备份 Rancher 集群节点虚拟机`}</h3>\n    <p>{`将 Rancher 管理节点的虚拟机纳入标准的虚拟机备份策略中。`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}