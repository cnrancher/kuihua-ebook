zBK,CA4BL,SAAQ,CACN,KAAM,WADA,CAAR,CAEK,WAFL,CA5BK,CA+BL,aAAK,qCAAL,CAA0C,kBAAY,UAAU,CAAC,GAAvB,EAA6B,YAA7B,CAA1C,CAAkG,WAAlG,CAA6G,kBAAG,UAAU,CAAC,GAAd,EAAsB,CAC/H,OAAQ,iEADuH,CAAtB,EAEtG,cAFsG,CAA7G,CAE2B,eAF3B,CA/BK,CAkCL,aAAK,iDAAL,CAAsD,kBAAG,UAAU,CAAC,GAAd,EAAsB,CACxE,OAAQ,sCADgE,CAAtB,EAE/C,WAF+C,CAAtD,CAEwB,eAFxB,CAlCK,CAqCL,SAAQ,CACN,KAAM,kBADA,CAAR,CAEK,oBAFL,CArCK,CAwCL,aAAK,qDAAL,CAxCK,CAyCL,aAAG,kBAAG,UAAU,CAAC,GAAd,EAAsB,CACrB,OAAQ,8DADa,CAAtB,EAEI,8DAFJ,CAAH,CAzCK,CAAP,CA6CD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '功能介绍',\n\tdescription: '用 Rancher 和 vSphere，您可以在本地体验云环境的操作。Rancher 可以在 vSphere 中创建节点并在其上安装 Kubernetes。在 vSphere 中创建 Kubernetes 集群时，Rancher 首先通过与 vCenter API 通信来创建指定数量的虚拟机。然后将 Kubernetes 安装在节点上。vSphere 集群可能由具有不同属性（例如内存或 vCPU 数量）的多组虚拟机组成。该分组允许对每个 Kubernetes 角色的节点大小进行细粒度的控制。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'创建集群',\n\t\t'创建节点和集群',\n\t\t'vSphere',\n\t\t'功能介绍'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/cluster-provisioning/rke-clusters/node-pools/vsphere/_index\",\n  \"title\": \"功能介绍\",\n  \"description\": \"用 Rancher 和 vSphere，您可以在本地体验云环境的操作。Rancher 可以在 vSphere 中创建节点并在其上安装 Kubernetes。在 vSphere 中创建 Kubernetes 集群时，Rancher 首先通过与 vCenter API 通信来创建指定数量的虚拟机。然后将 Kubernetes 安装在节点上。vSphere 集群可能由具有不同属性（例如内存或 vCPU 数量）的多组虚拟机组成。该分组允许对每个 Kubernetes 角色的节点大小进行细粒度的控制。\",\n  \"source\": \"@site/docs/rancher2/cluster-provisioning/rke-clusters/node-pools/vsphere/_index.md\",\n  \"permalink\": \"/docs/rancher2/cluster-provisioning/rke-clusters/node-pools/vsphere/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/cluster-provisioning/rke-clusters/node-pools/vsphere/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar\": \"rancher2\",\n  \"previous\": {\n    \"title\": \"Azure\",\n    \"permalink\": \"/docs/rancher2/cluster-provisioning/rke-clusters/node-pools/azure/_index\"\n  },\n  \"next\": {\n    \"title\": \"创建集群\",\n    \"permalink\": \"/docs/rancher2/cluster-provisioning/rke-clusters/node-pools/vsphere/provisioning-vsphere-clusters/_index\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '概述',\n  id: '概述',\n  children: []\n}, {\n  value: 'vSphere 增强',\n  id: 'vsphere-增强',\n  children: [{\n    value: '自我修复的节点池',\n    id: '自我修复的节点池',\n    children: []\n  }, {\n    value: '实例配置选项的动态获取',\n    id: '实例配置选项的动态获取',\n    children: []\n  }, {\n    value: '更多操作系统的支持',\n    id: '更多操作系统的支持',\n    children: []\n  }]\n}, {\n  value: 'v2.3.3 节点模板功能的视频介绍',\n  id: 'v233-节点模板功能的视频介绍',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <h2 {...{\n      \"id\": \"概述\"\n    }}>{`概述`}</h2>\n    <p>{`您可以使用 Rancher 和 vSphere 在本地体验云环境的操作。Rancher 可以在 vSphere 中创建节点并在其上安装 Kubernetes。创建 Kubernetes 集群时，Rancher 首先通过与 vCenter API 通信来创建指定数量的虚拟机，然后将 Kubernetes 安装在节点上。vSphere 集群可能由具有不同属性（例如内存或 vCPU 数量）的多组虚拟机组成。该分组允许对每个 Kubernetes 角色的节点大小进行细粒度的控制。`}</p>\n    <h2 {...{\n      \"id\": \"vsphere-增强\"\n    }}>{`vSphere 增强`}</h2>\n    <p>{`我们优化了 vSphere 节点驱动，使您可以通过以下增强功能在本地体验云环境的部署操作：`}</p>\n    <h3 {...{\n      \"id\": \"自我修复的节点池\"\n    }}>{`自我修复的节点池`}</h3>\n    <p><em parentName=\"p\">{`从 Rancher v2.3.0 开始可用`}</em></p>\n    <p>{`自我修复节点池，也称为`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-provisioning/rke-clusters/node-pools/_index\"\n      }}>{`节点自动替换功能`}</a>{`，是使用 Rancher 设置 vSphere 节点的最大优势。自我修复节点池可以帮助您替换无状态应用程序的工作节点，当您使用 Rancher 通过节点模板创建节点时，Rancher 可以自动替换无法访问的节点。`}</p>\n    <blockquote>\n      <p parentName=\"blockquote\"><strong parentName=\"p\">{`重要：`}</strong>{` 不建议在 master 节点或具有持久卷连接的节点的节点池上启用节点自动替换。当节点池中的节点失去与集群的连接时，其持久卷会被破坏，从而导致有状态应用程序丢失数据。`}</p>\n    </blockquote>\n    <h3 {...{\n      \"id\": \"实例配置选项的动态获取\"\n    }}>{`实例配置选项的动态获取`}</h3>\n    <p><em parentName=\"p\">{`从 Rancher v2.3.3 开始可用`}</em></p>\n    <p>{`增强后的 vSphere 的节点模板在使用 vSphere 凭证创建节点模板时，模板选项会自动获取与在 vSphere 控制台中配置 VM 相同的选项。`}</p>\n    <p>{`对于要自动获取的字段，您的设置需要满足一些`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-provisioning/rke-clusters/node-pools/vsphere/provisioning-vsphere-clusters/_index\"\n      }}>{`先决条件`}</a>{`。`}</p>\n    <h3 {...{\n      \"id\": \"更多操作系统的支持\"\n    }}>{`更多操作系统的支持`}</h3>\n    <p>{`在 Rancher v2.3.3+中，您可以在创建虚拟机时选择任意支持`}<inlineCode parentName=\"p\">{`cloud-init`}</inlineCode>{`的操作系统。目前 `}<a parentName=\"p\" {...{\n        \"href\": \"https://cloudinit.readthedocs.io/en/latest/topics/examples.html\"\n      }}>{`cloud config`}</a>{` 只支持 YAML 格式。`}</p>\n    <p>{`在 v2.3.3 之前版本的 Rancher 中包含的 vSphere 节点驱动程序仅支持以 `}<a parentName=\"p\" {...{\n        \"href\": \"https://rancher.com/docs/os/v1.x/en/\"\n      }}>{`RancherOS`}</a>{` 作为 VM 的操作系统。`}</p>\n    <h2 {...{\n      \"id\": \"v233-节点模板功能的视频介绍\"\n    }}>{`v2.3.3 节点模板功能的视频介绍`}</h2>\n    <p>{`在这段 YouTube 视频中，我们演示了如何使用新的节点模板来帮助您在本地环境体验云环境一样的操作。`}</p>\n    <p><a parentName=\"p\" {...{\n        \"href\": \"https://www.youtube.com/watch?v=dPIwg6x1AlU&feature=emb_logo\"\n      }}>{`https://www.youtube.com/watch?v=dPIwg6x1AlU&feature=emb_logo`}</a></p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}