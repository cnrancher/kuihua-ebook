P,CAkCD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: 'Fleet 介绍',\n\tdescription: 'Fleet 是轻量级的集群管理工具，您可以使用 Fleet 管理多达一百万个集群。Fleet 是一个独立于 Rancher 的项目，可以用 Helm 安装在任何 Kubernetes 集群上。Fleet 可以从 git 管理原始 Kubernetes YAML、Helm chart 或 Kustomize 或三者的任意组合的部署。无论来源如何，所有的资源都会被动态地转化为 Helm chart，并以 Helm 作为引擎，来实现部署集群中的一切。这给了一个高度的控制，一致性和可审计性。Fleet 不仅关注于扩展能力，还关注于给人高度的控制和可视性，以确切地了解集群上安装了什么。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'跨集群部署应用',\n\t\t'Fleet'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/deploy-across-clusters/fleet/_index\",\n  \"title\": \"Fleet 介绍\",\n  \"description\": \"Fleet 是轻量级的集群管理工具，您可以使用 Fleet 管理多达一百万个集群。Fleet 是一个独立于 Rancher 的项目，可以用 Helm 安装在任何 Kubernetes 集群上。Fleet 可以从 git 管理原始 Kubernetes YAML、Helm chart 或 Kustomize 或三者的任意组合的部署。无论来源如何，所有的资源都会被动态地转化为 Helm chart，并以 Helm 作为引擎，来实现部署集群中的一切。这给了一个高度的控制，一致性和可审计性。Fleet 不仅关注于扩展能力，还关注于给人高度的控制和可视性，以确切地了解集群上安装了什么。\",\n  \"source\": \"@site/docs/rancher2/deploy-across-clusters/fleet/_index.md\",\n  \"permalink\": \"/docs/rancher2/deploy-across-clusters/fleet/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/deploy-across-clusters/fleet/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar\": \"rancher2\",\n  \"previous\": {\n    \"title\": \"跨集群部署应用程序概述\",\n    \"permalink\": \"/docs/rancher2/deploy-across-clusters/_index\"\n  },\n  \"next\": {\n    \"title\": \"多集群应用\",\n    \"permalink\": \"/docs/rancher2/deploy-across-clusters/multi-cluster-apps/_index\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '概述',\n  id: '概述',\n  children: []\n}, {\n  value: '在 Rancher 用户界面中访问 Fleet',\n  id: '在-rancher-用户界面中访问-fleet',\n  children: []\n}, {\n  value: 'GitHub 仓库',\n  id: 'github-仓库',\n  children: []\n}, {\n  value: '文档',\n  id: '文档',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p><em parentName=\"p\">{`适用于 Rancher v2.5+`}</em></p>\n    <h2 {...{\n      \"id\": \"概述\"\n    }}>{`概述`}</h2>\n    <p>{`Fleet 是轻量级的集群管理工具，您可以使用 Fleet 管理多达一百万个集群。对于`}<a parentName=\"p\" {...{\n        \"href\": \"https://fleet.rancher.io/single-cluster-install/\"\n      }}>{`单个集群`}</a>{`也很好用，但当你达到`}<a parentName=\"p\" {...{\n        \"href\": \"https://fleet.rancher.io/multi-cluster-install/\"\n      }}>{`大规模`}</a>{`时，它才真正发挥出它的威力。`}</p>\n    <p>{`Fleet 是一个独立于 Rancher 的项目，可以用 Helm 安装在任何 Kubernetes 集群上。Fleet 的架构如下图所示：`}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/fleet-architecture.png\",\n        \"alt\": \"架构\"\n      }}></img></p>\n    <p>{`Fleet 可以从 git 管理原始 Kubernetes YAML、Helm chart 或 Kustomize 或三者的任意组合的部署。无论来源如何，所有的资源都会被动态地转化为 Helm chart，并以 Helm 作为引擎，来实现部署集群中的一切。这给了一个高度的控制，一致性和可审计性。Fleet 不仅关注于扩展能力，还关注于给人高度的控制和可视性，以确切地了解集群上安装了什么。`}</p>\n    <h2 {...{\n      \"id\": \"在-rancher-用户界面中访问-fleet\"\n    }}>{`在 Rancher 用户界面中访问 Fleet`}</h2>\n    <p>{`Rancher v2.5 中预装了 Fleet。要访问它，请进入 Rancher 用户界面中的`}<strong parentName=\"p\">{`集群资源管理器`}</strong>{`。在左上角的下拉菜单中，单击`}<strong parentName=\"p\">{`集群资源管理器 > Fleet`}</strong>{`在这个页面上，你可以编辑 Kubernetes 资源和由 Fleet 管理的集群组。`}</p>\n    <h2 {...{\n      \"id\": \"github-仓库\"\n    }}>{`GitHub 仓库`}</h2>\n    <p>{`Fleet 的 Helm chart 可在`}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/rancher/fleet/releases/latest\"\n      }}>{`这里`}</a>{`查阅。`}</p>\n    <h2 {...{\n      \"id\": \"文档\"\n    }}>{`文档`}</h2>\n    <p>{`Fleet 的文档请参考见`}<a parentName=\"p\" {...{\n        \"href\": \"https://fleet.rancher.io/\"\n      }}>{`Fleet 文档`}</a>{`。`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}