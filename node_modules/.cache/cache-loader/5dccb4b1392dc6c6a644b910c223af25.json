,kBAAG,UAAU,CAAC,GAAd,EAAsB,CACpD,OAAQ,mEAD4C,CAAtB,EAE3B,IAF2B,CAAlC,CAEiB;IAFjB,CAGC,kBAAY,UAAU,CAAC,GAAvB,EAA6B,mBAA7B,CAHD,CAGgE,gDAHhE,CAGgH,kBAAY,UAAU,CAAC,GAAvB,EAA6B,uBAA7B,CAHhH,CAGmL,GAHnL,CAzEK,CA6EL,eAAK,qBAAM,UAAU,CAAC,KAAjB,EAA2B,CAC5B,YAAa,eADe,CAA3B,EAEE;;;;;;;;;;;;CAFF,CAAL,CA7EK,CA4FL,aAAK,uCAAL,CA5FK,CA6FL,SAAQ,CACN,KAAM,MADA,CAAR,CAEK,MAFL,CA7FK,CAgGL,aAAK,qHAAL,CAhGK,CAiGL,aAAK,YAAL,CAjGK,CAkGL,aAAK,6BAAL,CAlGK,CAmGL,cACE,UAAI,UAAU,CAAC,IAAf,EACE,SAAG,UAAU,CAAC,IAAd,EAAqB,kCAArB,CADF,CAEE,WAAK,UAAU,CAAC,IAAhB,EAAqB,qBAAM,UAAU,CAAC,KAAjB,EAA2B,CAC5C,YAAa,eAD+B,CAA3B,EAEd;;;;;;;;;;;;;;;;CAFc,CAArB,CAFF,CADF,CAuBE,UAAI,UAAU,CAAC,IAAf,EACE,SAAG,UAAU,CAAC,IAAd,EAAqB,0BAArB,CADF,CAEE,SAAG,UAAU,CAAC,IAAd,EAAmB,oBAAK,UAAU,CAAC,GAAhB,EAAwB,CACvC,MAAO,+DADgC,CAEvC,MAAO,yBAFgC,CAAxB,EAAnB,CAFF,CAvBF,CAnGK,CAkIL,aAAK,0CAAL,CAA+C,kBAAG,UAAU,CAAC,GAAd,EAAsB,CACjE,OAAQ,6DADyD,CAAtB,EAExC,MAFwC,CAA/C,CAEmB,GAFnB,CAlIK,CAqIL,SAAQ,CACN,KAAM,aADA,CAAR,CAEK,aAFL,CArIK,CAwIL,aAAK,2DAAL,CAAgE,kBAAG,UAAU,CAAC,GAAd,EAAsB,CAClF,OAAQ,oGAD0E,CAAtB,EAEzD,sBAFyD,CAAhE,CAEmC,UAFnC,CAxIK,CAAP,CA4ID,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '迁移到Rancher v2.5监控',\n\tdescription: '如果您在 Rancher v2.5 之前曾在 Rancher 中启用过监控、告警或通知器，则没有自动升级路径来切换到新的监控/告警解决方案。在通过 Cluster Explore 部署新的监控解决方案之前，您需要禁用和删除整个集群和所有项目中所有现有的自定义告警、通知器和监控安装。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'rancher 2.5',\n\t\t'监控和告警',\n\t\t'迁移到Rancher v2.5监控'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/monitoring-alerting/2.5/migrating/_index\",\n  \"title\": \"迁移到Rancher v2.5监控\",\n  \"description\": \"如果您在 Rancher v2.5 之前曾在 Rancher 中启用过监控、告警或通知器，则没有自动升级路径来切换到新的监控/告警解决方案。在通过 Cluster Explore 部署新的监控解决方案之前，您需要禁用和删除整个集群和所有项目中所有现有的自定义告警、通知器和监控安装。\",\n  \"source\": \"@site/docs/rancher2/monitoring-alerting/2.5/migrating/_index.md\",\n  \"permalink\": \"/docs/rancher2/monitoring-alerting/2.5/migrating/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/monitoring-alerting/2.5/migrating/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '概述',\n  id: '概述',\n  children: []\n}, {\n  value: 'Rancher v2.5 之前的监控',\n  id: 'rancher-v25-之前的监控',\n  children: []\n}, {\n  value: '通过 Rancher 2.5 中的集群浏览器进行监控和告警',\n  id: '通过-rancher-25-中的集群浏览器进行监控和告警',\n  children: []\n}, {\n  value: '基于角色的访问控制的变更',\n  id: '基于角色的访问控制的变更',\n  children: []\n}, {\n  value: '从监控 V1 迁移到监控 V2',\n  id: '从监控-v1-迁移到监控-v2',\n  children: [{\n    value: '迁移 Grafana 仪表盘',\n    id: '迁移-grafana-仪表盘',\n    children: []\n  }, {\n    value: '迁移告警',\n    id: '迁移告警',\n    children: []\n  }, {\n    value: '迁移 notifier',\n    id: '迁移-notifier',\n    children: []\n  }]\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <h2 {...{\n      \"id\": \"概述\"\n    }}>{`概述`}</h2>\n    <p>{`如果您在 Rancher v2.5 之前曾在 Rancher 中启用过监控、告警或通知，则无法通过自动升级路径切换到新的监控告警解决方案。在通过 Cluster Explorer 部署新的监控解决方案之前，您需要禁用和删除整个集群和所有项目中所有现有的监控、自定义告警和通知。`}</p>\n    <h2 {...{\n      \"id\": \"rancher-v25-之前的监控\"\n    }}>{`Rancher v2.5 之前的监控`}</h2>\n    <p>{`从 v2.2.0 开始，Rancher 的集群管理器允许用户在集群内独立启用监控和告警 V1（均由`}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/prometheus-operator/prometheus-operator\"\n      }}>{`Prometheus Operator`}</a>{`提供支持）。有关如何配置 Monitoring & Alerting V1 的更多信息，请参阅`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/monitoring-alerting/2.0-2.4/_index\"\n      }}>{`关于 Rancher v2.5 之前的监控的文档`}</a>{`。`}</p>\n    <p>{`启用 Monitoring 后，Monitoring V1 会将`}<a parentName=\"p\" {...{\n        \"href\": \"https://prometheus.io/\"\n      }}>{`Prometheus`}</a>{`和`}<a parentName=\"p\" {...{\n        \"href\": \"https://grafana.com/docs/grafana/latest/getting-started/\"\n      }}>{`Grafana`}</a>{`部署到集群上，以监控集群节点、Kubernetes 组件和软件部署的进程状态，并创建自定义仪表盘，使其易于可视化收集的指标。`}</p>\n    <p>{`Monitoring V1 可以在集群级别和项目级别上配置，并将自动拉取 Rancher 集群上部署为 Apps 的某些工作负载。`}</p>\n    <p>{`当启用 Alerts 或 Notifiers 时，Alerting V1 会将`}<a parentName=\"p\" {...{\n        \"href\": \"https://prometheus.io/docs/alerting/latest/alertmanager/\"\n      }}>{`Prometheus Alertmanager`}</a>{`和一组 Rancher 控制器部署到集群上，用户可以通过 Email、Slack、PagerDuty 等方式定义告警和配置基于告警的通知。用户可以根据需要监控的内容（如系统服务、资源、CIS 扫描等）选择创建不同类型的告警；但是，只有在启用了 Monitoring V1 的情况下，才能创建基于 PromQL 表达式的告警。`}</p>\n    <h2 {...{\n      \"id\": \"通过-rancher-25-中的集群浏览器进行监控和告警\"\n    }}>{`通过 Rancher 2.5 中的集群浏览器进行监控和告警`}</h2>\n    <p>{`从 v2.5.0 开始，Rancher 的集群浏览器现在允许用户在集群内同时启用 Monitoring & Alerting V2（均由`}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/prometheus-operator/prometheus-operator\"\n      }}>{`Prometheus Operator`}</a>{`提供支持）。`}</p>\n    <p>{`与 Monitoring & Alerting V1 不同的是，这两个功能都打包在一个单一的 Helm 图表中，可以在`}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/rancher/charts/blob/main/charts/rancher-monitoring\"\n      }}>{`这里`}</a>{`找到。该图表和可配置字段的行为与`}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/prometheus-community/helm-charts/tree/main/charts/kube-prometheus-stack\"\n      }}>{`kube-prometheus-stack`}</a>{`、Prometheus Community Helm 图表紧密匹配，任何与上游图表的偏差都可以在与该图表一起维护的`}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/rancher/charts/blob/main/charts/rancher-monitoring/CHANGELOG.md\"\n      }}>{`CHANGELOG.md`}</a>{`中找到。`}</p>\n    <p>{`监控 V2 只能在集群层面进行配置。不再支持项目级监控和告警。`}</p>\n    <p>{`有关如何配置 Monitoring & Alerting V2 的更多信息，请参阅 `}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/monitoring-alerting/2.5/configuration/_index\"\n      }}>{`配置用于监控的自定义资源`}</a>{`。`}</p>\n    <h2 {...{\n      \"id\": \"基于角色的访问控制的变更\"\n    }}>{`基于角色的访问控制的变更`}</h2>\n    <p>{`项目所有者和成员不再能默认访问 Grafana 或 Prometheus。如果仅有视图的用户可以访问 Grafana，他们将能够看到任何命名空间的数据。对于 Kiali，任何用户都可以在任何命名空间中编辑他们不拥有的东西。`}</p>\n    <p>{`关于`}<inlineCode parentName=\"p\">{`rancher-monitoring`}</inlineCode>{`中基于角色的访问控制的更多信息，请参考`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/monitoring-alerting/2.5/rbac/_index\"\n      }}>{`基于角色的访问控制`}</a>{`。`}</p>\n    <h2 {...{\n      \"id\": \"从监控-v1-迁移到监控-v2\"\n    }}>{`从监控 V1 迁移到监控 V2`}</h2>\n    <p>{`虽然没有自动迁移功能，但是您可以将 Monitoring V1 中创建的自定义 Grafana 仪表盘和告警手动迁移到 Monitoring V2 中。`}</p>\n    <p>{`在安装 Monitoring V2 之前，请执行以下步骤，彻底卸载 Monitoring V1：`}</p>\n    <ol>\n      <li parentName=\"ol\">{`移除所有集群和项目的告警和告警组。`}</li>\n      <li parentName=\"ol\">{`删除所有 notifier。`}</li>\n      <li parentName=\"ol\">{`在 `}<strong parentName=\"li\">{`“集群 > 项目 > 工具 > 监控”`}</strong>{` 下禁用所有项目监控安装。`}</li>\n      <li parentName=\"ol\">{`确保所有项目中的所有项目监控应用都已被删除，并且在几分钟后不再重新创建。`}</li>\n      <li parentName=\"ol\">{`在 `}<strong parentName=\"li\">{`“集群 > 工具 > 监控”`}</strong>{` 下禁用集群监控安装。`}</li>\n      <li parentName=\"ol\">{`确保系统项目中的集群监控应用和监控操作应用已经被删除，并且在几分钟后不重新创建。`}</li>\n    </ol>\n    <h3 {...{\n      \"id\": \"迁移-grafana-仪表盘\"\n    }}>{`迁移 Grafana 仪表盘`}</h3>\n    <p>{`您可以将监控 V1 中添加到 Grafana 的任何仪表盘迁移到监控 V2 中。在 监控 V1 中，您可以执行以下步骤，导出现有的仪表盘。`}</p>\n    <ol>\n      <li parentName=\"ol\">{`登录 Grafana。`}</li>\n      <li parentName=\"ol\">{`导航到您要导出的仪表盘。`}</li>\n      <li parentName=\"ol\">{`进入仪表板设置。`}</li>\n      <li parentName=\"ol\">{`复制`}<a parentName=\"li\" {...{\n          \"href\": \"https://grafana.com/docs/grafana/latest/dashboards/json-model/\"\n        }}>{`JSON 模型`}</a>{`。`}</li>\n    </ol>\n    <p>{`在 JSON 模型中，将所有`}<inlineCode parentName=\"p\">{`datasource`}</inlineCode>{`字段从 `}<inlineCode parentName=\"p\">{`RANCHER_MONITORING`}</inlineCode>{`改为`}<inlineCode parentName=\"p\">{`Prometheus`}</inlineCode>{`。你可以通过替换所有出现的`}<inlineCode parentName=\"p\">{`\"datasource \"`}</inlineCode>{`来轻松做到这一点。`}<inlineCode parentName=\"p\">{`\"RANCHER_MONITORING\"`}</inlineCode>{`替换为`}<inlineCode parentName=\"p\">{`\"datasource\": \"Prometheus\"`}</inlineCode>{`。`}</p>\n    <p>{`如果 Grafana 是由一个持久卷支持的，你现在可以`}<a parentName=\"p\" {...{\n        \"href\": \"https://grafana.com/docs/grafana/latest/dashboards/export-import/\"\n      }}>{`导入`}</a>{`这个 JSON 模型到 Monitoring V2 Grafana UI 中。\n建议在`}<inlineCode parentName=\"p\">{`cattle-dashboards`}</inlineCode>{`命名空间中向 Grafana 提供一个 ConfigMap，该 ConfigMap 的标签为`}<inlineCode parentName=\"p\">{`grafana_dashboard。\"1\"`}</inlineCode>{`:`}</p>\n    <pre><code parentName=\"pre\" {...{\n        \"className\": \"language-yaml\"\n      }}>{`apiVersion: v1\nkind: ConfigMap\nmetadata:\n  name: custom-dashboard\n  namespace: cattle-dashboards\n  labels:\n    grafana_dashboard: \"1\"\ndata:\n  custom-dashboard.json: |\n    { \n      ... \n    }\n`}</code></pre>\n    <p>{`创建此 ConfigMap 以后，仪表板将自动添加到 Grafana 中。`}</p>\n    <h3 {...{\n      \"id\": \"迁移告警\"\n    }}>{`迁移告警`}</h3>\n    <p>{`只有将基于表达式的告警直接迁移到 Monitoring V2 中才有可能。幸运的是，基于事件的告警可以设置为对系统组件、节点或工作负载事件进行告警，这些告警已经被 Monitoring V2 的一部分所覆盖。所以没有必要迁移它们。`}</p>\n    <p>{`要迁移以下表达式告警`}</p>\n    <p>{`有两种方式可以迁移告警表表达式，选择其中一种执行即可：`}</p>\n    <ul>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`在命名空间创建一个像这样的 PrometheusRule 配置。`}</p>\n        <pre parentName=\"li\"><code parentName=\"pre\" {...{\n            \"className\": \"language-yaml\"\n          }}>{`apiVersion: monitoring.coreos.com/v1\nkind: PrometheusRule\nmetadata:\nname: custom-rules\nnamespace: default\nspec:\ngroups:\n  - name: custom.rules\n    rules:\n      - alert: Custom Expression Alert\n        expr: prometheus_query > 5\n        for: 5m\n        labels:\n          severity: critical\n        annotations:\n          summary: \"The result of prometheus_query has been larger than 5 for 5m. Current value {{ $value }}\"\n`}</code></pre>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`通过集群浏览器添加 Prometheus 规则。`}</p>\n        <p parentName=\"li\"><img parentName=\"p\" {...{\n            \"src\": \"/img/rancher/monitoring/migration/alert_2.4_to_2.5_source.png\",\n            \"alt\": \"alert_2.4_to_2.5_source\"\n          }}></img></p>\n      </li>\n    </ul>\n    <p>{`有关如何在监控 V2 中配置 PrometheusRules 的更多细节，请参见`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/monitoring-alerting/2.5/configuration/_index\"\n      }}>{`监控配置`}</a>{`。`}</p>\n    <h3 {...{\n      \"id\": \"迁移-notifier\"\n    }}>{`迁移 notifier`}</h3>\n    <p>{`在 Monitoring V1 中，没有直接对应的通知器工作方式。相反，你必须在 Monitoring V2 中用`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/monitoring-alerting/2.5/configuration/_index#%E9%85%8D%E7%BD%AE-Alertmanager-Config\"\n      }}>{`Routes and Receivers`}</a>{`复制所需的设置。`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}