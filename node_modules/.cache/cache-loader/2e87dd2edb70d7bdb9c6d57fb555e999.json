U,CAAC,GAAnB,EAAyB,KAAzB,CAAH,CAA4C,6CAA5C,CAAyF,kBAAY,UAAU,CAAC,GAAvB,EAA6B,SAA7B,CAAzF,CAA8I,QAA9I,CAAsJ,kBAAY,UAAU,CAAC,GAAvB,EAA6B,IAA7B,CAAtJ,CAAsM,GAAtM,CAAyM,kBAAY,UAAU,CAAC,GAAvB,EAA6B,IAA7B,CAAzM,CAAyP,SAAzP,CAtDK,CAuDL,SAAQ,CACN,KAAM,aADA,CAAR,CAEG,kBAAG,UAAU,CAAC,IAAd,EAAuB,CACtB,OAAQ,4DADc,CAAvB,EAEI,aAFJ,CAFH,CAvDK,CAAP,CA6DD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '6、设置 Istio 的流量管理组件',\n\tdescription: 'Istio 中流量管理的主要优势在于它允许动态请求路由。动态请求路由的一些常见应用包括金丝雀部署和蓝/绿部署。Istio 流量管理中的两个关键资源是虚拟服务（virtual services）和目标规则（destination rules）。本节介绍如何在示例 BookInfo 应用中添加与`reviews`微服务相对应的示例虚拟服务。该服务的目的是在两个版本的`reviews`服务之间分配流量。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'集群管理员指南',\n\t\t'集群访问控制',\n\t\t'告警',\n\t\t'Istio',\n\t\t'Istio使用指南',\n\t\t'设置 Istio 的流量管理组件'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/cluster-admin/tools/istio/setup/set-up-traffic-management/_index\",\n  \"title\": \"6、设置 Istio 的流量管理组件\",\n  \"description\": \"Istio 中流量管理的主要优势在于它允许动态请求路由。动态请求路由的一些常见应用包括金丝雀部署和蓝/绿部署。Istio 流量管理中的两个关键资源是虚拟服务（virtual services）和目标规则（destination rules）。本节介绍如何在示例 BookInfo 应用中添加与`reviews`微服务相对应的示例虚拟服务。该服务的目的是在两个版本的`reviews`服务之间分配流量。\",\n  \"source\": \"@site/docs/rancher2/cluster-admin/tools/istio/setup/set-up-traffic-management/_index.md\",\n  \"permalink\": \"/docs/rancher2/cluster-admin/tools/istio/setup/set-up-traffic-management/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/cluster-admin/tools/istio/setup/set-up-traffic-management/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar\": \"rancher2\",\n  \"previous\": {\n    \"title\": \"5、设置 Istio 网关\",\n    \"permalink\": \"/docs/rancher2/cluster-admin/tools/istio/setup/gateway/_index\"\n  },\n  \"next\": {\n    \"title\": \"7、产生并查看流量\",\n    \"permalink\": \"/docs/rancher2/cluster-admin/tools/istio/setup/view-traffic/_index\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p>{`Istio 中流量管理的主要优势在于它允许动态请求路由。动态请求路由的一些常见应用包括金丝雀部署和蓝/绿部署。Istio 流量管理中的两个关键资源是`}<em parentName=\"p\">{`虚拟服务（virtual services）`}</em>{`和`}<em parentName=\"p\">{`目标规则（destination rules）`}</em>{`。`}</p>\n    <ul>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"https://istio.io/docs/reference/config/networking/v1alpha3/virtual-service/\"\n        }}>{`虚拟服务`}</a>{`拦截流量并将其定向到您的 Kubernetes 服务，从而使您可以将流量百分比从请求分配到其他服务。您可以使用它们来定义一组路由规则，以在寻址主机时应用。`}</li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"https://istio.io/docs/reference/config/networking/v1alpha3/destination-rule/\"\n        }}>{`目标规则`}</a>{`可以作为有关哪些服务版本可用于接收虚拟服务流量的唯一来源。您可以使用这些资源来定义策略，该策略适用于路由发生后的服务流量。`}</li>\n    </ul>\n    <p>{`本节介绍如何在示例 BookInfo 应用中添加与`}<inlineCode parentName=\"p\">{`reviews`}</inlineCode>{`微服务相对应的示例虚拟服务。该服务的目的是在两个版本的`}<inlineCode parentName=\"p\">{`reviews`}</inlineCode>{`服务之间分配流量。`}</p>\n    <p>{`在此示例中，我们将流量转到`}<inlineCode parentName=\"p\">{`reviews`}</inlineCode>{`服务，并对其进行拦截，以使流量的 50％到达服务的`}<inlineCode parentName=\"p\">{`v1`}</inlineCode>{`版本，而 50％到达`}<inlineCode parentName=\"p\">{`v2`}</inlineCode>{`版本。`}</p>\n    <p>{`部署此虚拟服务后，我们将生成流量，并从 Kiali 可视化中看到流量在服务的两个版本之间平均路由。`}</p>\n    <ol>\n      <li parentName=\"ol\">{`转到项目视图，然后单击`}<strong parentName=\"li\">{`导入 YAML`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`将下面的资源复制到表单中。`}</li>\n      <li parentName=\"ol\">{`单击`}<strong parentName=\"li\">{`导入`}</strong>{`。`}</li>\n    </ol>\n    <pre><code parentName=\"pre\" {...{}}>{`apiVersion: networking.istio.io/v1alpha3\nkind: VirtualService\nmetadata:\n  name: reviews\nspec:\n  hosts:\n  - reviews\n  http:\n  - route:\n    - destination:\n        host: reviews\n        subset: v1\n      weight: 50\n    - destination:\n        host: reviews\n        subset: v3\n      weight: 50\n---\napiVersion: networking.istio.io/v1alpha3\nkind: DestinationRule\nmetadata:\n  name: reviews\nspec:\n  host: reviews\n  subsets:\n  - name: v1\n    labels:\n      version: v1\n  - name: v2\n    labels:\n      version: v2\n  - name: v3\n    labels:\n      version: v3\n`}</code></pre>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{` 当您为此服务生成流量时（例如通过刷新入口网关 URL），Kiali 流量图将反映出对`}<inlineCode parentName=\"p\">{`reviews`}</inlineCode>{`服务的流量在`}<inlineCode parentName=\"p\">{`v1`}</inlineCode>{`和`}<inlineCode parentName=\"p\">{`v3`}</inlineCode>{`之间平均分配。`}</p>\n    <h4 {...{\n      \"id\": \"下一步：产生并查看流量\"\n    }}><a parentName=\"h4\" {...{\n        \"href\": \"/docs/rancher2/istio/2.3.x-2.4.x/setup/view-traffic/_index\"\n      }}>{`下一步：产生并查看流量`}</a></h4>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}