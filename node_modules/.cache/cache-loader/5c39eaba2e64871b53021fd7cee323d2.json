om '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '持久化存储',\n\tdescription: '在部署需要保留数据的应用程序时，您需要创建永久性存储。持久化存储允许您将应用程序数据存储在运行应用程序的 Pod 外部。即使应用程序的 Pod 发生故障，这种存储方法也可以使您维护应用程序数据。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'集群管理员指南',\n\t\t'存储卷和存储类',\n\t\t'持久化存储'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/cluster-admin/volumes-and-storage/_index\",\n  \"title\": \"持久化存储\",\n  \"description\": \"在部署需要保留数据的应用程序时，您需要创建永久性存储。持久化存储允许您将应用程序数据存储在运行应用程序的 Pod 外部。即使应用程序的 Pod 发生故障，这种存储方法也可以使您维护应用程序数据。\",\n  \"source\": \"@site/docs/rancher2/cluster-admin/volumes-and-storage/_index.md\",\n  \"permalink\": \"/docs/rancher2/cluster-admin/volumes-and-storage/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/cluster-admin/volumes-and-storage/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar\": \"rancher2\",\n  \"previous\": {\n    \"title\": \"节点和节点池\",\n    \"permalink\": \"/docs/rancher2/cluster-admin/nodes/_index\"\n  },\n  \"next\": {\n    \"title\": \"工作原理\",\n    \"permalink\": \"/docs/rancher2/cluster-admin/volumes-and-storage/how-storage-works/_index\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '先决条件',\n  id: '先决条件',\n  children: []\n}, {\n  value: '设置现有的存储',\n  id: '设置现有的存储',\n  children: []\n}, {\n  value: '在 Rancher 中动态设置新存储',\n  id: '在-rancher-中动态设置新存储',\n  children: []\n}, {\n  value: '存储设置的实例',\n  id: '存储设置的实例',\n  children: []\n}, {\n  value: 'GlusterFS 卷',\n  id: 'glusterfs-卷',\n  children: []\n}, {\n  value: 'iSCSI 卷',\n  id: 'iscsi-卷',\n  children: []\n}, {\n  value: 'HostPath 卷',\n  id: 'hostpath-卷',\n  children: []\n}, {\n  value: '相关链接',\n  id: '相关链接',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p>{`持久化存储允许您将应用程序数据存储在运行应用程序的 Pod 外部。如果应用程序的 Pod 发生故障，这种存储方法可以保护应用程序数据不受故障影响。在部署需要保留数据的应用程序时，您需要创建永久性存储。`}</p>\n    <p>{`本章节默认您已了解 Kubernetes 持久卷、持久卷声明和存储类的概念。请参阅`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/volumes-and-storage/how-storage-works/_index\"\n      }}>{`存储是如何工作的`}</a>{`，获取更多相关信息。`}</p>\n    <h2 {...{\n      \"id\": \"先决条件\"\n    }}>{`先决条件`}</h2>\n    <p>{`配置持久化存储，需要用拥有 `}<inlineCode parentName=\"p\">{`管理卷（Manage Volumes）`}</inlineCode>{` 权限的`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/admin-settings/rbac/cluster-project-roles/_index\"\n      }}>{`角色`}</a>{`的用户。`}</p>\n    <p>{`如果要在基础设施提供商托管的集群中设置存储，则需要使来自同一个基础设施提供商的存储和集群主机。在 Rancher 中对接新的云存储，必须配置 Cloud Provider，关于如何配置的详细信息可以浏览`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-provisioning/rke-clusters/cloud-providers/_index\"\n      }}>{`这里`}</a>{`。`}</p>\n    <p>{`如果对接现有的存储，则无需配置启用基础设施提供商。`}</p>\n    <h2 {...{\n      \"id\": \"设置现有的存储\"\n    }}>{`设置现有的存储`}</h2>\n    <p>{`设置现有存储的总体流程如下：`}</p>\n    <ol>\n      <li parentName=\"ol\">{`在基础架构提供商中启动持久化存储。`}</li>\n      <li parentName=\"ol\">{`添加一个持久卷（PV）指向上面启动的持久化存储。`}</li>\n      <li parentName=\"ol\">{`添加一个持久卷声明（PVC）指向上面添加的持久卷。`}</li>\n      <li parentName=\"ol\">{`把上面添加的 PVC 挂载到对应的工作负载。`}</li>\n    </ol>\n    <p>{`更多的细节和要求，请参阅`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/volumes-and-storage/attaching-existing-storage/_index\"\n      }}>{`这里`}</a>{`。`}</p>\n    <h2 {...{\n      \"id\": \"在-rancher-中动态设置新存储\"\n    }}>{`在 Rancher 中动态设置新存储`}</h2>\n    <p>{`设置新存储的总体流程如下：`}</p>\n    <ol>\n      <li parentName=\"ol\">{`添加一个存储类并配置其使用对应的存储提供商。`}</li>\n      <li parentName=\"ol\">{`添加一个持久卷声明（PVC）指向上面添加的存储类型。`}</li>\n      <li parentName=\"ol\">{`把上面添加的 PVC 挂载到对应的工作负载。`}</li>\n    </ol>\n    <p>{`更多的细节和要求，请参阅`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/volumes-and-storage/provisioning-new-storage/_index\"\n      }}>{`这里`}</a>{`。`}</p>\n    <h2 {...{\n      \"id\": \"存储设置的实例\"\n    }}>{`存储设置的实例`}</h2>\n    <p>{`我们提供了一些例子来展示如何进行存储设置：`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/volumes-and-storage/examples/nfs/_index\"\n      }}>{`NFS`}</a>{`，`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/volumes-and-storage/examples/vsphere/_index\"\n      }}>{`vSphere`}</a>{` 以及`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/volumes-and-storage/examples/ebs/_index\"\n      }}>{`亚马逊 EBS`}</a>{`。`}</p>\n    <h2 {...{\n      \"id\": \"glusterfs-卷\"\n    }}>{`GlusterFS 卷`}</h2>\n    <p>{`在 `}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-provisioning/rke-clusters/_index\"\n      }}>{`Rancher 启动的 Kubernetes 集群`}</a>{`里，将数据存储到 GlusterFS 卷时，您可能会遇到一个问题：在重启 `}<inlineCode parentName=\"p\">{`kubelet`}</inlineCode>{` 之后，Pod 无法安装卷。关于如何防止这种情况发生的相关细节，可以参阅`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/volumes-and-storage/glusterfs-volumes/_index\"\n      }}>{`这里`}</a>{`。`}</p>\n    <h2 {...{\n      \"id\": \"iscsi-卷\"\n    }}>{`iSCSI 卷`}</h2>\n    <p>{`在 `}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-provisioning/rke-clusters/_index\"\n      }}>{`Rancher 启动的 Kubernetes 集群`}</a>{`里，将数据存储到 iSCSI 卷时，您可能会遇到一个问题：`}<inlineCode parentName=\"p\">{`kubelet`}</inlineCode>{` 无法自动连接 iSCSI 卷。关于如何解决这个问题的相关细节，可以参阅`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/volumes-and-storage/iscsi-volumes/_index\"\n      }}>{`这里`}</a>{`。`}</p>\n    <h2 {...{\n      \"id\": \"hostpath-卷\"\n    }}>{`HostPath 卷`}</h2>\n    <p>{`在创建 HostPath 卷之前，您需要在您的集群配置文件中设置 `}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rke/config-options/services/services-extras/_index\"\n      }}>{`extra_bind`}</a>{`。这会将这些路径作为卷挂载到您的 `}<inlineCode parentName=\"p\">{`kubelet`}</inlineCode>{` 中，然后可以在工作负载中通过 hostPath 卷使用它们。`}</p>\n    <h2 {...{\n      \"id\": \"相关链接\"\n    }}>{`相关链接`}</h2>\n    <ul>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"https://kubernetes.io/docs/concepts/storage/\"\n        }}>{`Kubernetes 存储文档`}</a>{`。`}</li>\n    </ul>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}