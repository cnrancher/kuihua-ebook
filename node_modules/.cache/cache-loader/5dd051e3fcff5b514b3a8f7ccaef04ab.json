AAR,CAEK,aAFL,CA7BK,CAgCL,aAAK,yBAAL,CAA8B,kBAAG,UAAU,CAAC,GAAd,EAAsB,CAChD,OAAQ,2BADwC,CAAtB,EAEvB,aAFuB,CAA9B,CAE0B,qCAF1B,CAE+D,kBAAY,UAAU,CAAC,GAAvB,EAA6B,SAA7B,CAF/D,CAEoH,MAFpH,CAhCK,CAmCL,SAAQ,CACN,KAAM,aADA,CAAR,CAEK,aAFL,CAnCK,CAsCL,aAAK,+CAAL,CAAoD,kBAAG,UAAU,CAAC,GAAd,EAAsB,CACtE,OAAQ,8CAD8D,CAAtB,EAE7C,SAF6C,CAApD,CAEsB,0CAFtB,CAEgE,cAAQ,UAAU,CAAC,GAAnB,EAAyB,aAAzB,CAFhE,CAEiH,sBAFjH,CAtCK,CAAP,CA0CD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '访问集群',\n\tdescription: '您可以通过多种方式与由 Rancher 管理的 Kubernetes 集群进行交互，例如，Rancher UI、kubectl、Rancher kubectl shell、终端远程连接、Rancher CLI、Rancher API等。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'集群管理员指南',\n\t\t'集群访问控制',\n\t\t'访问集群'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/cluster-admin/cluster-access/_index\",\n  \"title\": \"访问集群\",\n  \"description\": \"您可以通过多种方式与由 Rancher 管理的 Kubernetes 集群进行交互，例如，Rancher UI、kubectl、Rancher kubectl shell、终端远程连接、Rancher CLI、Rancher API等。\",\n  \"source\": \"@site/docs/rancher2/cluster-admin/cluster-access/_index.md\",\n  \"permalink\": \"/docs/rancher2/cluster-admin/cluster-access/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/cluster-admin/cluster-access/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar\": \"rancher2\",\n  \"previous\": {\n    \"title\": \"集群管理概览\",\n    \"permalink\": \"/docs/rancher2/cluster-admin/_index\"\n  },\n  \"next\": {\n    \"title\": \"通过 Kubectl 和 kubeconfig 访问集群\",\n    \"permalink\": \"/docs/rancher2/cluster-admin/cluster-access/kubectl/_index\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: 'Rancher UI',\n  id: 'rancher-ui',\n  children: []\n}, {\n  value: 'kubectl',\n  id: 'kubectl',\n  children: [{\n    value: 'Rancher kubectl Shell',\n    id: 'rancher-kubectl-shell',\n    children: []\n  }, {\n    value: '终端远程连接',\n    id: '终端远程连接',\n    children: []\n  }]\n}, {\n  value: 'Rancher CLI',\n  id: 'rancher-cli',\n  children: []\n}, {\n  value: 'Rancher API',\n  id: 'rancher-api',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p>{`您可以通过多种方式与由 Rancher 管理的 Kubernetes 集群进行交互：`}</p>\n    <h2 {...{\n      \"id\": \"rancher-ui\"\n    }}>{`Rancher UI`}</h2>\n    <p>{`Rancher 为与集群交互提供了直观的用户界面。UI 中所有可用的选项都调用 Rancher API。因此，在 UI 中任何可行的操作在 Rancher CLI 或 Rancher API 中也是可行的。`}</p>\n    <h2 {...{\n      \"id\": \"kubectl\"\n    }}>{`kubectl`}</h2>\n    <p>{`您可以使用 Kubernetes 命令行工具`}<a parentName=\"p\" {...{\n        \"href\": \"https://kubernetes.io/docs/reference/kubectl/overview/\"\n      }}>{`kubectl`}</a>{`来管理您的集群，使用 kubectl 有两种选择：`}</p>\n    <h3 {...{\n      \"id\": \"rancher-kubectl-shell\"\n    }}>{`Rancher kubectl Shell`}</h3>\n    <p>{`通过启动 Rancher UI 中可用的 kubectl Shell 与集群进行交互。此选项不需要您进行任何配置操作。`}</p>\n    <p>{`有关更多信息，请参见 `}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/cluster-access/kubectl/_index\"\n      }}>{`使用 kubectl Shell 访问集群`}</a>{`.`}</p>\n    <h3 {...{\n      \"id\": \"终端远程连接\"\n    }}>{`终端远程连接`}</h3>\n    <p>{`您还可以通过在本地桌面安装 `}<a parentName=\"p\" {...{\n        \"href\": \"https://kubernetes.io/docs/tasks/tools/install-kubectl/\"\n      }}>{`kubectl`}</a>{` 来与集群进行交互，然后将集群的 kubeconfig 文件复制到本地 `}<inlineCode parentName=\"p\">{`~/.kube/config`}</inlineCode>{` 目录中。`}</p>\n    <p>{`有关更多信息，请参见`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/cluster-access/kubectl/_index\"\n      }}>{`使用 kubectl 和 kubeconfig 文件访问集群`}</a>{`。`}</p>\n    <h2 {...{\n      \"id\": \"rancher-cli\"\n    }}>{`Rancher CLI`}</h2>\n    <p>{`您可以使用 Rancher CLI 控制集群，`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cli/_index\"\n      }}>{`Rancher CLI`}</a>{`。 这个 CLI 工具可以直接与不同的集群和项目交互，或者向它们传递 `}<inlineCode parentName=\"p\">{`kubectl`}</inlineCode>{` 命令。`}</p>\n    <h2 {...{\n      \"id\": \"rancher-api\"\n    }}>{`Rancher API`}</h2>\n    <p>{`您可以通过 Rancher API 与集群进行交互。在使用 API 之前，您必须获得一个 `}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/user-settings/api-keys/_index\"\n      }}>{`API Key`}</a>{`，要查看 API 对象的不同资源字段和操作，请打开 API UI，可以通过单击 `}<strong parentName=\"p\">{`View in API`}</strong>{` 来访问任何 Rancher UI 对象`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}