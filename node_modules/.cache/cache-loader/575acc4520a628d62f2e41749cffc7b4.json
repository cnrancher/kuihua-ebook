P,CAuCD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '流水线中的概念',\n\tdescription: '本页面讲说的是与流水线相关的常见概念和术语。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'用户指南',\n\t\t'流水线',\n\t\t'流水线中概念'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/pipelines/concepts/_index\",\n  \"title\": \"流水线中的概念\",\n  \"description\": \"本页面讲说的是与流水线相关的常见概念和术语。\",\n  \"source\": \"@site/docs/rancher2/pipelines/concepts/_index.md\",\n  \"permalink\": \"/docs/rancher2/pipelines/concepts/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/pipelines/concepts/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar\": \"rancher2\",\n  \"previous\": {\n    \"title\": \"功能介绍\",\n    \"permalink\": \"/docs/rancher2/pipelines/_index\"\n  },\n  \"next\": {\n    \"title\": \"流水线配置参考\",\n    \"permalink\": \"/docs/rancher2/pipelines/config/_index\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p>{`本页面讲说的是与流水线相关的常见概念和术语。`}</p>\n    <ul>\n      <li parentName=\"ul\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`流水线：`}</strong></p>\n        <p parentName=\"li\">{`一个流水线主要包括阶段和步骤两个部分。它是基于一个特定的代码库，定义了您的代码从构建，测试到部署的过程。Rancher 采用了`}<a parentName=\"p\" {...{\n            \"href\": \"https://jenkins.io/doc/book/pipeline-as-code/\"\n          }}>{`流水线即代码`}</a>{`的模式。流水线配置被作为源代码库里的一个流水线文件存在，它使用的文件名为`}<inlineCode parentName=\"p\">{`.rancher-pipeline.yml`}</inlineCode>{` 或者 `}<inlineCode parentName=\"p\">{`.rancher-pipeline.yaml`}</inlineCode>{`。`}</p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`阶段：`}</strong></p>\n        <p parentName=\"li\">{`一个流水线的阶段包括多个步骤。阶段是按照流水线文件中定义的顺序执行，而阶段中的步骤也是同步顺序执行。当前一个阶段中的所有步骤都正确执行完毕下一个阶段才开始执行。`}</p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`步骤：`}</strong></p>\n        <p parentName=\"li\">{`流水线的“步骤”在指定阶段内执行。如果步骤以“0”以外的代码退出，则该步骤失败。如果步骤以一个失败码退出，则整个流水线将失败并且终止。`}</p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`工作空间：`}</strong></p>\n        <p parentName=\"li\">{`工作空间是所有流水线步骤共享的工作目录。在流水线的开始时，Rancher 会将源代码提取到工作空间。每个步骤的命令都会在工作空间中执行。在流水线执行期间，来自上一步的制品将在以后的步骤中可用。工作目录是一个临时卷，当流水线执行完成时，Rancher 将对其进行清理。`}</p>\n      </li>\n    </ul>\n    <p>{`通常，流水线阶段包括：`}</p>\n    <ul>\n      <li parentName=\"ul\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`构建：`}</strong></p>\n        <p parentName=\"li\">{`每次将代码推送到代码库时，流水线都会自动克隆代码库并构建软件的新迭代。在整个过程中，通常会通过自动化测试来检查软件。`}</p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`发布：`}</strong></p>\n        <p parentName=\"li\">{`构建完成后，将构建 Docker 镜像并将其发布到 Docker 镜像仓库，或者发布应用商店应用模板。`}</p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`部署：`}</strong></p>\n      </li>\n    </ul>\n    <p>{`构建完成后，您可以发布您的应用程序，以便用户可以开始使用更新的产品。`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}