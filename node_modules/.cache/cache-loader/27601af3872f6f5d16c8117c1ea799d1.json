,qBAAM,UAAU,CAAC,KAAjB,EAA2B,EAA3B,EAAiC;CAAjC,CAAL,CA7EK,CA+EL,aAAK,oBAAL,CAAyB,kBAAY,UAAU,CAAC,GAAvB,EAA6B,iBAA7B,CAAzB,CAAsF,IAAtF,CA/EK,CAgFL,aAAK,4CAAL,CAhFK,CAiFL,eAAK,qBAAM,UAAU,CAAC,KAAjB,EAA2B,EAA3B,EAAiC;CAAjC,CAAL,CAjFK,CAmFL,aAAK,uBAAL,CAnFK,CAoFL,eAAK,qBAAM,UAAU,CAAC,KAAjB,EAA2B,EAA3B,EAAiC;;CAAjC,CAAL,CApFK,CAuFL,SAAQ,CACN,KAAM,WADA,CAAR,CAEK,UAFL,CAvFK,CA0FL,eAAK,qBAAM,UAAU,CAAC,KAAjB,EAA2B,EAA3B,EAAiC;;;;;;;;;;;CAAjC,CAAL,CA1FK,CAsGL,SAAQ,CACN,KAAM,WADA,CAAR,CAEK,UAFL,CAtGK,CAyGL,eAAK,qBAAM,UAAU,CAAC,KAAjB,EAA2B,EAA3B,EAAiC;;;;;;;;;;;;;;;;;;;CAAjC,CAAL,CAzGK,CA6HL,aAAK,kBAAL,CA7HK,CA8HL,eAAK,qBAAM,UAAU,CAAC,KAAjB,EAA2B,EAA3B,EAAiC;;CAAjC,CAAL,CA9HK,CAiIL,aAAK,cAAL,CAjIK,CAAP,CAmID,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '卷和存储',\n\tdescription: '当部署一个需要保留数据的应用程序时，你需要创建持久存储。持久存储允许您从运行应用程序的 pod 外部存储应用程序数据。即使应用程序的 pod 发生故障，这种存储方式也可以使您维护应用程序数据。',\n\tkeywords: [\n\t\t'k3s中文文档',\n\t\t'k3s 中文文档',\n\t\t'k3s中文',\n\t\t'k3s 中文',\n\t\t'k3s',\n\t\t'k3s教程',\n\t\t'k3s中国',\n\t\t'rancher',\n\t\t'k3s 中文教程',\n\t\t'卷和存储'\n\t]\n};\nexport const metadata = {\n  \"id\": \"k3s/storage/_index\",\n  \"title\": \"卷和存储\",\n  \"description\": \"当部署一个需要保留数据的应用程序时，你需要创建持久存储。持久存储允许您从运行应用程序的 pod 外部存储应用程序数据。即使应用程序的 pod 发生故障，这种存储方式也可以使您维护应用程序数据。\",\n  \"source\": \"@site/docs/k3s/storage/_index.md\",\n  \"permalink\": \"/docs/k3s/storage/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/k3s/storage/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '设置 Local Storage Provider',\n  id: '设置-local-storage-provider',\n  children: []\n}, {\n  value: '设置 Longhorn',\n  id: '设置-longhorn',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p>{`当部署一个需要保留数据的应用程序时，你需要创建持久存储。持久存储允许您从运行应用程序的 pod 外部存储应用程序数据。即使应用程序的 pod 发生故障，这种存储方式也可以使您维护应用程序数据。`}</p>\n    <p>{`持久卷(PV)是 Kubernetes 集群中的一块存储，而持久卷声明(PVC)是对存储的请求。关于 PV 和 PVC 的工作原理，请参阅有关`}<a parentName=\"p\" {...{\n        \"href\": \"https://kubernetes.io/docs/concepts/storage/volumes/\"\n      }}>{`存储`}</a>{`的 Kubernetes 官方文档。`}</p>\n    <p>{`本页介绍了如何通过 local storage provider 或 `}<a parentName=\"p\" {...{\n        \"href\": \"#%E8%AE%BE%E7%BD%AE-longhorn\"\n      }}>{`Longhorn`}</a>{`来设置持久存储。`}</p>\n    <h2 {...{\n      \"id\": \"设置-local-storage-provider\"\n    }}>{`设置 Local Storage Provider`}</h2>\n    <p>{`K3s 自带 Rancher 的 Local Path Provisioner，这使得能够使用各自节点上的本地存储来开箱即用地创建持久卷声明。下面我们介绍一个简单的例子。有关更多信息，请参考`}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/rancher/local-path-provisioner/blob/master/README.md#usage\"\n      }}>{`此处`}</a>{`的官方文档。`}</p>\n    <p>{`创建一个由 hostPath 支持的持久卷声明和一个使用它的 pod：`}</p>\n    <h4 {...{\n      \"id\": \"pvcyaml\"\n    }}>{`pvc.yaml`}</h4>\n    <pre><code parentName=\"pre\" {...{}}>{`apiVersion: v1\nkind: PersistentVolumeClaim\nmetadata:\n  name: local-path-pvc\n  namespace: default\nspec:\n  accessModes:\n    - ReadWriteOnce\n  storageClassName: local-path\n  resources:\n    requests:\n      storage: 2Gi\n`}</code></pre>\n    <h4 {...{\n      \"id\": \"podyaml\"\n    }}>{`pod.yaml`}</h4>\n    <pre><code parentName=\"pre\" {...{}}>{`apiVersion: v1\nkind: Pod\nmetadata:\n  name: volume-test\n  namespace: default\nspec:\n  containers:\n  - name: volume-test\n    image: nginx:stable-alpine\n    imagePullPolicy: IfNotPresent\n    volumeMounts:\n    - name: volv\n      mountPath: /data\n    ports:\n    - containerPort: 80\n  volumes:\n  - name: volv\n    persistentVolumeClaim:\n      claimName: local-path-pvc\n`}</code></pre>\n    <p>{`应用 yaml:`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`kubectl create -f pvc.yaml\nkubectl create -f pod.yaml\n`}</code></pre>\n    <p>{`确认 PV 和 PVC 已创建：`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`kubectl get pv\nkubectl get pvc\n`}</code></pre>\n    <p>{`状态应该都为 Bound`}</p>\n    <h2 {...{\n      \"id\": \"设置-longhorn\"\n    }}>{`设置 Longhorn`}</h2>\n    <blockquote>\n      <p parentName=\"blockquote\"><strong parentName=\"p\">{`注意：`}</strong>{` 目前 Longhorn 只支持 amd64。`}</p>\n    </blockquote>\n    <p>{`K3s 支持 `}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/longhorn/longhorn\"\n      }}>{`Longhorn`}</a>{`. Longhorn 是 Kubernetes 的一个开源分布式块存储系统。`}</p>\n    <p>{`下面我们介绍一个简单的例子。有关更多信息，请参阅`}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/longhorn/longhorn/blob/master/README.md\"\n      }}>{`此处`}</a>{`的官方文档。`}</p>\n    <p>{`应用 longhorn.yaml 来安装 Longhorn：`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`kubectl apply -f https://raw.githubusercontent.com/longhorn/longhorn/master/deploy/longhorn.yaml\n`}</code></pre>\n    <p>{`Longhorn 将被安装在命名空间`}<inlineCode parentName=\"p\">{`longhorn-system`}</inlineCode>{`中。`}</p>\n    <p>{`在创建 PVC 之前，我们将用这个 yaml 为 Longhorn 创建一个存储类：`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`kubectl create -f https://raw.githubusercontent.com/longhorn/longhorn/master/examples/storageclass.yaml\n`}</code></pre>\n    <p>{`应用 yaml 创建 PVC 和 pod：`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`kubectl create -f pvc.yaml\nkubectl create -f pod.yaml\n`}</code></pre>\n    <h4 {...{\n      \"id\": \"pvcyaml-1\"\n    }}>{`pvc.yaml`}</h4>\n    <pre><code parentName=\"pre\" {...{}}>{`apiVersion: v1\nkind: PersistentVolumeClaim\nmetadata:\n  name: longhorn-volv-pvc\nspec:\n  accessModes:\n    - ReadWriteOnce\n  storageClassName: longhorn\n  resources:\n    requests:\n      storage: 2Gi\n`}</code></pre>\n    <h4 {...{\n      \"id\": \"podyaml-1\"\n    }}>{`pod.yaml`}</h4>\n    <pre><code parentName=\"pre\" {...{}}>{`apiVersion: v1\nkind: Pod\nmetadata:\n  name: volume-test\n  namespace: default\nspec:\n  containers:\n  - name: volume-test\n    image: nginx:stable-alpine\n    imagePullPolicy: IfNotPresent\n    volumeMounts:\n    - name: volv\n      mountPath: /data\n    ports:\n    - containerPort: 80\n  volumes:\n  - name: volv\n    persistentVolumeClaim:\n      claimName: longhorn-volv-pvc\n`}</code></pre>\n    <p>{`确认 PV 和 PVC 已创建：`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`kubectl get pv\nkubectl get pvc\n`}</code></pre>\n    <p>{`状态应该都为 Bound`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}