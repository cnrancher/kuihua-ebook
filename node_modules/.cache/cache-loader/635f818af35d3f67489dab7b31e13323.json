,GAAf,EAFrC,CAE+D;CAF/D,CAGA,kCAHA,CAtDK,CA0DL,aAAG,oBAAK,UAAU,CAAC,GAAhB,EAAwB,CACvB,MAAO,+NADgB,CAEvB,MAAO,IAFgB,CAAxB,EAAH,CA1DK,CA8DL,SAAQ,CACN,KAAM,MADA,CAAR,CAEK,MAFL,CA9DK,CAiEL,aAAK;CAAL,CACF,oBAAK,UAAU,CAAC,GAAhB,EAAwB,CAClB,MAAO,+NADW,CAElB,MAAO,IAFW,CAAxB,EADE,CAjEK,CAsEL,aAAK,qFAAL,CAtEK,CAuEL,aAAK,eAAL,CAvEK,CAwEL,aAAK,wCAAL,CAxEK,CAyEL,aAAG,oBAAK,UAAU,CAAC,GAAhB,EAAwB,CACvB,MAAO,+NADgB,CAEvB,MAAO,IAFgB,CAAxB,EAAH,CAzEK,CA6EL,aAAK,yDAAL,CA7EK,CA8EL,aAAK,cAAL,CA9EK,CA+EL,aAAG,oBAAK,UAAU,CAAC,GAAhB,EAAwB,CACvB,MAAO,+NADgB,CAEvB,MAAO,IAFgB,CAAxB,EAAH,CA/EK,CAmFL,aAAK,4GAAL,CAnFK,CAoFL,aAAK,mKAAL,CApFK,CAqFL,SAAQ,CACN,KAAM,MADA,CAAR,CAEK,MAFL,CArFK,CAwFL,aAAK,iDAAL,CAxFK,CAAP,CA0FD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {};\nexport const metadata = {\n  \"id\": \"troubleshooting/Kubernetes/Network相关/Pod内无法访问api-server\",\n  \"title\": \"Pod内无法访问api-server\",\n  \"description\": \"# Pod内无法访问Api-server\",\n  \"source\": \"@site/docs/troubleshooting/Kubernetes/Network相关/Pod内无法访问api-server.md\",\n  \"permalink\": \"/docs/troubleshooting/Kubernetes/Network相关/Pod内无法访问api-server\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/troubleshooting/Kubernetes/Network相关/Pod内无法访问api-server.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar\": \"troubleshooting\",\n  \"previous\": {\n    \"title\": \"Pod IP无法释放导致无法新建Workload\",\n    \"permalink\": \"/docs/troubleshooting/Kubernetes/Network相关/Pod IP无法释放导致无法新建Workload\"\n  },\n  \"next\": {\n    \"title\": \"双网卡网络Macvlan路由配置问题导致vxlan网卡访问丢包\",\n    \"permalink\": \"/docs/troubleshooting/Kubernetes/Network相关/双网卡网络Macvlan路由配置问题导致vxlan网卡访问丢包\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '问题现象',\n  id: '问题现象',\n  children: []\n}, {\n  value: '问题原因',\n  id: '问题原因',\n  children: []\n}, {\n  value: '问题解决',\n  id: '问题解决',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <h1 {...{\n      \"id\": \"pod内无法访问api-server\"\n    }}>{`Pod内无法访问Api-server`}</h1>\n    <table>\n      <thead parentName=\"table\">\n        <tr parentName=\"thead\">\n          <th parentName=\"tr\" {...{\n            \"align\": null\n          }}>{`作者`}</th>\n          <th parentName=\"tr\" {...{\n            \"align\": null\n          }}>{`万绍远`}</th>\n        </tr>\n      </thead>\n      <tbody parentName=\"table\">\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": null\n          }}>{`团队`}</td>\n          <td parentName=\"tr\" {...{\n            \"align\": null\n          }}>{`Filed&&Suppoort`}</td>\n        </tr>\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": null\n          }}>{`编写时间`}</td>\n          <td parentName=\"tr\" {...{\n            \"align\": null\n          }}>{`2020/10/28`}</td>\n        </tr>\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": null\n          }}>{`类型`}</td>\n          <td parentName=\"tr\" {...{\n            \"align\": null\n          }}>{`常见问题-网络相关`}</td>\n        </tr>\n      </tbody>\n    </table>\n    <hr></hr>\n    <h2 {...{\n      \"id\": \"问题现象\"\n    }}>{`问题现象`}</h2>\n    <p>{`部署集群后发现集群内部分系统组件无法正常启动，查看日志报错如下\n`}<img parentName=\"p\" {...{\n        \"src\": \"https://rancher-support-1256858200.cos.ap-shenzhen-fsi.myqcloud.com/Rancher%E5%B8%B8%E8%A7%81%E9%97%AE%E9%A2%98/%E7%BD%91%E7%BB%9C%E7%9B%B8%E5%85%B3/Pod%E5%86%85%E6%97%A0%E6%B3%95%E8%AE%BF%E9%97%AEApi-server/problem_5.png\",\n        \"alt\": null\n      }}></img></p>\n    <p>{`10.43.0.1对应的是连接集群Api-server的地址，此Service记录默认创建在default命名空间中`}</p>\n    <p>{`在集群内启动Buybox:1.28进行测试验证，副本数为2,分布到不同的节点`}</p>\n    <p>{`测试方法:`}<br parentName=\"p\"></br>{`\n`}{`1、POD之间进行跨主机网络通信（pass)`}<br parentName=\"p\"></br>{`\n`}{`2、在POD内telnet 10.43.0.1的443端口（不可通信）`}<br parentName=\"p\"></br>{`\n`}{`3、在POD内ping非本POD所在宿主机的IP（不可通信）  `}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"https://rancher-support-1256858200.cos.ap-shenzhen-fsi.myqcloud.com/Rancher%E5%B8%B8%E8%A7%81%E9%97%AE%E9%A2%98/%E7%BD%91%E7%BB%9C%E7%9B%B8%E5%85%B3/Pod%E5%86%85%E6%97%A0%E6%B3%95%E8%AE%BF%E9%97%AEApi-server/problem_6.png\",\n        \"alt\": null\n      }}></img></p>\n    <h2 {...{\n      \"id\": \"问题原因\"\n    }}>{`问题原因`}</h2>\n    <p>{`canal网络整体转发路径\n`}<img parentName=\"p\" {...{\n        \"src\": \"https://rancher-support-1256858200.cos.ap-shenzhen-fsi.myqcloud.com/Rancher%E5%B8%B8%E8%A7%81%E9%97%AE%E9%A2%98/%E7%BD%91%E7%BB%9C%E7%9B%B8%E5%85%B3/Pod%E5%86%85%E6%97%A0%E6%B3%95%E8%AE%BF%E9%97%AEApi-server/problem_9.png\",\n        \"alt\": null\n      }}></img></p>\n    <p>{`问题原因主要是因为在POD内无法与Master节点的所在宿主机IP进行通信，因为10.43.0.1最终是会通过iptables DNAT到master节点的443端口`}</p>\n    <p>{`在master节点抓包分析`}</p>\n    <p>{`通过抓包可以看见在对应网卡上有收到pod发送过来的icmp的Request包`}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"https://rancher-support-1256858200.cos.ap-shenzhen-fsi.myqcloud.com/Rancher%E5%B8%B8%E8%A7%81%E9%97%AE%E9%A2%98/%E7%BD%91%E7%BB%9C%E7%9B%B8%E5%85%B3/Pod%E5%86%85%E6%97%A0%E6%B3%95%E8%AE%BF%E9%97%AEApi-server/problem_7.png\",\n        \"alt\": null\n      }}></img></p>\n    <p>{`1、发现源IP还是POD的IP，正常情况下canal访问非集群内网络地址ip会进行SNAT成宿主机的IP。  `}</p>\n    <p>{`查看master节点路由`}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"https://rancher-support-1256858200.cos.ap-shenzhen-fsi.myqcloud.com/Rancher%E5%B8%B8%E8%A7%81%E9%97%AE%E9%A2%98/%E7%BD%91%E7%BB%9C%E7%9B%B8%E5%85%B3/Pod%E5%86%85%E6%97%A0%E6%B3%95%E8%AE%BF%E9%97%AEApi-server/problem_8.png\",\n        \"alt\": null\n      }}></img></p>\n    <p>{`路由走的flannel.1网卡，另外发包的源IP使用的是本机的flannel.1 ip，造成的问题就是默认情况下Linux是开启了反向路径过滤功能，如果一个数据包出入非同一块网卡会被Linux进行反向过滤掉。  `}</p>\n    <p>{`主要原因是因为发出来的数据包没有进行SNAT成宿主机的IP，通过排除，发现是canal配置文件内定义的pod-cidr与实际在controller-manager内的配置冲突，导致POD实际上分配的是controller-manager内配置的所以POD-ip访问外部网络没有自动进行SNAT和flannel.1不进行转发`}</p>\n    <h2 {...{\n      \"id\": \"问题解决\"\n    }}>{`问题解决`}</h2>\n    <p>{`将canal配置文件中的POD-cidr范围修改与controller-manager中一致。`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}