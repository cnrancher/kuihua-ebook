0CL,SAAQ,CACN,KAAM,MADA,CAAR,CAEK,MAFL,CA1CK,CA6CL,aAAK,aAAL,CAAkB,kBAAG,UAAU,CAAC,GAAd,EAAsB,CACpC,OAAQ,8DAD4B,CAAtB,EAEX,KAFW,CAAlB,CAEkB,GAFlB,CAEqB,kBAAG,UAAU,CAAC,GAAd,EAAsB,CACvC,OAAQ,iEAD+B,CAAtB,EAEd,OAFc,CAFrB,CAIoB,GAJpB,CAIuB,kBAAG,UAAU,CAAC,GAAd,EAAsB,CACzC,OAAQ,iEADiC,CAAtB,EAEhB,OAFgB,CAJvB,CAMoB,gCANpB,CA7CK,CAoDL,aAAK,aAAL,CAAkB,kBAAG,UAAU,CAAC,GAAd,EAAsB,CACpC,OAAQ,iEAD4B,CAAtB,EAEX,OAFW,CAAlB,CAEoB,GAFpB,CApDK,CAuDL,SAAQ,CACN,KAAM,MADA,CAAR,CAEK,MAFL,CAvDK,CA0DL,aAAK,0CAAL,CA1DK,CA2DL,cACE,UAAI,UAAU,CAAC,IAAf,EAAsB,GAAtB,CAAyB,cAAQ,UAAU,CAAC,IAAnB,EAA0B,IAA1B,CAAzB,CAAkE,gBAAlE,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAsB,SAAtB,CAA+B,cAAQ,UAAU,CAAC,IAAnB,EAA0B,SAA1B,CAA/B,CAFF,CAGE,UAAI,UAAU,CAAC,IAAf,EAAsB,QAAtB,CAHF,CA3DK,CAAP,CAiED,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '项目管理'\n};\nexport const metadata = {\n  \"id\": \"rancher2/project-admin/_index\",\n  \"title\": \"项目管理\",\n  \"description\": \"_项目_ 是 Rancher 中的一个概念，能够帮助您管理 Kubernetes 集群中的命名空间，您可使用项目创建多租户集群，这种集群允许多个用户使用相同的底层资源创建应用，而应用之间不会相互影响。\",\n  \"source\": \"@site/docs/rancher2/project-admin/_index.md\",\n  \"permalink\": \"/docs/rancher2/project-admin/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/project-admin/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar\": \"rancher2\",\n  \"previous\": {\n    \"title\": \"删除状态为 Failed、Evicted 的 Pod\",\n    \"permalink\": \"/docs/rancher2/cluster-admin/kubectl-delete-evicted-pods/_index\"\n  },\n  \"next\": {\n    \"title\": \"添加项目成员\",\n    \"permalink\": \"/docs/rancher2/project-admin/project-members/_index\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '集群、项目和命名空间的关系',\n  id: '集群、项目和命名空间的关系',\n  children: []\n}, {\n  value: '项目的功能',\n  id: '项目的功能',\n  children: []\n}, {\n  value: '认证授权',\n  id: '认证授权',\n  children: []\n}, {\n  value: '切换项目',\n  id: '切换项目',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p><em parentName=\"p\">{`项目`}</em>{` 是 Rancher 中的一个概念，能够帮助您管理 Kubernetes 集群中的命名空间，您可使用项目创建多租户集群，这种集群允许多个用户使用相同的底层资源创建应用，而应用之间不会相互影响。`}</p>\n    <h2 {...{\n      \"id\": \"集群、项目和命名空间的关系\"\n    }}>{`集群、项目和命名空间的关系`}</h2>\n    <p>{`集群、项目和命名空间的从属关系如下：`}</p>\n    <ul>\n      <li parentName=\"ul\">{`集群包含项目`}</li>\n      <li parentName=\"ul\">{`项目包含命名空间`}</li>\n    </ul>\n    <h2 {...{\n      \"id\": \"项目的功能\"\n    }}>{`项目的功能`}</h2>\n    <p>{`您可以使用 Rancher 的项目功能，一次性管理多个命名空间。在原生 Kubernetes 里面没有项目这个功能，所以角色权限控制、集群资源等功能只能分配给单个命名空间。在某些集群中，如果多个命名空间需要分配同样的访问权限，分配权限会变成一项很麻烦的工作。虽然所有的命名空间需要的权限都是一样的，但是执行分配权限的操作次数和命名空间的数量是相同的，不能执行一次操作就将权限分配给所有的命名空间。Rancher 的项目功能则完美地解决了这个痛点，将多个命名空间看做一个整体，把访问权限和资源分配给这个整体，避免了重复操作。`}</p>\n    <p>{`Rancher 的项目功能允许您在项目层级进行应用资源配置和权限配置，解决了重复配置多个命名空间的问题。项目中的每一个命名空间都继承了项目的资源配置和权限配置，所以您只需要给项目配置一次，而不用逐个配置命名空间。`}</p>\n    <p>{`您可以使用项目进行如下操作：`}</p>\n    <ul>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"/docs/rancher2/project-admin/project-members/_index\"\n        }}>{`设置用户访问权限`}</a></li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"/docs/rancher2/admin-settings/rbac/cluster-project-roles/_index\"\n        }}>{`设置用户角色`}</a>{`，默认的角色包括：项目所有者、项目成员和只读权限，您也可以通过`}<a parentName=\"li\" {...{\n          \"href\": \"/docs/rancher2/admin-settings/rbac/default-custom-roles/_index\"\n        }}>{`自定义角色`}</a>{`，创建新的角色，然后将其分配给其他用户。`}</li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"/docs/rancher2/project-admin/resource-quotas/_index\"\n        }}>{`设置项目资源配额`}</a></li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"/docs/rancher2/project-admin/namespaces/_index\"\n        }}>{`管理命名空间`}</a></li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"/docs/rancher2/project-admin/tools/_index\"\n        }}>{`设置日志、监控等工具`}</a></li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"/docs/rancher2/project-admin/pipelines/_index\"\n        }}>{`设置流水线和 CI/CD 环境`}</a></li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"/docs/rancher2/project-admin/pod-security-policies/_index\"\n        }}>{`设置 Pod 安全策略`}</a></li>\n    </ul>\n    <h2 {...{\n      \"id\": \"认证授权\"\n    }}>{`认证授权`}</h2>\n    <p>{`非 admin 用户在`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/admin-settings/rbac/global-permissions/_index\"\n      }}>{`管理员`}</a>{`、`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/admin-settings/rbac/cluster-project-roles/_index\"\n      }}>{`集群所有者`}</a>{`和`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/admin-settings/rbac/cluster-project-roles/_index\"\n      }}>{`项目所有者`}</a>{`将用户加入到项目成员名单中之前，都不能访问项目内的任何内容。`}</p>\n    <p>{`创建项目的人将自动成为`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/admin-settings/rbac/cluster-project-roles/_index\"\n      }}>{`项目所有者`}</a>{`。`}</p>\n    <h2 {...{\n      \"id\": \"切换项目\"\n    }}>{`切换项目`}</h2>\n    <p>{`您可以使用 Rancher 用户界面中的导航栏和下拉菜单切换项目。操作步骤如下：`}</p>\n    <ol>\n      <li parentName=\"ol\">{`从`}<strong parentName=\"li\">{`全局`}</strong>{`视图，导航到您想切换到的项目`}</li>\n      <li parentName=\"ol\">{`在导航栏中选中`}<strong parentName=\"li\">{`项目/命名空间`}</strong></li>\n      <li parentName=\"ol\">{`单击切换项目`}</li>\n    </ol>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}