AtB,EAE7D,YAF6D,CAApE,CAEyB,yBAFzB,CA/IK,CAkJL,cACE,UAAI,UAAU,CAAC,IAAf,EAAsB,aAAtB,CAAmC,cAAQ,UAAU,CAAC,IAAnB,EAA0B,IAA1B,CAAnC,CAA4E,QAA5E,CAAoF,cAAQ,UAAU,CAAC,IAAnB,EAA0B,IAA1B,CAApF,CAA6H,MAA7H,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAsB,uBAAtB,CAA6C,cAAQ,UAAU,CAAC,IAAnB,EAA0B,uBAA1B,CAA7C,CAAyG,GAAzG,CAFF,CAGE,UAAI,UAAU,CAAC,IAAf,EAAsB,qBAAtB,CAA2C,cAAQ,UAAU,CAAC,IAAnB,EAA0B,IAA1B,CAA3C,CAAoF,GAApF,CAHF,CAlJK,CAuJL,aAAG,cAAQ,UAAU,CAAC,GAAnB,EAAyB,KAAzB,CAAH,CAvJK,CAwJL,cACE,UAAI,UAAU,CAAC,IAAf,EAAsB,iBAAtB,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAsB,eAAtB,CAFF,CAGE,UAAI,UAAU,CAAC,IAAf,EAAsB,IAAtB,CAA0B,kBAAG,UAAU,CAAC,IAAd,EAAuB,CAC7C,OAAQ,uEADqC,CAAvB,EAEnB,gBAFmB,CAA1B,CAE6B,GAF7B,CAHF,CAxJK,CAAP,CAgKD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '创建和修改模板',\n\tdescription: '本节介绍如何管理 RKE 模板和修订版。您可以在全局视图下的工具 > RKE 集群模板中创建，共享，更新和删除模板。模板更新通过修订系统进行处理。模板所有者想要更改或升级模板时，会创建该模板的新修订版。各个修订不能编辑。但是，如果要防止使用某个修订来创建新集群，则可以将其禁用。可以使用两种方式来使用模板修订：创建新集群，或升级使用较早版本的模板创建的集群。模板创建者可以设置默认修订版，但是最终用户创建集群时，他们可以选择任何模板以及可供使用的任何模板修订版。从指定的修订版创建集群后，就无法将其更改为另一个模板，但是可以将集群升级为同一模板的较新的可用修订版。模板所有者对模板修订版具有完全控制权，并且可以创建新的修订版来更新模板，删除或禁用不应被用于创建集群的修订版，和设置默认的模板修订版。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'系统管理员指南',\n\t\t'RKE模板',\n\t\t'创建和修改模板'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/admin-settings/rke-templates/creating-and-revising/_index\",\n  \"title\": \"创建和修改模板\",\n  \"description\": \"本节介绍如何管理 RKE 模板和修订版。您可以在全局视图下的工具 > RKE 集群模板中创建，共享，更新和删除模板。模板更新通过修订系统进行处理。模板所有者想要更改或升级模板时，会创建该模板的新修订版。各个修订不能编辑。但是，如果要防止使用某个修订来创建新集群，则可以将其禁用。可以使用两种方式来使用模板修订：创建新集群，或升级使用较早版本的模板创建的集群。模板创建者可以设置默认修订版，但是最终用户创建集群时，他们可以选择任何模板以及可供使用的任何模板修订版。从指定的修订版创建集群后，就无法将其更改为另一个模板，但是可以将集群升级为同一模板的较新的可用修订版。模板所有者对模板修订版具有完全控制权，并且可以创建新的修订版来更新模板，删除或禁用不应被用于创建集群的修订版，和设置默认的模板修订版。\",\n  \"source\": \"@site/docs/rancher2/admin-settings/rke-templates/creating-and-revising/_index.md\",\n  \"permalink\": \"/docs/rancher2/admin-settings/rke-templates/creating-and-revising/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/admin-settings/rke-templates/creating-and-revising/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '先决条件',\n  id: '先决条件',\n  children: []\n}, {\n  value: '创建模板',\n  id: '创建模板',\n  children: []\n}, {\n  value: '编辑模板',\n  id: '编辑模板',\n  children: []\n}, {\n  value: '删除模板',\n  id: '删除模板',\n  children: []\n}, {\n  value: '基于默认版创建新修订版',\n  id: '基于默认版创建新修订版',\n  children: []\n}, {\n  value: '复制某个修订版',\n  id: '复制某个修订版',\n  children: []\n}, {\n  value: '禁用模板修订版',\n  id: '禁用模板修订版',\n  children: []\n}, {\n  value: '重新启用修订版模板',\n  id: '重新启用修订版模板',\n  children: []\n}, {\n  value: '将模板修订版设置为默认版本',\n  id: '将模板修订版设置为默认版本',\n  children: []\n}, {\n  value: '删除模板修订版',\n  id: '删除模板修订版',\n  children: []\n}, {\n  value: '升级集群以使用新修订版',\n  id: '升级集群以使用新修订版',\n  children: []\n}, {\n  value: '从已有集群中导出到新的 RKE 模板并进行修订',\n  id: '从已有集群中导出到新的-rke-模板并进行修订',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p>{`本节介绍如何管理 RKE 模板，您可以在`}<strong parentName=\"p\">{`全局`}</strong>{`视图下的`}<strong parentName=\"p\">{`工具 > RKE 集群模板`}</strong>{`中创建、共享、编辑或删除 RKE 模板。`}</p>\n    <p>{`模板更新通过修订系统进行处理。模板所有者想要更改或升级模板时，会创建该模板的新修订版。各个修订不能编辑。但是，如果要防止使用某个修订来创建新集群，则可以将其禁用。`}</p>\n    <p>{`可以使用两种方式来使用模板修订：创建新集群，或升级使用较早版本的模板创建的集群。模板创建者可以设置默认修订版，但是最终用户创建集群时，他们可以选择任何模板以及可供使用的任何模板修订版。从指定的修订版创建集群后，就无法将其更改为另一个模板，但是可以将集群升级为同一模板的较新的可用修订版。`}</p>\n    <p>{`模板所有者对模板修订版具有完全控制权，并且可以创建新的修订版来更新模板，删除或禁用不应被用于创建集群的修订版，和设置默认的模板修订版。`}</p>\n    <h2 {...{\n      \"id\": \"先决条件\"\n    }}>{`先决条件`}</h2>\n    <p>{`用户需要有`}<strong parentName=\"p\">{`创建 RKE 模板`}</strong>{`权限，才可以`}<strong parentName=\"p\">{`创建 RKE 模板`}</strong>{`，该权限可以`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/admin-settings/rke-templates/creator-permissions/_index\"\n      }}>{`由系统管理员配置`}</a>{`。`}</p>\n    <p>{`如果您是模板的所有者，则可以修改、共享或删除模板，详情请参考`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/admin-settings/rke-templates/template-access-and-sharing/_index\"\n      }}>{`有关共享模板所有权的文档`}</a>{`。`}</p>\n    <h2 {...{\n      \"id\": \"创建模板\"\n    }}>{`创建模板`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`在`}<strong parentName=\"li\">{`全局`}</strong>{`视图中，单击`}<strong parentName=\"li\">{`工具 > RKE 集群模板`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`单击`}<strong parentName=\"li\">{`添加模板`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`输入模板的`}<strong parentName=\"li\">{`名称`}</strong>{`。Rancher 为模板的第一个版本提供了自动生成的名称，您也可以按个人喜好修改名称，该名称随该模板一起创建。`}</li>\n      <li parentName=\"ol\">{`可选：通过`}<a parentName=\"li\" {...{\n          \"href\": \"/docs/rancher2/admin-settings/rke-templates/template-access-and-sharing/_index\"\n        }}>{`将其他用户或组添加为成员`}</a>{`与其他用户或组共享模板。您还可以将模板公开，以与 Rancher 中的所有用户共享。`}</li>\n      <li parentName=\"ol\">{`按照屏幕上的表单将集群配置参数保存为模板修订版的一部分。可以将该版本标记为该模板的默认版本。`}</li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{` 配置了一个修订版的 RKE 模板。当您启动`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-provisioning/rke-clusters/_index\"\n      }}>{`RKE 集群`}</a>{`时，可以使用此 RKE 模板修订版。通过 RKE 模板管理集群后，集群无法解除与模板的绑定，并且无法取消选中`}<strong parentName=\"p\">{`使用现有 RKE 模板和修订版`}</strong>{`。`}</p>\n    <h2 {...{\n      \"id\": \"编辑模板\"\n    }}>{`编辑模板`}</h2>\n    <p>{`当现有模板不能满足业务需求时，您可以修改现有模板的配置参数，创建一个基于现有模板的修订版模板。创建修订版模板的过程和创建普通版的模板没有明显差别，请参考上文操作步骤。创建修订版模板与创建普通版的模板的区别如下：`}</p>\n    <ul>\n      <li parentName=\"ul\">{`创建修订版模板的过程中，修改的参数只对修订版模板有效，不对已有模板生效。`}</li>\n      <li parentName=\"ul\">{`创建新的模板修订版后，使用现有模板的集群不受修订版影响，仍可正常使用。您可以继续使用该集群，或将该集群的模板升级为修订版。`}</li>\n      <li parentName=\"ul\">{`只能创建现有模板的修订版，而不能基于修订版再创建修订版的修订版模板。`}</li>\n      <li parentName=\"ul\">{`如果您创建了修订版，且暂时不需要用户使用该模板，您可以`}<a parentName=\"li\" {...{\n          \"href\": \"#%E7%A6%81%E7%94%A8%E6%A8%A1%E6%9D%BF%E4%BF%AE%E8%AE%A2%E7%89%88\"\n        }}>{`禁用修订版`}</a>{`，禁止用户使用该模板。`}</li>\n    </ul>\n    <ol>\n      <li parentName=\"ol\">{`在`}<strong parentName=\"li\">{`全局`}</strong>{`视图中，单击`}<strong parentName=\"li\">{`工具 > RKE 集群模板`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`转到要编辑的模板，然后单击`}<strong parentName=\"li\">{`垂直省略号(...) > 升级`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`编辑所需的信息，然后单击`}<strong parentName=\"li\">{`保存`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`可选：您可以更改此模板的默认版本，也可以更改与谁共享。`}</li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{` 模板已更新。要将其应用于使用较旧版本模板的集群，请参阅`}<a parentName=\"p\" {...{\n        \"href\": \"#%E5%8D%87%E7%BA%A7%E9%9B%86%E7%BE%A4%E4%BB%A5%E4%BD%BF%E7%94%A8%E6%96%B0%E4%BF%AE%E8%AE%A2%E7%89%88\"\n      }}>{`升级集群以使用模板的新修订版`}</a>{`。`}</p>\n    <h2 {...{\n      \"id\": \"删除模板\"\n    }}>{`删除模板`}</h2>\n    <p>{`当不再需要对任何集群使用某个 RKE 模板时，可以将其删除。`}</p>\n    <ol>\n      <li parentName=\"ol\">{`在`}<strong parentName=\"li\">{`全局`}</strong>{`视图中，单击`}<strong parentName=\"li\">{`工具 > RKE 集群模板`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`转到要删除的 RKE 模板，然后单击`}<strong parentName=\"li\">{`垂直省略号(...) > 删除`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`出现提示时，确认删除。`}</li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{` 模板被删除。`}</p>\n    <h2 {...{\n      \"id\": \"基于默认版创建新修订版\"\n    }}>{`基于默认版创建新修订版`}</h2>\n    <p>{`您可以复制默认模板修订版并快速更新其设置，而无需从头开始创建新修订版。复制模板为您节省了重新输入集群创建所需的访问密钥和其他参数的时间。`}</p>\n    <ol>\n      <li parentName=\"ol\">{`在`}<strong parentName=\"li\">{`全局`}</strong>{`视图中，单击`}<strong parentName=\"li\">{`工具 > RKE 集群模板`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`转到要复制的 RKE 模板，然后单击`}<strong parentName=\"li\">{`垂直省略号(...) > 新修订的默认值`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`完成表单的其余部分以创建新修订。`}</li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{` 复制并配置了 RKE 模板修订版。`}</p>\n    <h2 {...{\n      \"id\": \"复制某个修订版\"\n    }}>{`复制某个修订版`}</h2>\n    <p>{`通过用户设置创建新的 RKE 模板修订版时，可以复制现有修订版并快速更新其设置，而无需从头开始创建新的修订版。复制模板修订版可省去重新输入集群参数的麻烦。`}</p>\n    <ol>\n      <li parentName=\"ol\">{`在`}<strong parentName=\"li\">{`全局`}</strong>{`视图中，单击`}<strong parentName=\"li\">{`工具 > RKE 集群模板`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`转到要复制的模板修订版。然后选择`}<strong parentName=\"li\">{`省略号>复制修订`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`修改您需要改动的参数。`}</li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{` 复制并配置了 RKE 模板修订版。设置集群后，可以在以后使用 RKE 模板修订版。使用此 RKE 模板的任何现有集群都可以升级到此新修订版。`}</p>\n    <h2 {...{\n      \"id\": \"禁用模板修订版\"\n    }}>{`禁用模板修订版`}</h2>\n    <p>{`当您不需要将 RKE 模板修订版本用于创建新集群时，为了防止用户误触，可以禁用修订版模板。当您需要使用该模板时，您可以重新启用该模板。`}</p>\n    <ol>\n      <li parentName=\"ol\">{`在`}<strong parentName=\"li\">{`全局`}</strong>{`视图中，单击`}<strong parentName=\"li\">{`工具 > RKE 集群模板`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`转到要禁用的模板修订版。然后单击`}<strong parentName=\"li\">{`省略号 > 禁用`}</strong>{`，禁用该模板。`}</li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{` RKE 模板修订版不能用于创建新集群。`}</p>\n    <h2 {...{\n      \"id\": \"重新启用修订版模板\"\n    }}>{`重新启用修订版模板`}</h2>\n    <p>{`如果您决定应该使用禁用的 RKE 模板修订版来创建新集群，您可以重新启用该模板。`}</p>\n    <ol>\n      <li parentName=\"ol\">{`在`}<strong parentName=\"li\">{`全局`}</strong>{`视图中，单击`}<strong parentName=\"li\">{`工具 > RKE 集群模板`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`转到您要重新启用的模板修订版。然后选择`}<strong parentName=\"li\">{`省略号>启用`}</strong>{`。`}</li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{` RKE 模板修订版可用于创建新集群。`}</p>\n    <h2 {...{\n      \"id\": \"将模板修订版设置为默认版本\"\n    }}>{`将模板修订版设置为默认版本`}</h2>\n    <p>{`最终用户使用 RKE 模板创建集群时，用户可以选择用于创建集群的版本。您可以配置默认使用哪个修订版。`}</p>\n    <ol>\n      <li parentName=\"ol\">{`在`}<strong parentName=\"li\">{`全局`}</strong>{`视图中，单击`}<strong parentName=\"li\">{`工具 > RKE 集群模板`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`转到应该为默认值的 RKE 模板修订，然后单击`}<strong parentName=\"li\">{`省略号(...) > 设置为缺省`}</strong>{`。`}</li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{` 当使用模板创建集群时，RKE 模板修订版将用作默认选项。`}</p>\n    <h2 {...{\n      \"id\": \"删除模板修订版\"\n    }}>{`删除模板修订版`}</h2>\n    <p>{`您可以删除模板的所有修订版（默认修订版除外）。`}</p>\n    <ol>\n      <li parentName=\"ol\">{`在`}<strong parentName=\"li\">{`全局`}</strong>{`视图中，单击`}<strong parentName=\"li\">{`工具 > RKE 集群模板`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`转到应删除的 RKE 模板修订，然后单击`}<strong parentName=\"li\">{`省略号(...) > 删除`}</strong>{`。`}</li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{` RKE 模板修订已删除。`}</p>\n    <h2 {...{\n      \"id\": \"升级集群以使用新修订版\"\n    }}>{`升级集群以使用新修订版`}</h2>\n    <blockquote>\n      <p parentName=\"blockquote\">{`本节假定您已经有一个集群，其中该集群`}<a parentName=\"p\" {...{\n          \"href\": \"/docs/rancher2/admin-settings/rke-templates/applying-templates/_index\"\n        }}>{`已应用 RKE 模板`}</a>{`。\n本节还假设您已经`}<a parentName=\"p\" {...{\n          \"href\": \"#%E6%9B%B4%E6%96%B0%E6%A8%A1%E6%9D%BF\"\n        }}>{`更新了集群使用的模板`}</a>{`，以便可以使用新的模板修订版。`}</p>\n    </blockquote>\n    <p>{`升级集群以使用新修订版的前提条件是：`}</p>\n    <ul>\n      <li parentName=\"ul\">{`已有使用默认模板创建的集群。`}</li>\n      <li parentName=\"ul\">{`已有基于默认模板创建的修订版模板。`}</li>\n    </ul>\n    <ol>\n      <li parentName=\"ol\">{`从 Rancher 的`}<strong parentName=\"li\">{`全局`}</strong>{`视图中，单击`}<strong parentName=\"li\">{`集群`}</strong>{`选项卡。`}</li>\n      <li parentName=\"ol\">{`转到要升级的集群，然后单击`}<strong parentName=\"li\">{`省略号(...) > 编辑`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`在`}<strong parentName=\"li\">{`集群选项`}</strong>{`部分中，单击模板修订的下拉菜单，然后选择新的模板修订。`}</li>\n      <li parentName=\"ol\">{`单击`}<strong parentName=\"li\">{`保存`}</strong>{`。`}</li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{` 集群已升级为使用新模板修订版中定义的设置。`}</p>\n    <h2 {...{\n      \"id\": \"从已有集群中导出到新的-rke-模板并进行修订\"\n    }}>{`从已有集群中导出到新的 RKE 模板并进行修订`}</h2>\n    <p>{`您可以将现有集群的设置另存为 RKE 模板。这会将集群的设置导出为新的 RKE 模板，并且将集群绑定到该模板。然后，只有在`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/admin-settings/rke-templates/creating-and-revising/_index\"\n      }}>{`更新了 RKE 模板`}</a>{`且集群将升级到较新版本的模板时才可以修改集群。`}</p>\n    <ol>\n      <li parentName=\"ol\">{`从 Rancher 的`}<strong parentName=\"li\">{`全局`}</strong>{`视图中，单击`}<strong parentName=\"li\">{`集群`}</strong>{`选项卡。`}</li>\n      <li parentName=\"ol\">{`转到将转换为使用 RKE 模板的集群。单击`}<strong parentName=\"li\">{`省略号(...) > 另存为 RKE 模板`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`在显示的表单中输入模板的名称，然后单击`}<strong parentName=\"li\">{`创建`}</strong>{`。`}</li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong></p>\n    <ul>\n      <li parentName=\"ul\">{`创建了一个新的 RKE 模板。`}</li>\n      <li parentName=\"ul\">{`将集群转换为使用该新模板。`}</li>\n      <li parentName=\"ul\">{`可以`}<a parentName=\"li\" {...{\n          \"href\": \"/docs/rancher2/admin-settings/rke-templates/applying-templates/_index\"\n        }}>{`从新模板和修订版中创建新集群`}</a>{`。`}</li>\n    </ul>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}