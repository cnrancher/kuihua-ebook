,CAvGK,CAwGL,SAAQ,CACN,KAAM,SADA,CAAR,CAEK,SAFL,CAxGK,CA2GL,aAAK,YAAL,CAAiB,kBAAG,UAAU,CAAC,GAAd,EAAsB,CACnC,OAAQ,8DAD2B,CAAtB,EAEV,WAFU,CAAjB,CAEwB,iBAFxB,CAEyC,kBAAY,UAAU,CAAC,GAAvB,EAA6B,QAA7B,CAFzC,CAE6F,qBAF7F,CAEkH,kBAAG,UAAU,CAAC,GAAd,EAAsB,CACpI,OAAQ,uFAD4H,CAAtB,EAE3G,YAF2G,CAFlH,CAIyB,KAJzB,CA3GK,CAAP,CAiHD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '网络插件'\n};\nexport const metadata = {\n  \"id\": \"rke/config-options/add-ons/network-plugins/_index\",\n  \"title\": \"网络插件\",\n  \"description\": \"RKE 提供了以下网络插件，作为附加组件部署：\",\n  \"source\": \"@site/docs/rke/config-options/add-ons/network-plugins/_index.md\",\n  \"permalink\": \"/docs/rke/config-options/add-ons/network-plugins/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rke/config-options/add-ons/network-plugins/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '禁用网络插件 Deployment',\n  id: '禁用网络插件-deployment',\n  children: []\n}, {\n  value: '网络插件选项',\n  id: '网络插件选项',\n  children: [{\n    value: 'Canal 插件选项',\n    id: 'canal-插件选项',\n    children: []\n  }, {\n    value: 'Canal 接口',\n    id: 'canal-接口',\n    children: []\n  }, {\n    value: 'Flannel 插件选项',\n    id: 'flannel-插件选项',\n    children: []\n  }, {\n    value: 'Flannel 接口',\n    id: 'flannel-接口',\n    children: []\n  }, {\n    value: 'Calico 插件选项',\n    id: 'calico-插件选项',\n    children: []\n  }, {\n    value: 'Calico 云服务提供商',\n    id: 'calico-云服务提供商',\n    children: []\n  }, {\n    value: 'Weave 插件选项',\n    id: 'weave-插件选项',\n    children: []\n  }, {\n    value: 'Weave 加密',\n    id: 'weave-加密',\n    children: []\n  }, {\n    value: '自定义网络插件',\n    id: '自定义网络插件',\n    children: []\n  }]\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p>{`RKE 提供了以下网络插件，作为附加组件部署：`}</p>\n    <ul>\n      <li parentName=\"ul\">{`Flannel`}</li>\n      <li parentName=\"ul\">{`Calico`}</li>\n      <li parentName=\"ul\">{`Canal`}</li>\n      <li parentName=\"ul\">{`Weave`}</li>\n    </ul>\n    <blockquote>\n      <p parentName=\"blockquote\"><strong parentName=\"p\">{`注意：`}</strong>{`启动集群后，不能更改网络提供商。因此，请仔细选择您要使用的网络提供商，因为 Kubernetes 不允许切换网络提供商之。一旦使用网络提供商创建集群，更换网络提供商就需要重建整个集群及其所有应用。`}</p>\n    </blockquote>\n    <p>{`默认情况下，RKE 使用的网络插件是`}<inlineCode parentName=\"p\">{`canal`}</inlineCode>{`。如果你想使用另一个网络插件，你需要在`}<inlineCode parentName=\"p\">{`cluster.yml`}</inlineCode>{`中指定在集群级别启用哪个网络插件。`}</p>\n    <pre><code parentName=\"pre\" {...{\n        \"className\": \"language-yaml\"\n      }}>{`# 设置flannel网络插件\nnetwork:\n  plugin: flannel\n`}</code></pre>\n    <p>{`网络插件使用的镜像在`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rke/config-options/system-images/_index\"\n      }}><inlineCode parentName=\"a\">{`system_images`}</inlineCode></a>{`中。每个 Kubernetes 版本，都有与每个网络插件相关联的默认镜像，但这些镜像可以通过更改`}<inlineCode parentName=\"p\">{`system_images`}</inlineCode>{`中的镜像标签来覆盖。`}</p>\n    <h2 {...{\n      \"id\": \"禁用网络插件-deployment\"\n    }}>{`禁用网络插件 Deployment`}</h2>\n    <p>{`您可以将集群配置中的 `}<inlineCode parentName=\"p\">{`plugin`}</inlineCode>{`设置为`}<inlineCode parentName=\"p\">{`none`}</inlineCode>{`，禁用默认的 网络插件 Deployment。`}</p>\n    <pre><code parentName=\"pre\" {...{\n        \"className\": \"language-yaml\"\n      }}>{`network:\n  plugin: none\n`}</code></pre>\n    <h2 {...{\n      \"id\": \"网络插件选项\"\n    }}>{`网络插件选项`}</h2>\n    <p>{`除了可以用来部署网络插件的不同镜像外，有些网络插件还支持配置其他选项。`}</p>\n    <h3 {...{\n      \"id\": \"canal-插件选项\"\n    }}>{`Canal 插件选项`}</h3>\n    <pre><code parentName=\"pre\" {...{\n        \"className\": \"language-yaml\"\n      }}>{`network:\n  plugin: canal\n  options:\n    canal_iface: eth1\n    canal_flannel_backend_type: vxlan\n`}</code></pre>\n    <h3 {...{\n      \"id\": \"canal-接口\"\n    }}>{`Canal 接口`}</h3>\n    <p>{`通过设置`}<inlineCode parentName=\"p\">{`canal_iface`}</inlineCode>{`，可以配置主机间通信使用的接口。\n`}<inlineCode parentName=\"p\">{`canal_flannel_backend_type`}</inlineCode>{`选项允许你指定要使用的`}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/coreos/flannel/blob/master/Documentation/backends.md\"\n      }}>{`flannel backend`}</a>{`的类型。默认情况下使用`}<inlineCode parentName=\"p\">{`vxlan`}</inlineCode>{`后端。`}</p>\n    <h3 {...{\n      \"id\": \"flannel-插件选项\"\n    }}>{`Flannel 插件选项`}</h3>\n    <pre><code parentName=\"pre\" {...{\n        \"className\": \"language-yaml\"\n      }}>{`network:\n  plugin: flannel\n  options:\n    flannel_iface: eth1\n    flannel_backend_type: vxlan\n`}</code></pre>\n    <h3 {...{\n      \"id\": \"flannel-接口\"\n    }}>{`Flannel 接口`}</h3>\n    <p>{`通过设置`}<inlineCode parentName=\"p\">{`flannel_iface`}</inlineCode>{`，可以配置主机间通信使用的接口。\n`}<inlineCode parentName=\"p\">{`flannel_backend_type`}</inlineCode>{`选项允许你指定要使用的`}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/coreos/flannel/blob/master/Documentation/backends.md\"\n      }}>{`flannel backend`}</a>{`的类型。默认情况下使用`}<inlineCode parentName=\"p\">{`vxlan`}</inlineCode>{`后端。`}</p>\n    <h3 {...{\n      \"id\": \"calico-插件选项\"\n    }}>{`Calico 插件选项`}</h3>\n    <pre><code parentName=\"pre\" {...{\n        \"className\": \"language-yaml\"\n      }}>{`network:\n  plugin: calico\n  options:\n    calico_cloud_provider: aws\n`}</code></pre>\n    <h3 {...{\n      \"id\": \"calico-云服务提供商\"\n    }}>{`Calico 云服务提供商`}</h3>\n    <p>{`Calico 目前只支持 2 个云提供商，AWS 或 GCE，可以使用`}<inlineCode parentName=\"p\">{`calico_cloud_provider`}</inlineCode>{`进行设置。`}</p>\n    <p><strong parentName=\"p\">{`可选值`}</strong></p>\n    <ul>\n      <li parentName=\"ul\"><inlineCode parentName=\"li\">{`aws`}</inlineCode></li>\n      <li parentName=\"ul\"><inlineCode parentName=\"li\">{`gce`}</inlineCode></li>\n    </ul>\n    <h3 {...{\n      \"id\": \"weave-插件选项\"\n    }}>{`Weave 插件选项`}</h3>\n    <pre><code parentName=\"pre\" {...{\n        \"className\": \"language-yaml\"\n      }}>{`network:\n  plugin: weave\n  weave_network_provider:\n    password: \"Q]SZ******oijz\"\n`}</code></pre>\n    <h3 {...{\n      \"id\": \"weave-加密\"\n    }}>{`Weave 加密`}</h3>\n    <p>{`Weave 加密可以通过向网络提供商配置传递一个字符串密码来启用。`}</p>\n    <h3 {...{\n      \"id\": \"自定义网络插件\"\n    }}>{`自定义网络插件`}</h3>\n    <p>{`可以通过 RKE 的`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rke/config-options/add-ons/user-defined-add-ons/_index\"\n      }}>{`用户自定义插件功能`}</a>{`来添加自定义网络插件。可以在 `}<inlineCode parentName=\"p\">{`addons`}</inlineCode>{`字段中添加网络插件的集群的加载清单，如`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rke/config-options/add-ons/network-plugins/custom-network-plugin-example/_index\"\n      }}>{`自定义的网络插件示例`}</a>{`所示。`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}