A0B,cAAQ,UAAU,CAAC,IAAnB,EAA0B,MAA1B,CAA1B,CAAqE,uBAArE,CAA4F,cAAQ,UAAU,CAAC,IAAnB,EAA0B,8BAA1B,CAA5F,CAA+J,GAA/J,CAAkK,cAAQ,UAAU,CAAC,IAAnB,EAA0B,QAA1B,CAAlK,CAA+M,qCAA/M,CAFF,CA7EK,CAiFL,SAAQ,CACN,KAAM,IADA,CAAR,CAEK,IAFL,CAjFK,CAoFL,aAAK,wBAAL,CAA6B,kBAAG,UAAU,CAAC,GAAd,EAAsB,CAC/C,OAAQ,mGADuC,CAAtB,EAEtB,OAFsB,CAA7B,CAEoB,2EAFpB,CApFK,CAuFL,aAAK,uCAAL,CAvFK,CAwFL,aAAK,gFAAL,CAxFK,CAyFL,aAAK,6EAAL,CAzFK,CA0FL,aAAK,6BAAL,CAAkC,kBAAG,UAAU,CAAC,GAAd,EAAsB,CACpD,OAAQ,4EAD4C,CAAtB,EAE3B,UAF2B,CAAlC,CAEuB,GAFvB,CA1FK,CA6FL,SAAQ,CACN,KAAM,MADA,CAAR,CAEK,MAFL,CA7FK,CAgGL,aAAK,iHAAL,CAhGK,CAiGL,aAAG,oBAAK,UAAU,CAAC,GAAhB,EAAwB,CACvB,MAAO,gCADgB,CAEvB,MAAO,wDAFgB,CAAxB,EAAH,CAjGK,CAAP,CAsGD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '功能介绍',\n\tdescription: 'Istio是一种开源工具，可使 DevOps 团队更轻松地观察，控制，故障排查和保护复杂的微服务网络中的流量。随着微服务网络的变化和增长，它们之间的交互会变得更加难以管理和理解。在这种情况下，将服务网格作为单独的基础结构层很有用。 Istio 的服务网格使您可以管理微服务之间的流量，而无需直接更改微服务。我们与 Istio 的集成旨在使 Rancher 运维（例如管理员或集群所有者）可以将 Istio 交付给开发人员。然后，开发人员可以使用 Istio 实施安全策略，解决问题或管理绿色/蓝色部署，金丝雀部署或 A / B 测试的流量。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'Istio',\n\t\t'功能介绍'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/istio/2.3.x-2.4.x/_index\",\n  \"title\": \"功能介绍\",\n  \"description\": \"Istio是一种开源工具，可使 DevOps 团队更轻松地观察，控制，故障排查和保护复杂的微服务网络中的流量。随着微服务网络的变化和增长，它们之间的交互会变得更加难以管理和理解。在这种情况下，将服务网格作为单独的基础结构层很有用。 Istio 的服务网格使您可以管理微服务之间的流量，而无需直接更改微服务。我们与 Istio 的集成旨在使 Rancher 运维（例如管理员或集群所有者）可以将 Istio 交付给开发人员。然后，开发人员可以使用 Istio 实施安全策略，解决问题或管理绿色/蓝色部署，金丝雀部署或 A / B 测试的流量。\",\n  \"source\": \"@site/docs/rancher2/istio/2.3.x-2.4.x/_index.md\",\n  \"permalink\": \"/docs/rancher2/istio/2.3.x-2.4.x/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/istio/2.3.x-2.4.x/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '先决条件',\n  id: '先决条件',\n  children: []\n}, {\n  value: '设定指南',\n  id: '设定指南',\n  children: []\n}, {\n  value: '禁用 Istio',\n  id: '禁用-istio',\n  children: []\n}, {\n  value: '访问可视化',\n  id: '访问可视化',\n  children: []\n}, {\n  value: '查看 Kiali 流量图',\n  id: '查看-kiali-流量图',\n  children: []\n}, {\n  value: '查看流量指标',\n  id: '查看流量指标',\n  children: []\n}, {\n  value: '架构',\n  id: '架构',\n  children: []\n}, {\n  value: '两个入口',\n  id: '两个入口',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p><em parentName=\"p\">{`从 v2.3.0 版本开始支持`}</em></p>\n    <p>{`在 Rancher 2.5 中，Istio 应用得到了改进。现在有两种方式来启用 Istio。旧的方法在本节中进行了说明，新的 Istio 应用在`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/istio/2.5/_index\"\n      }}>{`这里`}</a>{`中进行了说明。`}</p>\n    <p><a parentName=\"p\" {...{\n        \"href\": \"https://istio.io/\"\n      }}>{`Istio`}</a>{` 是一种开源工具，可以让 DevOps 团队更轻松地监控保护微服务网络中的流量。`}</p>\n    <p>{`随着微服务网络的变化和增长，它们之间的交互会变得更加难以管理。在这种情况下，将 Istio 服务网格作为单独的基础结构，可以操纵微服务之间的流量，而无需直接更改微服务。`}</p>\n    <p>{`Rancher 与 Istio 的集成旨在使 Rancher 运维人员（例如管理员或集群所有者）可以将 Istio 交付给开发人员。然后，开发人员可以使用 Istio 实施安全策略，解决问题或管理绿色/蓝色部署，金丝雀部署或 A / B 测试的流量。`}</p>\n    <p>{`服务网格提供的功能包括但不限于以下功能：`}</p>\n    <ul>\n      <li parentName=\"ul\">{`流量管理功能`}</li>\n      <li parentName=\"ul\">{`增强的监视和跟踪`}</li>\n      <li parentName=\"ul\">{`服务发现和路由`}</li>\n      <li parentName=\"ul\">{`使用双向 TLS 的安全连接和服务到服务的身份验证`}</li>\n      <li parentName=\"ul\">{`负载均衡`}</li>\n      <li parentName=\"ul\">{`自动重试，退避和断路`}</li>\n    </ul>\n    <p>{`在集群中启用 Istio 之后，您可以通过 `}<inlineCode parentName=\"p\">{`kubectl`}</inlineCode>{` 来使用 Istio 的 control-plane 功能。`}</p>\n    <p>{`Rancher 的 Istio 集成附带了全面的可视化辅助工具：`}</p>\n    <ul>\n      <li parentName=\"ul\"><strong parentName=\"li\">{`使用 Jaeger 跟踪错误的根本原因。`}</strong>{` `}<a parentName=\"li\" {...{\n          \"href\": \"https://www.jaegertracing.io/\"\n        }}>{`Jaeger`}</a>{` 是一个开放源代码工具，它为分布式跟踪系统提供 UI，这对于根本原因分析和确定导致性能下降的原因很有用。分布式跟踪使您可以查看整个调用链，这些调用可能源于用户请求并遍历数十个微服务。`}</li>\n      <li parentName=\"ul\"><strong parentName=\"li\">{`使用 Kiali 全面了解您的微服务架构。`}</strong>{` `}<a parentName=\"li\" {...{\n          \"href\": \"https://www.kiali.io/\"\n        }}>{`Kiali`}</a>{` 提供了一个图表，显示了服务网格中的服务及其连接方式，包括流量速率和它们之间的延迟。您可以检查服务网格的运行状况，或深入查看单个组件的传入和传出请求。`}</li>\n      <li parentName=\"ul\"><strong parentName=\"li\">{`使用 Grafana 仪表板从时间序列分析中获得见解。`}</strong>{` `}<a parentName=\"li\" {...{\n          \"href\": \"https://grafana.com/\"\n        }}>{`Grafana`}</a>{` 是一个分析平台，可让您查询，可视化，警告和了解 Prometheus 收集的数据。`}</li>\n      <li parentName=\"ul\"><strong parentName=\"li\">{`使用 Prometheus UI 编写时间序列数据的自定义查询。`}</strong>{` `}<a parentName=\"li\" {...{\n          \"href\": \"https://prometheus.io/\"\n        }}>{`Prometheus`}</a>{` 是系统监视和警报工具包。Prometheus 会从您的集群中抓取数据，然后由 Grafana 使用。Prometheus UI 也集成到 Rancher 中，使您可以编写时间序列数据的自定义查询并在 UI 中查看结果。`}</li>\n    </ul>\n    <h2 {...{\n      \"id\": \"先决条件\"\n    }}>{`先决条件`}</h2>\n    <p>{`在启用 Istio 之前，我们建议您确认 Rancher worker 节点具有足够的 `}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/istio/2.3.x-2.4.x/resources/_index\"\n      }}>{`CPU 和内存`}</a>{`以运行 Istio 的所有组件。`}</p>\n    <h2 {...{\n      \"id\": \"设定指南\"\n    }}>{`设定指南`}</h2>\n    <p>{`有关如何设置 Istio 并将其在项目中使用的说明，请参考`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/istio/2.3.x-2.4.x/setup/_index\"\n      }}>{`设定指南`}</a>{`。`}</p>\n    <h2 {...{\n      \"id\": \"禁用-istio\"\n    }}>{`禁用 Istio`}</h2>\n    <p>{`要从集群，命名空间或工作负载中删除 Istio 组件，请参阅`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/istio/2.3.x-2.4.x/disabling-istio/_index\"\n      }}>{`禁用 Istio`}</a>{`。`}</p>\n    <h2 {...{\n      \"id\": \"访问可视化\"\n    }}>{`访问可视化`}</h2>\n    <blockquote>\n      <p parentName=\"blockquote\">{`默认情况下，只有集群所有者才能访问 Jaeger 和 Kiali。关于如何允许项目成员访问它们的说明，请参阅`}<a parentName=\"p\" {...{\n          \"href\": \"/docs/rancher2/istio/2.3.x-2.4.x/rbac/_index\"\n        }}>{`访问可视化`}</a>{`。`}</p>\n    </blockquote>\n    <p>{`在集群中设置 Istio 之后，Rancher UI 中将提供 Grafana，Prometheus，Jaeger 和 Kiali 访问。`}</p>\n    <p>{`您对可视化的访问取决于您的角色。Grafana 和 Prometheus 仅适用于集群所有者。默认情况下，Kiali 和 Jaeger UI 仅对集群所有者可用，但是集群所有者可以通过编辑 Istio 设置来允许项目成员访问它们。当您转到项目并单击`}<strong parentName=\"p\">{`资源> Istio`}</strong>{`时，您可以通过单击页面右上角的图标来转到 Kiali，Jaeger，Grafana 和 Prometheus 各自的 UI。`}</p>\n    <p>{`要查看可视化效果，请转到设置 Istio 的集群，然后单击`}<strong parentName=\"p\">{`工具 > Istio`}</strong>{`。您能在页面顶部看到每个 UI 的链接。`}</p>\n    <p>{`您还可以从项目视图访问可视化工具。`}</p>\n    <h2 {...{\n      \"id\": \"查看-kiali-流量图\"\n    }}>{`查看 Kiali 流量图`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`在 Rancher 的项目视图中，单击`}<strong parentName=\"li\">{`资源 > Istio`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`如果您是集群所有者，则可以转到`}<strong parentName=\"li\">{`流量图`}</strong>{`标签页。该标签页将 Kiali 网络可视化集成到 UI 中。`}</li>\n    </ol>\n    <h2 {...{\n      \"id\": \"查看流量指标\"\n    }}>{`查看流量指标`}</h2>\n    <p>{`Istio 的监控功能使您可以查看所有服务的性能。`}</p>\n    <ol>\n      <li parentName=\"ol\">{`在 Rancher 中的项目视图中，单击`}<strong parentName=\"li\">{`资源 > Istio`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`转到`}<strong parentName=\"li\">{`流量指标`}</strong>{`标签。在集群中生成流量之后，您应该能够看到`}<strong parentName=\"li\">{`成功率，请求量，4xx 响应计数，项目 5xx 响应计数`}</strong>{`和`}<strong parentName=\"li\">{`请求持续时间`}</strong>{`的指标。集群所有者可以查看所有指标 ，而项目成员可以查看指标的一部分。`}</li>\n    </ol>\n    <h2 {...{\n      \"id\": \"架构\"\n    }}>{`架构`}</h2>\n    <p>{`Istio 安装了一个服务网格，该网格使用 `}<a parentName=\"p\" {...{\n        \"href\": \"https://www.envoyproxy.io/docs/envoy/v1.15.0/intro/arch_overview/upstream/service_discovery.html/\"\n      }}>{`Envoy`}</a>{` sidecar 代理来拦截到每个工作负载的流量。这些 sidecar 拦截并管理服务到服务的通信，从而可以对集群内的流量进行细粒度的观察和控制。`}</p>\n    <p>{`Istio 只能跟踪和控制已注入 Istio sidecar 的工作负载。`}</p>\n    <p>{`在 Rancher 中启用 Istio 将启用集群监控，并在集群中创建的所有新命名空间中启用 Istio。您需要在预先存在的命名空间中手动启用 Istio。`}</p>\n    <p>{`当命名空间启用 Istio 时，在命名空间中部署的新工作负载将自动具有 Istio sidecar。您需要在预先存在的工作负载中手动启用 Istio。`}</p>\n    <p>{`有关 Istio sidecar 的更多信息，请参阅 `}<a parentName=\"p\" {...{\n        \"href\": \"https://istio.io/docs/setup/kubernetes/additional-setup/sidecar-injection/\"\n      }}>{`Istio 文档`}</a>{`。`}</p>\n    <h2 {...{\n      \"id\": \"两个入口\"\n    }}>{`两个入口`}</h2>\n    <p>{`默认情况下，每个 Rancher 配置的集群都有一个 NGINX ingress 控制器，允许流量进入集群。要允许 Istio 接收外部流量，您需要为集群启用 Istio ingress 网关。结果是您的集群将有两个入口。`}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/istio-ingress.svg\",\n        \"alt\": \"在启用Istio的集群中，您可以有两个入口：默认的Nginx ingress控制器和默认的Istio控制器。\"\n      }}></img></p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}