CAAlC,CAA2E,GAA3E,CAFF,CAFF,CAJF,CAWE,UAAI,UAAU,CAAC,IAAf,EACE,SAAG,UAAU,CAAC,IAAd,EAAqB,KAArB,CAA0B,cAAQ,UAAU,CAAC,GAAnB,EAAyB,IAAzB,CAA1B,CAAkE,YAAlE,CADF,CAXF,CAzDK,CAwEL,aAAG,cAAQ,UAAU,CAAC,GAAnB,EAAyB,KAAzB,CAAH,CAA4C,aAA5C,CAxEK,CAAP,CA0ED,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '管理云凭证',\n\tdescription: '当您创建集群时，需要用到[节点模板](/docs/rancher2/cluster-provisioning/rke-clusters/node-pools/_index)创建集群内的节点。这些模板使用 Docker Machine 中可用的配置选项，定义节点上的操作系统的镜像和配置参数。主机节点可以使用云凭证访问基础设施提供商，并创建节点。一个云凭证可用于多个节点模板。云凭证的优势是，使用同一个云服务提供商创建多个节点模板时，您不需要重复输入多次认证信息。云凭证以 Kubernetes 密文的方式存储。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'用户设置',\n\t\t'管理云凭证'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/user-settings/cloud-credentials/_index\",\n  \"title\": \"管理云凭证\",\n  \"description\": \"当您创建集群时，需要用到[节点模板](/docs/rancher2/cluster-provisioning/rke-clusters/node-pools/_index)创建集群内的节点。这些模板使用 Docker Machine 中可用的配置选项，定义节点上的操作系统的镜像和配置参数。主机节点可以使用云凭证访问基础设施提供商，并创建节点。一个云凭证可用于多个节点模板。云凭证的优势是，使用同一个云服务提供商创建多个节点模板时，您不需要重复输入多次认证信息。云凭证以 Kubernetes 密文的方式存储。\",\n  \"source\": \"@site/docs/rancher2/user-settings/cloud-credentials/_index.md\",\n  \"permalink\": \"/docs/rancher2/user-settings/cloud-credentials/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/user-settings/cloud-credentials/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar\": \"rancher2\",\n  \"previous\": {\n    \"title\": \"管理节点模版\",\n    \"permalink\": \"/docs/rancher2/user-settings/node-templates/_index\"\n  },\n  \"next\": {\n    \"title\": \"用户偏好\",\n    \"permalink\": \"/docs/rancher2/user-settings/preferences/_index\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '概述',\n  id: '概述',\n  children: []\n}, {\n  value: '创建云凭证',\n  id: '创建云凭证',\n  children: []\n}, {\n  value: '更新云凭证',\n  id: '更新云凭证',\n  children: []\n}, {\n  value: '删除云凭证',\n  id: '删除云凭证',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p><em parentName=\"p\">{`v2.2.0 或更新版可用`}</em></p>\n    <h2 {...{\n      \"id\": \"概述\"\n    }}>{`概述`}</h2>\n    <p>{`当您创建集群时，需要用到`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-provisioning/rke-clusters/node-pools/_index\"\n      }}>{`节点模板`}</a>{`创建集群内的节点。这些模板使用 Docker Machine 中可用的配置选项，定义节点上的操作系统的镜像和配置参数。`}</p>\n    <p>{`主机节点可以使用云凭证访问基础设施提供商，并创建节点。一个云凭证可用于多个节点模板。云凭证的优势是，使用同一个云服务提供商创建多个节点模板时，您不需要重复输入多次认证信息。云凭证以 Kubernetes 密文的方式存储。`}</p>\n    <p>{`云凭证只能在有`}<inlineCode parentName=\"p\">{`password`}</inlineCode>{`类型字段的节点模板里使用。在默认处于`}<inlineCode parentName=\"p\">{`active`}</inlineCode>{`状态的节点驱动里，它们的账号相关字段都是`}<inlineCode parentName=\"p\">{`password`}</inlineCode>{`类型。但是在一些默认处于`}<inlineCode parentName=\"p\">{`inactive`}</inlineCode>{`状态的节点驱动里，我们还没有将它们的账号相关字段转换为`}<inlineCode parentName=\"p\">{`password`}</inlineCode>{`类型。所以它们暂时还不支持使用云凭证。`}</p>\n    <p>{`您可以在以下两种场景中创建云凭证：`}</p>\n    <ul>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`为集群创建节点模板时，创建云凭证，详情请参考`}<a parentName=\"p\" {...{\n            \"href\": \"/docs/rancher2/cluster-provisioning/rke-clusters/node-pools/_index\"\n          }}>{`创建节点模板`}</a>{`。`}</p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`在`}<strong parentName=\"p\">{`用户设置`}</strong>{`中创建云凭证。`}</p>\n      </li>\n    </ul>\n    <blockquote>\n      <p parentName=\"blockquote\"><strong parentName=\"p\">{`说明：`}</strong>{` 云凭证与创建该凭证的用户绑定，用户之间不可用分享凭证。`}</p>\n    </blockquote>\n    <h2 {...{\n      \"id\": \"创建云凭证\"\n    }}>{`创建云凭证`}</h2>\n    <ol>\n      <li parentName=\"ol\">{`单击页面右上方的 `}<strong parentName=\"li\">{`用户头像`}</strong>{`，选择 `}<strong parentName=\"li\">{`云凭证`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`单击`}<strong parentName=\"li\">{`添加云凭证`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`输入凭证名称。`}</li>\n      <li parentName=\"ol\">{`从下拉菜单选择一个云凭证类型。可选的类型由 Rancher 内状态为`}<inlineCode parentName=\"li\">{`active`}</inlineCode>{`的`}<a parentName=\"li\" {...{\n          \"href\": \"/docs/rancher2/admin-settings/drivers/node-drivers/_index\"\n        }}>{`节点驱动`}</a>{`决定。`}</li>\n      <li parentName=\"ol\">{`输入认证信息。`}</li>\n      <li parentName=\"ol\">{`单击`}<strong parentName=\"li\">{`创建`}</strong>{`，添加新的云凭证。`}</li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{` 添加了新的云凭证，`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-provisioning/rke-clusters/node-pools/_index\"\n      }}>{`创建节点模板`}</a>{`时可以使用该凭证。`}</p>\n    <h2 {...{\n      \"id\": \"更新云凭证\"\n    }}>{`更新云凭证`}</h2>\n    <p>{`当访问凭证信息泄露或被修改过后，更新云凭证允许您在替换这些认证信息的同时，保留相同的节点模板。`}</p>\n    <ol>\n      <li parentName=\"ol\">{`单击页面右上方的 `}<strong parentName=\"li\">{`用户头像`}</strong>{`，选择 `}<strong parentName=\"li\">{`云凭证`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`选择您需要更新的云凭证，单击 `}<strong parentName=\"li\">{`... > 编辑`}</strong>{`。`}</li>\n      <li parentName=\"ol\">{`输入最新的云凭证信息，单击 `}<strong parentName=\"li\">{`保存`}</strong>{`，完成更新。`}</li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{` 完成云凭证更新。新建节点时，所有的主机节点都会自动使用更新后的云凭证。`}</p>\n    <h2 {...{\n      \"id\": \"删除云凭证\"\n    }}>{`删除云凭证`}</h2>\n    <p>{`删除云凭证前，请确保所有节点模板都已经不再使用该凭证。如果出现删除凭证失败的场景，则表示还有节点模板在使用该凭证。请先删除正在使用该凭证的所有节点模板，详情请参考`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/user-settings/node-templates/_index\"\n      }}>{`删除节点模板`}</a>{`。`}</p>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`单击页面右上方的 `}<strong parentName=\"p\">{`用户头像`}</strong>{`，选择 `}<strong parentName=\"p\">{`云凭证`}</strong>{`。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`删除单个云凭证或批量删除云凭证`}</p>\n        <ul parentName=\"li\">\n          <li parentName=\"ul\">{`选择您需要更新的云凭证，单击 `}<strong parentName=\"li\">{`... > 删除`}</strong>{`。`}</li>\n          <li parentName=\"ul\">{`勾选多个云凭证，单击`}<strong parentName=\"li\">{`删除`}</strong>{`。`}</li>\n        </ul>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`单击 `}<strong parentName=\"p\">{`确认`}</strong>{`，删除选定的云凭证。`}</p>\n      </li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{` 已删除选定的云凭证。`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}