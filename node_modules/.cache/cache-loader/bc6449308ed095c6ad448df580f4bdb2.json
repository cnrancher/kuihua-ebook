kBAFa,CAApB,CAJF,CAOE,UAAI,UAAU,CAAC,IAAf,EAAoB,kBAAG,UAAU,CAAC,IAAd,EAAuB,CACvC,OAAQ,6DAD+B,CAAvB,EAEb,YAFa,CAApB,CAPF,CA1EK,CAqFL,SAAQ,CACN,KAAM,QADA,CAAR,CAEK,QAFL,CArFK,CAwFL,cACE,UAAI,UAAU,CAAC,IAAf,EAAoB,kBAAG,UAAU,CAAC,IAAd,EAAuB,CACvC,OAAQ,mEAD+B,CAAvB,EAEb,yCAFa,CAApB,CADF,CAIE,UAAI,UAAU,CAAC,IAAf,EAAoB,kBAAG,UAAU,CAAC,IAAd,EAAuB,CACvC,OAAQ,kDAD+B,CAAvB,EAEb,0BAFa,CAApB,CAEuC,4DAFvC,CAJF,CAxFK,CAgGL,SAAQ,CACN,KAAM,WADA,CAAR,CAEK,WAFL,CAhGK,CAmGL,aAAG,kBAAG,UAAU,CAAC,GAAd,EAAsB,CACrB,OAAQ,uDADa,CAAtB,EAEI,eAFJ,CAAH,CAnGK,CAsGL,sBACE,SAAG,UAAU,CAAC,YAAd,EAA2B,cAAQ,UAAU,CAAC,GAAnB,EAAyB,8CAAzB,CAA3B,CADF,CAEE,SAAG,UAAU,CAAC,YAAd,EAA6B,+DAA7B,CAA4F,kBAAG,UAAU,CAAC,GAAd,EAAsB,CAC9G,OAAQ,gDADsG,CAAtB,EAErF,eAFqF,CAA5F,CAFF,CAKE,SAAG,UAAU,CAAC,YAAd,EAA6B,+BAA7B,CAA4D,kBAAG,UAAU,CAAC,GAAd,EAAsB,CAC9E,OAAQ,mEADsE,CAAtB,EAErD,yCAFqD,CAA5D,CAEsD,6BAFtD,CALF,CAtGK,CAAP,CAgHD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '安装介绍',\n\tdescription: '对于生产环境，我们建议以高可用配置安装 Rancher，以便您的用户始终可以访问 Rancher Server。当 Rancher 安装在 Kubernetes 集群中时，Rancher 将与集群的 etcd 或 MySQL 等数据库集成，并利用 Kubernetes 调度来实现高可用。本节介绍如何使用 RKE 或 K3s 创建和管理集群，然后将 Rancher 安装到该集群上。对于这种类型的架构，您将需要在基础设施提供商中创建节点（通常为虚拟机）。您还需要配置负载均衡器，将前端流量定向到这些节点中。当节点运行起来并满足节点要求时，可以使用 RKE 或 K3s 将 Kubernetes 部署到这些节点上，然后使用 Helm 软件包管理器将 Rancher 部署到 Kubernetes 上。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'安装指南',\n\t\t'Rancher高可用安装',\n\t\t'安装介绍'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/installation/k8s-install/_index\",\n  \"title\": \"安装介绍\",\n  \"description\": \"对于生产环境，我们建议以高可用配置安装 Rancher，以便您的用户始终可以访问 Rancher Server。当 Rancher 安装在 Kubernetes 集群中时，Rancher 将与集群的 etcd 或 MySQL 等数据库集成，并利用 Kubernetes 调度来实现高可用。本节介绍如何使用 RKE 或 K3s 创建和管理集群，然后将 Rancher 安装到该集群上。对于这种类型的架构，您将需要在基础设施提供商中创建节点（通常为虚拟机）。您还需要配置负载均衡器，将前端流量定向到这些节点中。当节点运行起来并满足节点要求时，可以使用 RKE 或 K3s 将 Kubernetes 部署到这些节点上，然后使用 Helm 软件包管理器将 Rancher 部署到 Kubernetes 上。\",\n  \"source\": \"@site/docs/rancher2/installation/k8s-install/_index.md\",\n  \"permalink\": \"/docs/rancher2/installation/k8s-install/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/installation/k8s-install/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '可选：在单节点 Kubernetes 集群上安装 Rancher',\n  id: '可选：在单节点-kubernetes-集群上安装-rancher',\n  children: [{\n    value: '通过 RKE 安装',\n    id: '通过-rke-安装',\n    children: []\n  }, {\n    value: '通过 K3s 安装',\n    id: '通过-k3s-安装',\n    children: []\n  }]\n}, {\n  value: '关于架构的重要说明',\n  id: '关于架构的重要说明',\n  children: []\n}, {\n  value: '需要的 CLI 工具',\n  id: '需要的-cli-工具',\n  children: []\n}, {\n  value: '安装摘要',\n  id: '安装摘要',\n  children: []\n}, {\n  value: '其他安装选项',\n  id: '其他安装选项',\n  children: []\n}, {\n  value: '已经弃用的安装方法',\n  id: '已经弃用的安装方法',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p>{`对于生产环境，我们建议以高可用配置安装 Rancher，以便您的用户始终可以访问 Rancher Server。当 Rancher 安装在 Kubernetes 集群中时，Rancher 将与集群的 etcd 数据库集成，并利用 Kubernetes 调度来实现高可用。`}</p>\n    <p>{`本节介绍如何使用 RKE 或 K3s 创建和管理集群，然后将 Rancher 安装到该集群上。对于这种类型的架构，您将需要在基础设施提供商中创建节点（通常为虚拟机）。您还需要配置负载均衡器，将前端流量定向到这些节点中。当节点运行起来并满足`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/installation/requirements/_index\"\n      }}>{`节点要求`}</a>{`时，可以使用 RKE 或 K3s 将 Kubernetes 部署到这些节点上，然后使用 Helm 软件包管理器将 Rancher 部署到 Kubernetes 上。`}</p>\n    <h2 {...{\n      \"id\": \"可选：在单节点-kubernetes-集群上安装-rancher\"\n    }}>{`可选：在单节点 Kubernetes 集群上安装 Rancher`}</h2>\n    <p>{`如果您只有一个节点，但您想在将来的生产中使用 Rancher Server，则最好将 Rancher 安装在单节点 Kubernetes 集群上，而不是使用 Docker 安装它。`}</p>\n    <p>{`要创建单节点 RKE 集群，只需在`}<inlineCode parentName=\"p\">{`cluster.yml`}</inlineCode>{`中配置一个节点。这个节点应该具有所有三个角色：`}<inlineCode parentName=\"p\">{`etcd`}</inlineCode>{`,`}<inlineCode parentName=\"p\">{`controlplane`}</inlineCode>{`和`}<inlineCode parentName=\"p\">{`worker`}</inlineCode>{`。`}</p>\n    <p>{`要创建单节点 K3s 集群，只需在一个节点上运行安装命令即可，并不需要像高可用集群一样在两个节点上安装。`}</p>\n    <p>{`配置完单节点的 Kubernetes 集群后，就可以像在其他高可用集群上安装一样，使用 Helm 安装 Rancher。`}</p>\n    <h3 {...{\n      \"id\": \"通过-rke-安装\"\n    }}>{`通过 RKE 安装`}</h3>\n    <p>{`通过为 RKE 配置 Kubernetes 集群时，可以通过在`}<inlineCode parentName=\"p\">{`cluster.yml`}</inlineCode>{`中只描述一个节点就可以实现单节点 Kubernetes 的安装。这个节点将具有所有三个角色：`}<inlineCode parentName=\"p\">{`etcd`}</inlineCode>{`，`}<inlineCode parentName=\"p\">{`controlplane`}</inlineCode>{`和`}<inlineCode parentName=\"p\">{`worker`}</inlineCode>{`。`}</p>\n    <h3 {...{\n      \"id\": \"通过-k3s-安装\"\n    }}>{`通过 K3s 安装`}</h3>\n    <p>{`通过 K3s 安装 Kubernetes 集群时，可以将这个节点同时作为 Server 和 Agent 节点`}</p>\n    <p>{`然后，就像在其他任何集群上安装一样，使用 Helm 将 Rancher 安装在集群上。`}</p>\n    <h2 {...{\n      \"id\": \"关于架构的重要说明\"\n    }}>{`关于架构的重要说明`}</h2>\n    <p>{`Rancher Server 只能在使用 RKE 或 K3s 安装的 Kubernetes 集群中运行。不支持在托管的 Kubernetes 集群（例如 EKS）上使用 Rancher。`}</p>\n    <p>{`为了获得最佳性能和安全性，我们建议为 Rancher Server 使用专用的 Kubernetes 集群。不建议在此集群上运行用户的其他工作负载。部署完 Rancher 之后，您可以`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-provisioning/_index\"\n      }}>{`创建或导入`}</a>{`用于运行用户工作负载的集群。`}</p>\n    <p>{`有关 Rancher 是如何工作的（与安装方法无关），请参阅`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/overview/architecture/_index\"\n      }}>{`产品架构`}</a>{`。`}</p>\n    <h2 {...{\n      \"id\": \"需要的-cli-工具\"\n    }}>{`需要的 CLI 工具`}</h2>\n    <p>{`此安装需要以下 CLI 工具。请确保这些工具已经安装并在`}<inlineCode parentName=\"p\">{`$PATH`}</inlineCode>{`中可用`}</p>\n    <ul>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"https://kubernetes.io/docs/tasks/tools/install-kubectl/#install-kubectl\"\n        }}>{`kubectl`}</a>{` - Kubernetes 命令行工具.`}</li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"/docs/rke/installation/_index\"\n        }}>{`rke`}</a>{` - Rancher Kubernetes Engine，用于构建 Kubernetes 集群的 cli。`}</li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"/docs/k3s/_index\"\n        }}>{`k3s`}</a>{` - Rancher K3s。`}</li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"https://docs.helm.sh/using_helm/#installing-helm\"\n        }}>{`helm`}</a>{` - Kubernetes 的软件包管理工具。请参阅`}<a parentName=\"li\" {...{\n          \"href\": \"/docs/rancher2/installation/options/helm-version/_index\"\n        }}>{`Helm 版本要求`}</a>{`选择 Helm 的版本来安装 Rancher。`}</li>\n    </ul>\n    <div {...{\n      \"className\": \"admonition admonition-note alert alert--secondary\"\n    }}><div parentName=\"div\" {...{\n        \"className\": \"admonition-heading\"\n      }}><h5 parentName=\"div\"><span parentName=\"h5\" {...{\n            \"className\": \"admonition-icon\"\n          }}><svg parentName=\"span\" {...{\n              \"xmlns\": \"http://www.w3.org/2000/svg\",\n              \"width\": \"14\",\n              \"height\": \"16\",\n              \"viewBox\": \"0 0 14 16\"\n            }}><path parentName=\"svg\" {...{\n                \"fillRule\": \"evenodd\",\n                \"d\": \"M6.3 5.69a.942.942 0 0 1-.28-.7c0-.28.09-.52.28-.7.19-.18.42-.28.7-.28.28 0 .52.09.7.28.18.19.28.42.28.7 0 .28-.09.52-.28.7a1 1 0 0 1-.7.3c-.28 0-.52-.11-.7-.3zM8 7.99c-.02-.25-.11-.48-.31-.69-.2-.19-.42-.3-.69-.31H6c-.27.02-.48.13-.69.31-.2.2-.3.44-.31.69h1v3c.02.27.11.5.31.69.2.2.42.31.69.31h1c.27 0 .48-.11.69-.31.2-.19.3-.42.31-.69H8V7.98v.01zM7 2.3c-3.14 0-5.7 2.54-5.7 5.68 0 3.14 2.56 5.7 5.7 5.7s5.7-2.55 5.7-5.7c0-3.15-2.56-5.69-5.7-5.69v.01zM7 .98c3.86 0 7 3.14 7 7s-3.14 7-7 7-7-3.12-7-7 3.14-7 7-7z\"\n              }}></path></svg></span>{`提示`}</h5></div><div parentName=\"div\" {...{\n        \"className\": \"admonition-content\"\n      }}><p parentName=\"div\">{`国内用户，可以导航到 `}<a parentName=\"p\" {...{\n            \"href\": \"http://mirror.cnrancher.com\"\n          }}>{`http://mirror.cnrancher.com`}</a>{` 下载所需资源`}</p></div></div>\n    <h2 {...{\n      \"id\": \"安装摘要\"\n    }}>{`安装摘要`}</h2>\n    <ul>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"/docs/rancher2/installation/k8s-install/create-nodes-lb/_index\"\n        }}>{`配置基础设施`}</a></li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"/docs/rancher2/installation/k8s-install/kubernetes-rke/_index\"\n        }}>{`安装 Kubernetes 集群`}</a></li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"/docs/rancher2/installation/k8s-install/helm-rancher/_index\"\n        }}>{`安装 Rancher`}</a></li>\n    </ul>\n    <h2 {...{\n      \"id\": \"其他安装选项\"\n    }}>{`其他安装选项`}</h2>\n    <ul>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"/docs/rancher2/upgrades/upgrades/migrating-from-rke-add-on/_index\"\n        }}>{`从 RKE Add-on 安装的 Rancher 高可用迁移到 Helm 安装`}</a></li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"/docs/rancher2/installation/options/helm2/_index\"\n        }}>{`使用 Helm 2 安装 Rancher 高可用`}</a>{`：本节提供了使用 Helm 2 安装高可用 Rancher 的说明，如果无法升级到 Helm 3，则可以使用该说明。`}</li>\n    </ul>\n    <h2 {...{\n      \"id\": \"已经弃用的安装方法\"\n    }}>{`已经弃用的安装方法`}</h2>\n    <p><a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/installation/options/rke-add-on/_index\"\n      }}>{`RKE add-on 安装`}</a></p>\n    <blockquote>\n      <p parentName=\"blockquote\"><strong parentName=\"p\">{`重要说明：RKE ADD-ON 安装仅在 RANCHER v2.0.8 之前到版本中支持`}</strong></p>\n      <p parentName=\"blockquote\">{`请使用 Rancher Helm Chart 在 Kubernetes 集群上安装 Rancher 高可用。更多信息请参阅`}<a parentName=\"p\" {...{\n          \"href\": \"/docs/rancher2/installation/k8s-install/_index\"\n        }}>{`Rancher 高可用安装`}</a></p>\n      <p parentName=\"blockquote\">{`如果您当前正在使用 RKE add-on 安装方式，请参阅`}<a parentName=\"p\" {...{\n          \"href\": \"/docs/rancher2/upgrades/upgrades/migrating-from-rke-add-on/_index\"\n        }}>{`从 RKE Add-on 安装的 Rancher 高可用迁移到 Helm 安装`}</a>{`以获取有关如何使用 Helm Chart 的详细信息。`}</p>\n    </blockquote>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}