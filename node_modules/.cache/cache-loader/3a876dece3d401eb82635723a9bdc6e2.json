ords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'集群管理员指南',\n\t\t'集群访问控制',\n\t\t'将用户添加到集群中'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/cluster-admin/cluster-access/cluster-members/_index\",\n  \"title\": \"将用户添加到集群中\",\n  \"description\": \"如果希望为用户提供对集群中的所有项目、节点和资源的访问和权限，请为该用户分配集群成员资格。以下两种情况下，您可以添加集群成员。向新集群添加成员，您可以在创建集群时将成员添加到集群中。向现有集群添加成员，在集群创建之后，随时可以向集群添加成员。\",\n  \"source\": \"@site/docs/rancher2/cluster-admin/cluster-access/cluster-members/_index.md\",\n  \"permalink\": \"/docs/rancher2/cluster-admin/cluster-access/cluster-members/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/cluster-admin/cluster-access/cluster-members/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '添加集群成员',\n  id: '添加集群成员',\n  children: []\n}, {\n  value: '编辑集群成员',\n  id: '编辑集群成员',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <h2 {...{\n      \"id\": \"添加集群成员\"\n    }}>{`添加集群成员`}</h2>\n    <p>{`如果希望为用户提供对集群中的 `}<em parentName=\"p\">{`所有`}</em>{` 项目、节点和资源的访问和权限，请为该用户分配集群成员资格。`}</p>\n    <blockquote>\n      <p parentName=\"blockquote\"><strong parentName=\"p\">{`提示:`}</strong>{` 希望为用户提供对集群中 `}<em parentName=\"p\">{`特定`}</em>{` 项目的访问权限？ 请参见 `}<a parentName=\"p\" {...{\n          \"href\": \"/docs/rancher2/project-admin/project-members/_index\"\n        }}>{`添加项目成员`}</a>{`。`}</p>\n    </blockquote>\n    <p>{`以下两种情况下，您可以添加集群成员：`}</p>\n    <ul>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`向新集群添加成员`}</p>\n        <p parentName=\"li\">{`您可以在创建集群时将成员添加到集群中。`}</p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\"><a parentName=\"p\" {...{\n            \"href\": \"#%E7%BC%96%E8%BE%91%E9%9B%86%E7%BE%A4%E6%88%90%E5%91%98\"\n          }}>{`向现有集群添加成员`}</a></p>\n        <p parentName=\"li\">{`在集群创建之后，随时可以向集群添加成员。`}</p>\n      </li>\n    </ul>\n    <h2 {...{\n      \"id\": \"编辑集群成员\"\n    }}>{`编辑集群成员`}</h2>\n    <p>{`集群管理员可以编辑集群的成员关系，控制哪些 Rancher 用户可以访问集群以及他们可以使用哪些功能。`}</p>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`在 `}<strong parentName=\"p\">{`全局`}</strong>{` 视图中，打开要向其添加成员的集群。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`从主菜单中选择 `}<strong parentName=\"p\">{`成员`}</strong>{`，然后单击 `}<strong parentName=\"p\">{`添加成员`}</strong>{`.`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`搜索要添加到集群中的用户或组。`}</p>\n        <p parentName=\"li\">{`如果配置了外部认证：`}</p>\n        <ul parentName=\"li\">\n          <li parentName=\"ul\">\n            <p parentName=\"li\">{`Rancher 在您登录时可以从 `}<a parentName=\"p\" {...{\n                \"href\": \"/docs/rancher2/admin-settings/authentication/_index\"\n              }}>{`外部身份验证`}</a>{` 源获取相应的用户信息。`}</p>\n            <blockquote parentName=\"li\">\n              <p parentName=\"blockquote\"><strong parentName=\"p\">{`使用 AD 却找不到您的用户？`}</strong>{`\n您的搜索属性配置可能存在问题.请参见 `}<a parentName=\"p\" {...{\n                  \"href\": \"/docs/rancher2/admin-settings/authentication/ad/_index\"\n                }}>{`配置 Active Directory 身份验证:步骤 5`}</a>{`.`}</p>\n            </blockquote>\n          </li>\n          <li parentName=\"ul\">\n            <p parentName=\"li\">{`下拉菜单允许您添加用户组，而不是单个用户。下拉列表只列出登录用户所属的组。`}</p>\n            <blockquote parentName=\"li\">\n              <p parentName=\"blockquote\"><strong parentName=\"p\">{`注意:`}</strong>{` 如果您是以本地用户登录的，那么外部用户不会显示在您的搜索结果中。更多信息，请参见 `}<a parentName=\"p\" {...{\n                  \"href\": \"/docs/rancher2/admin-settings/authentication/_index\"\n                }}>{`外部身份验证配置和主要用户`}</a>{`.`}</p>\n            </blockquote>\n          </li>\n        </ul>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`给用户或组分配 `}<strong parentName=\"p\">{`集群`}</strong>{` 角色。`}</p>\n        <p parentName=\"li\"><a parentName=\"p\" {...{\n            \"href\": \"/docs/rancher2/admin-settings/rbac/cluster-project-roles/_index\"\n          }}>{`什么是集群角色？`}</a></p>\n        <blockquote parentName=\"li\">\n          <p parentName=\"blockquote\"><strong parentName=\"p\">{`提示：`}</strong>{` 对于自定义角色，您可以修改可用于分配的各个角色的列表。`}</p>\n          <ul parentName=\"blockquote\">\n            <li parentName=\"ul\">{`将角色添加到列表中， `}<a parentName=\"li\" {...{\n                \"href\": \"/docs/rancher2/admin-settings/rbac/default-custom-roles/_index\"\n              }}>{`添加自定义角色`}</a>{`.`}</li>\n            <li parentName=\"ul\">{`要从列表中删除角色， `}<a parentName=\"li\" {...{\n                \"href\": \"/docs/rancher2/admin-settings/rbac/locked-roles/_index\"\n              }}>{`锁定/解锁角色`}</a>{`.`}</li>\n          </ul>\n        </blockquote>\n      </li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{` 选择的用户被添加到集群中。`}</p>\n    <ul>\n      <li parentName=\"ul\">{`要撤消集群成员资格，请选择用户并单击 `}<strong parentName=\"li\">{`删除`}</strong>{`，此操作删除的是成员关系，而不是用户。`}</li>\n      <li parentName=\"ul\">{`要修改用户在集群中的角色，请从集群中删除它们，然后给它们配置新的角色，重新添加到集群中。`}</li>\n    </ul>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}