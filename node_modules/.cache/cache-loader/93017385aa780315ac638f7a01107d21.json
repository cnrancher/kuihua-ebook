AAvB,EAA6B,cAA7B,CAA/E,CAAyI,YAAzI,CAAqJ,kBAAY,UAAU,CAAC,GAAvB,EAA6B,cAA7B,CAArJ,CAA+M,mBAA/M,CAtCK,CAuCL,SAAQ,CACN,KAAM,MADA,CAAR,CAEK,MAFL,CAvCK,CA0CL,aAAK,4BAAL,CA1CK,CA2CL,eAAK,qBAAM,UAAU,CAAC,KAAjB,EAA2B,EAA3B,EAAiC;;;;CAAjC,CAAL,CA3CK,CAAP,CAiDD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: 'SELinux 模式'\n};\nexport const metadata = {\n  \"id\": \"rancher1/installation/selinux/_index\",\n  \"title\": \"SELinux 模式\",\n  \"description\": \"# SELinux 模式\",\n  \"source\": \"@site/docs/rancher1/installation/selinux/_index.md\",\n  \"permalink\": \"/docs/rancher1/installation/selinux/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher1/installation/selinux/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar\": \"troubleshooting\",\n  \"previous\": {\n    \"title\": \"离线安装\",\n    \"permalink\": \"/docs/rancher1/installation/no-internet-access/_index\"\n  },\n  \"next\": {\n    \"title\": \"应用商店\",\n    \"permalink\": \"/docs/rancher1/configurations/catalog/_index\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '构建额外的 SELinux 模块',\n  id: '构建额外的-selinux-模块',\n  children: [{\n    value: '安装包并构建 SELinux 模块',\n    id: '安装包并构建-selinux-模块',\n    children: []\n  }, {\n    value: '构建模块',\n    id: '构建模块',\n    children: []\n  }, {\n    value: '加载模块',\n    id: '加载模块',\n    children: []\n  }]\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <h1 {...{\n      \"id\": \"selinux-模式\"\n    }}>{`SELinux 模式`}</h1>\n    <p><em parentName=\"p\">{`从 1.6+版本后支持`}</em>{` 在 SELinux 模式下安装 Rancher-RHEL/CentOS`}</p>\n    <p>{`为了使 Rancher 在 RHEL/CentOS 的 SELinux 模式下正常工作，您需要在安装有 RPM 包`}<inlineCode parentName=\"p\">{`container-selinux-2.14`}</inlineCode>{`(或更高的版本)的主机上运行 Rancher Server 容器。同时，全部的 agent`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher1/infrastructure/hosts/_index\"\n      }}>{`主机`}</a>{`也要安装这个包。如果您安装了较低版本的`}<inlineCode parentName=\"p\">{`container-selinux`}</inlineCode>{`包，您需要构建额外的 SELinux 模块，从而使 Rancher 正常工作。`}</p>\n    <p>{`您可以通过这个命令来查看这个包的版本:`}<inlineCode parentName=\"p\">{`rpm -q container-selinux`}</inlineCode>{`。`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`# Check container-selinux version\n$ rpm -q container-selinux\ncontainer-selinux-2.19-2.1.el7.noarch\n`}</code></pre>\n    <h2 {...{\n      \"id\": \"构建额外的-selinux-模块\"\n    }}>{`构建额外的 SELinux 模块`}</h2>\n    <h3 {...{\n      \"id\": \"安装包并构建-selinux-模块\"\n    }}>{`安装包并构建 SELinux 模块`}</h3>\n    <p>{`为了安装另外的 SELinux 模块，您需要安装 `}<inlineCode parentName=\"p\">{`selinux-policy-devel`}</inlineCode>{` 这个包。`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`$ yum install selinux-policy-devel\n`}</code></pre>\n    <h3 {...{\n      \"id\": \"构建模块\"\n    }}>{`构建模块`}</h3>\n    <p>{`创建一个名叫 `}<inlineCode parentName=\"p\">{`virtpatch.te`}</inlineCode>{` 的文件并写入以下内容`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`policy_module(virtpatch, 1.0)\n\ngen_require(\\`\n  type svirt_lxc_net_t;\n')\n\nallow svirt_lxc_net_t self:netlink_xfrm_socket create_netlink_socket_perms;\n`}</code></pre>\n    <p>{`构建模块`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`$ make -f /usr/share/selinux/devel/Makefile\n`}</code></pre>\n    <p>{`运行 `}<inlineCode parentName=\"p\">{`make`}</inlineCode>{` 命令后，当构建成功后，一个名叫 `}<inlineCode parentName=\"p\">{`virtpatch.pp`}</inlineCode>{` 的文件将会被创建。`}<inlineCode parentName=\"p\">{`virtpatch.pp`}</inlineCode>{` 是编译后的 SELinux 模块`}</p>\n    <h3 {...{\n      \"id\": \"加载模块\"\n    }}>{`加载模块`}</h3>\n    <p>{`在 SELinux 模块被构建后，使用以下命令加载。`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`# Load the module\n$ semodule -i virtpatch.pp\n# Verify the module is loaded\n$ semodule -l\n`}</code></pre>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}