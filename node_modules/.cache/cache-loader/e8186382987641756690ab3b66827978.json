ACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '配置用于监控的自定义资源',\n\tdescription: '本文讲述了一些配置监控的自定义资源时使用到的重要配置选项，如配置 Prometheus、为通知添加 CA 证书和配置拉取参数，并且提供了示例代码。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'rancher 2.5',\n\t\t'监控和告警',\n\t\t'配置用于监控的自定义资源'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/monitoring-alerting/2.5/configuration/_index\",\n  \"title\": \"配置用于监控的自定义资源\",\n  \"description\": \"本文讲述了一些配置监控的自定义资源时使用到的重要配置选项，如配置 Prometheus、为通知添加 CA 证书和配置拉取参数，并且提供了示例代码。\",\n  \"source\": \"@site/docs/rancher2/monitoring-alerting/2.5/configuration/_index.md\",\n  \"permalink\": \"/docs/rancher2/monitoring-alerting/2.5/configuration/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/monitoring-alerting/2.5/configuration/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '概述',\n  id: '概述',\n  children: []\n}, {\n  value: '配置 Prometheus',\n  id: '配置-prometheus',\n  children: []\n}, {\n  value: '使用 ServiceMonitors 和 PodMonitors 配置目标',\n  id: '使用-servicemonitors-和-podmonitors-配置目标',\n  children: [{\n    value: '配置 ServiceMonitors',\n    id: '配置-servicemonitors',\n    children: []\n  }, {\n    value: '配置 PodMonitors',\n    id: '配置-podmonitors',\n    children: []\n  }, {\n    value: '配置 PrometheusRules',\n    id: '配置-prometheusrules',\n    children: []\n  }, {\n    value: '配置 Alertmanager Config',\n    id: '配置-alertmanager-config',\n    children: []\n  }]\n}, {\n  value: '为通知添加 CA 证书',\n  id: '为通知添加-ca-证书',\n  children: []\n}, {\n  value: '配置拉取参数',\n  id: '配置拉取参数',\n  children: []\n}, {\n  value: '示例',\n  id: '示例',\n  children: [{\n    value: 'ServiceMonitor',\n    id: 'servicemonitor',\n    children: []\n  }, {\n    value: 'PodMonitor',\n    id: 'podmonitor',\n    children: []\n  }, {\n    value: 'PrometheusRule',\n    id: 'prometheusrule',\n    children: []\n  }, {\n    value: 'Alertmanager Config',\n    id: 'alertmanager-config',\n    children: []\n  }]\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <h2 {...{\n      \"id\": \"概述\"\n    }}>{`概述`}</h2>\n    <p>{`本文讲述了一些配置监控的自定义资源时使用到的重要配置选项。`}</p>\n    <p>{`有关为 Prometheus 配置自定义拉取目标和规则的信息，请参考 `}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/prometheus-operator/prometheus-operator\"\n      }}>{`Prometheus Operator 文档`}</a>{`。一些最重要的自定义资源在 Prometheus Operator `}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/prometheus-operator/prometheus-operator/blob/master/Documentation/design.md\"\n      }}>{`设计文档`}</a>{`中进行了说明。Prometheus Operator 文档也可以帮助您设置 RBAC、Thanos 或自定义配置。`}</p>\n    <h2 {...{\n      \"id\": \"配置-prometheus\"\n    }}>{`配置 Prometheus`}</h2>\n    <p>{`用户通过创建或修改与此部署相关的 ConfigMaps、Secrets 和 Custom Resources，可以配置特定的监控和告警用例。Prometheus Operator 引入了一组自定义资源定义，允许用户通过在集群上创建和修改这些自定义资源来部署和管理 Prometheus 和 Alertmanager 实例。Prometheus Operator 会根据这些自定义资源的实时状态自动更新您的 Prometheus 配置。`}</p>\n    <p>{`还有某些特殊类型的 ConfigMaps/Secrets，如对应于 Grafana Dashboards、Grafana Datasources 和 Alertmanager Configs 的 ConfigMaps/Secrets，它们将通过 sidecar 代理监控您集群内这些资源的实时状态，自动更新您的 Prometheus 配置。`}</p>\n    <p>{`默认情况下，这些资源（由 kube-prometheus 项目策划）的一组会被部署到您的集群上，作为安装 Rancher 监控应用程序的一部分，以建立基本的监控和告警。有关如何在部署 chart 后配置自定义目标、警报、通知器和仪表盘的更多信息，请参阅下文。`}</p>\n    <h2 {...{\n      \"id\": \"使用-servicemonitors-和-podmonitors-配置目标\"\n    }}>{`使用 ServiceMonitors 和 PodMonitors 配置目标`}</h2>\n    <p>{`自定义由 Prometheus 使用的 scrape 配置，以确定要从哪些资源中拉取指标，主要涉及在您的集群中创建或修改以下资源。`}</p>\n    <h3 {...{\n      \"id\": \"配置-servicemonitors\"\n    }}>{`配置 ServiceMonitors`}</h3>\n    <p>{`该 CRD 声明性地指定了应如何监控 Kubernetes 服务组。集群中任何符合 ServiceMonitor `}<inlineCode parentName=\"p\">{`selector`}</inlineCode>{`字段中标签的服务都将根据 ServiceMonitor 上指定的`}<inlineCode parentName=\"p\">{`endpoints`}</inlineCode>{`进行监控。有关可以指定哪些字段的详细信息，请查看 Prometheus Operator 提供的 `}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/prometheus-operator/prometheus-operator/blob/master/Documentation/api.md#servicemonitor\"\n      }}>{`规范`}</a>{`。`}</p>\n    <p>{`有关 ServiceMonitors 工作方式的更多信息，请参阅 `}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/prometheus-operator/prometheus-operator/blob/master/Documentation/user-guides/running-exporters.md\"\n      }}>{`Prometheus Operator 文档`}</a>{`。`}</p>\n    <h3 {...{\n      \"id\": \"配置-podmonitors\"\n    }}>{`配置 PodMonitors`}</h3>\n    <p>{`该 CRD 声明性地指定了应如何监测 pods 组。您集群中的任何符合 PodMonitor`}<inlineCode parentName=\"p\">{`selector`}</inlineCode>{`字段中标签的 Pod 将根据 PodMonitor 上指定的`}<inlineCode parentName=\"p\">{`podMetricsEndpoints`}</inlineCode>{`进行监控。关于可以指定哪些字段的更多信息，请查看 Prometheus Operator 提供的`}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/prometheus-operator/prometheus-operator/blob/master/Documentation/api.md#podmonitorspec\"\n      }}>{`规范`}</a>{`。`}</p>\n    <h3 {...{\n      \"id\": \"配置-prometheusrules\"\n    }}>{`配置 PrometheusRules`}</h3>\n    <p>{`此 CRD 定义了一组 Prometheus 警报和记录规则。`}</p>\n    <p>{`要添加一组警报/记录规则，您应该创建一个 PrometheusRule CR，该 CR 定义了一个带有您所需规则的 RuleGroup，每个规则都指定了。`}</p>\n    <ul>\n      <li parentName=\"ul\">{`新警报/记录的名称`}</li>\n      <li parentName=\"ul\">{`新警报/记录的 PromQL 表达式。`}</li>\n      <li parentName=\"ul\">{`应附加在警报/记录上的标签，以识别它（如群组名称或严重性）。`}</li>\n      <li parentName=\"ul\">{`注释，对需要在警报通知上显示的任何其他重要信息进行编码（如摘要、描述、信息、运行簿 URL 等）。记录规则不需要此字段。`}</li>\n    </ul>\n    <p>{`有关可指定哪些字段的详细信息，请查看 `}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/prometheus-operator/prometheus-operator/blob/master/Documentation/api.md#prometheusrulespec\"\n      }}>{`Prometheus Operator spec.`}</a></p>\n    <h3 {...{\n      \"id\": \"配置-alertmanager-config\"\n    }}>{`配置 Alertmanager Config`}</h3>\n    <p><a parentName=\"p\" {...{\n        \"href\": \"https://prometheus.io/docs/alerting/latest/configuration/#configuration-file\"\n      }}>{`Alertmanager Config`}</a>{` Secret 包含 Alertmanager 实例的配置，该实例根据从 Prometheus 收到的警报发送通知。`}</p>\n    <p>{`默认情况下，Rancher Monitoring 将单个 Alertmanager 部署到使用默认 Alertmanager Config Secret 的集群上。作为 chart 部署选项的一部分，您可以选择增加部署到您的集群上的 Alertmanager 的副本数量，这些副本都可以使用相同的基础 Alertmanager 配置秘籍进行管理。`}</p>\n    <p>{`这个密钥应该随时更新或修改。`}</p>\n    <ul>\n      <li parentName=\"ul\">{`添加新的通知器或接收器。`}</li>\n      <li parentName=\"ul\">{`更改应发送至特定通知者或接收者的警报。`}</li>\n      <li parentName=\"ul\">{`更改发送的警报群组。`}</li>\n    </ul>\n    <p>{`默认情况下，您可以选择提供一个现有的 Alertmanager Config Secret（即 \"cattle-monitoring-system \"命名空间中的任何 Secret），或者允许 Rancher Monitoring 在您的集群上部署一个默认的 Alertmanager Config Secret。默认情况下，Rancher 创建的 Alertmanager Config Secret 将永远不会在升级或卸载`}<inlineCode parentName=\"p\">{`rancher-monitoring`}</inlineCode>{`图表时被修改或删除，以防止用户在图表上执行操作时丢失或覆盖其警报配置。`}</p>\n    <p>{`关于这个密钥中可以指定哪些字段的更多信息，请查看`}<a parentName=\"p\" {...{\n        \"href\": \"https://prometheus.io/docs/alerting/latest/alertmanager/\"\n      }}>{`Prometheus Alertmanager docs`}</a>{`。`}</p>\n    <p>{`Alertmanager 配置文件的完整规格以及它所包含的内容可以在`}<a parentName=\"p\" {...{\n        \"href\": \"https://prometheus.io/docs/alerting/latest/configuration/#configuration-file\"\n      }}>{`这里`}</a>{`找到。`}</p>\n    <p>{`通知集成是通过`}<inlineCode parentName=\"p\">{`receiver`}</inlineCode>{`来配置的，这一点在文件中已有说明`}<a parentName=\"p\" {...{\n        \"href\": \"https://prometheus.io/docs/alerting/latest/configuration/#receiver\"\n      }}>{`这里`}</a>{`。`}</p>\n    <p>{`更多信息，请参考`}<a parentName=\"p\" {...{\n        \"href\": \"https://www.prometheus.io/docs/alerting/latest/configuration/#route\"\n      }}>{`关于配置路由的 Prometheus 官方文档`}</a></p>\n    <h2 {...{\n      \"id\": \"为通知添加-ca-证书\"\n    }}>{`为通知添加 CA 证书`}</h2>\n    <p>{`如果您需要将受信任的 CA 添加到您的 notifier 中，请按照以下步骤进行操作。`}</p>\n    <ol>\n      <li parentName=\"ol\">{`创建`}<inlineCode parentName=\"li\">{`cattle-monitoring-system`}</inlineCode>{`命名空间。`}</li>\n      <li parentName=\"ol\">{`将您的可信 CA 密钥添加到`}<inlineCode parentName=\"li\">{`cattle-monitoring-system`}</inlineCode>{`命名空间中。`}</li>\n      <li parentName=\"ol\">{`部署或升级`}<inlineCode parentName=\"li\">{`rancher-monitoring`}</inlineCode>{`Helm chart。在 chart 选项中，引用“`}<strong parentName=\"li\">{`警报 > 附加密钥`}</strong>{`”中的密钥。`}</li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{`默认的 Alertmanager 自定义资源将可以访问您的受信任 CA。`}</p>\n    <h2 {...{\n      \"id\": \"配置拉取参数\"\n    }}>{`配置拉取参数`}</h2>\n    <p>{`如果目前不能通过 ServiceMonitor 或 PodMonitor 指定你想要的 scrape 配置，你可以在部署或升级`}<inlineCode parentName=\"p\">{`rancher-monitoring`}</inlineCode>{`时提供一个`}<inlineCode parentName=\"p\">{`additionalScrapeConfigSecret`}</inlineCode>{`。`}</p>\n    <p>{`一个`}<a parentName=\"p\" {...{\n        \"href\": \"https://prometheus.io/docs/prometheus/latest/configuration/configuration/#scrape_config\"\n      }}>{`scrape_config 部分`}</a>{`指定了一组目标和描述如何拉取它们的参数。在一般情况下，一个 scrape 配置指定了一个 job。`}</p>\n    <p>{`Istio 就是一个可以使用这个配置的例子。更多信息，请参见`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/istio/2.5/setup/enable-istio-in-cluster/_index\"\n      }}>{`在集群中启用 Istio`}</a>{`。`}</p>\n    <h2 {...{\n      \"id\": \"示例\"\n    }}>{`示例`}</h2>\n    <h3 {...{\n      \"id\": \"servicemonitor\"\n    }}>{`ServiceMonitor`}</h3>\n    <p>{`可以在`}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/prometheus-operator/prometheus-operator/blob/master/example/prometheus-operator-crd/monitoring.coreos.com_servicemonitors.yaml\"\n      }}>{`这里`}</a>{`找到一个 ServiceMonitor 自定义资源的例子。`}</p>\n    <h3 {...{\n      \"id\": \"podmonitor\"\n    }}>{`PodMonitor`}</h3>\n    <p>{`可以找到一个 PodMonitor 的例子`}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/prometheus-operator/prometheus-operator/blob/master/example/user-guides/getting-started/example-app-pod-monitor.yaml\"\n      }}>{`这里`}</a>{` 可以找到一个引用 Prometheus 资源的例子`}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/prometheus-operator/prometheus-operator/blob/master/example/user-guides/getting-started/prometheus-pod-monitor.yaml\"\n      }}>{`这里`}</a>{`。`}</p>\n    <h3 {...{\n      \"id\": \"prometheusrule\"\n    }}>{`PrometheusRule`}</h3>\n    <p>{`Prometheus 规则文件保存在 PrometheusRule 自定义资源中。使用 Prometheus 对象中的标签选择器字段 ruleSelector 来定义您要装入 Prometheus 的规则文件。PrometheusRule 的示例在 `}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/prometheus-operator/prometheus-operator/blob/master/Documentation/user-guides/alerting.md\"\n      }}>{`本页`}</a>{`。`}</p>\n    <h3 {...{\n      \"id\": \"alertmanager-config\"\n    }}>{`Alertmanager Config`}</h3>\n    <p>{`要通过 Slack 设置通知，需要将以下 Alertmanager 配置 YAML 放入 Alertmanager 配置秘籍的`}<inlineCode parentName=\"p\">{`alertmanager.yaml`}</inlineCode>{`键中，其中`}<inlineCode parentName=\"p\">{`api_url`}</inlineCode>{`应更新为使用 Slack 中的 Webhook URL。`}</p>\n    <pre><code parentName=\"pre\" {...{\n        \"className\": \"language-yaml\"\n      }}>{`route:\n  group_by: [\"job\"]\n  group_wait: 30s\n  group_interval: 5m\n  repeat_interval: 3h\n  receiver: \"slack-notifications\"\nreceivers:\n  - name: \"slack-notifications\"\n    slack_configs:\n      - send_resolved: true\n        text: '{{ template \"slack.rancher.text\" . }}'\n        api_url: <user-provided slack webhook url here>\ntemplates:\n  - /etc/alertmanager/config/*.tmpl\n`}</code></pre>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}