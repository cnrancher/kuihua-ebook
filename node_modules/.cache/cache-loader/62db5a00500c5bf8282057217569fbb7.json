A3EK,CA4EL,aAAK,iGAAL,CA5EK,CA6EL,SAAQ,CACN,KAAM,IADA,CAAR,CAEK,IAFL,CA7EK,CAgFL,aAAK,aAAL,CAAkB,kBAAG,UAAU,CAAC,GAAd,EAAsB,CACpC,OAAQ,+EAD4B,CAAtB,EAEX,0BAFW,CAAlB,CAEuC,GAFvC,CAhFK,CAmFL,SAAQ,CACN,KAAM,mBADA,CAAR,CAEK,mBAFL,CAnFK,CAsFL,aAAK,uCAAL,CAtFK,CAuFL,eAAK,qBAAM,UAAU,CAAC,KAAjB,EAA2B,EAA3B,EAAiC;CAAjC,CAAL,CAvFK,CAyFL,aAAK,4CAAL,CAzFK,CA0FL,SAAQ,CACN,KAAM,eADA,CAAR,CAEK,eAFL,CA1FK,CA6FL,aAAK,eAAL,CAAoB,kBAAY,UAAU,CAAC,GAAvB,EAA6B,qBAA7B,CAApB,CAAqF,kBAArF,CA7FK,CA8FL,aAAK,gCAAL,CA9FK,CA+FL,SAAQ,CACN,KAAM,UADA,CAAR,CAEK,UAFL,CA/FK,CAkGL,aAAK,+EAAL,CAAoF,kBAAY,UAAU,CAAC,GAAvB,EAA6B,aAA7B,CAApF,CAA6I,KAA7I,CAAkJ,kBAAY,UAAU,CAAC,GAAvB,EAA6B,eAA7B,CAAlJ,CAA6M,gCAA7M,CAlGK,CAAP,CAoGD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '网络',\n\tdescription: '已知问题会定期更新，旨在告知您有关在下一发行版本中可能不会立即解决的问题。',\n\tkeywords: [\n\t\t'k3s中文文档',\n\t\t'k3s 中文文档',\n\t\t'k3s中文',\n\t\t'k3s 中文',\n\t\t'k3s',\n\t\t'k3s教程',\n\t\t'k3s中国',\n\t\t'rancher',\n\t\t'k3s 中文教程',\n\t\t'网络'\n\t]\n};\nexport const metadata = {\n  \"id\": \"k3s/networking/_index\",\n  \"title\": \"网络\",\n  \"description\": \"已知问题会定期更新，旨在告知您有关在下一发行版本中可能不会立即解决的问题。\",\n  \"source\": \"@site/docs/k3s/networking/_index.md\",\n  \"permalink\": \"/docs/k3s/networking/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/k3s/networking/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar\": \"k3s\",\n  \"previous\": {\n    \"title\": \"卷和存储\",\n    \"permalink\": \"/docs/k3s/storage/_index\"\n  },\n  \"next\": {\n    \"title\": \"Helm\",\n    \"permalink\": \"/docs/k3s/helm/_index\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '开放端口',\n  id: '开放端口',\n  children: []\n}, {\n  value: 'CoreDNS',\n  id: 'coredns',\n  children: []\n}, {\n  value: 'Traefik Ingress Controller',\n  id: 'traefik-ingress-controller',\n  children: []\n}, {\n  value: 'Service Load Balancer',\n  id: 'service-load-balancer',\n  children: [{\n    value: 'Service LB 如何工作',\n    id: 'service-lb-如何工作',\n    children: []\n  }, {\n    value: '用法',\n    id: '用法',\n    children: []\n  }, {\n    value: '从节点中排除 Service LB',\n    id: '从节点中排除-service-lb',\n    children: []\n  }, {\n    value: '禁用 Service LB',\n    id: '禁用-service-lb',\n    children: []\n  }]\n}, {\n  value: '没有主机名的节点',\n  id: '没有主机名的节点',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <blockquote>\n      <p parentName=\"blockquote\"><strong parentName=\"p\">{`注意：`}</strong>{` 本页解释了 CoreDNS、Traefik Ingress 控制器和 Klipper service load balancer 如何在 K3s 中工作。`}</p>\n    </blockquote>\n    <h2 {...{\n      \"id\": \"开放端口\"\n    }}>{`开放端口`}</h2>\n    <p>{`请参阅`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/k3s/installation/network-options/_index\"\n      }}>{`安装网络选项`}</a>{`页面，以获取有关 Flannel 配置选项和后端选择，或如何设置自己的 CNI 的详细信息。`}</p>\n    <h2 {...{\n      \"id\": \"coredns\"\n    }}>{`CoreDNS`}</h2>\n    <p>{`关于 K3s 需要开放哪些端口，请参考`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/k3s/installation/installation-requirements/_index#%E7%BD%91%E7%BB%9C\"\n      }}>{`安装要求`}</a>{`。`}</p>\n    <ul>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"#coredns\"\n        }}>{`CoreDNS`}</a></li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"#traefik-ingress-controller\"\n        }}>{`Traefik Ingress Controller`}</a></li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"#service-load-balancer\"\n        }}>{`Service Load Balancer`}</a><ul parentName=\"li\">\n          <li parentName=\"ul\"><a parentName=\"li\" {...{\n              \"href\": \"#service-lb-%E5%A6%82%E4%BD%95%E5%B7%A5%E4%BD%9C\"\n            }}>{`Service LB 如何工作`}</a></li>\n          <li parentName=\"ul\"><a parentName=\"li\" {...{\n              \"href\": \"#%E7%94%A8%E6%B3%95\"\n            }}>{`用法`}</a></li>\n          <li parentName=\"ul\"><a parentName=\"li\" {...{\n              \"href\": \"#%E4%BB%8E%E8%8A%82%E7%82%B9%E4%B8%AD%E6%8E%92%E9%99%A4-service-lb\"\n            }}>{`从节点中排除 Service LB`}</a></li>\n          <li parentName=\"ul\"><a parentName=\"li\" {...{\n              \"href\": \"#%E7%A6%81%E7%94%A8-service-lb\"\n            }}>{`禁用 Service LB`}</a></li>\n        </ul></li>\n    </ul>\n    <h2 {...{\n      \"id\": \"traefik-ingress-controller\"\n    }}>{`Traefik Ingress Controller`}</h2>\n    <p><a parentName=\"p\" {...{\n        \"href\": \"https://traefik.io/\"\n      }}>{`Traefik`}</a>{`是一个现代的 HTTP 反向代理和负载均衡器，它是为了轻松部署微服务而生的。在设计，部署和运行应用程序时，它简化了网络复杂性。`}</p>\n    <p>{`启动 server 时，默认情况下会部署 Traefik。更多信息请参见`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/k3s/advanced/_index#%E8%87%AA%E5%8A%A8%E9%83%A8%E7%BD%B2%E6%B8%85%E5%8D%95\"\n      }}>{`自动部署清单`}</a>{`。默认的配置文件在`}<inlineCode parentName=\"p\">{`/var/lib/rancher/k3s/server/manifests/traefik.yaml`}</inlineCode>{`中，对该文件的任何修改都会以类似`}<inlineCode parentName=\"p\">{`kubectl apply`}</inlineCode>{`的方式自动部署到 Kubernetes 中。`}</p>\n    <p>{`Traefik ingress controller 将使用主机上的 80、443 和 8080 端口（即这些端口不能用于 HostPort 或 NodePort）。`}</p>\n    <p>{`Traefik 可以通过编辑`}<inlineCode parentName=\"p\">{`traefik.yaml`}</inlineCode>{`文件进行配置。为了防止 k3s 使用或覆盖修改后的版本，请使用`}<inlineCode parentName=\"p\">{`--no-deploy traefik`}</inlineCode>{`部署 k3s，并将修改后的副本存储在`}<inlineCode parentName=\"p\">{`k3s/server/manifests`}</inlineCode>{`目录中。更多信息请参考官方的`}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/helm/charts/tree/master/stable/traefik#configuration\"\n      }}>{`Traefik 配置参数`}</a>{`。`}</p>\n    <p>{`要禁用它，请使用`}<inlineCode parentName=\"p\">{`--disable traefik`}</inlineCode>{`选项启动每个 server。`}</p>\n    <h2 {...{\n      \"id\": \"service-load-balancer\"\n    }}>{`Service Load Balancer`}</h2>\n    <p>{`在你的 Kubernetes 集群中可以使用 service load balancer（LB）。K3s 提供了一个名为`}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/rancher/klipper-lb\"\n      }}>{`Klipper Load Balancer`}</a>{`的负载均衡器，它可以使用可用的主机端口。`}</p>\n    <p>{`上游 Kubernetes 允许创建 LoadBalancer 类型的 Service，但不包括 LB 的实现。某些 LB 服务需要云提供商，例如 Amazon EC2 或 Microsoft Azure。相比之下，K3s service LB 使得可以在没有云提供商的情况下使用 LB 服务。`}</p>\n    <h3 {...{\n      \"id\": \"service-lb-如何工作\"\n    }}>{`Service LB 如何工作`}</h3>\n    <p>{`一些云提供商（如 Linode）会以 \"localhost \"作为主机名创建机器，而其他提供商可能根本没有设置主机名。这可能会导致域名解析的问题。你可以用`}<inlineCode parentName=\"p\">{`--node-name`}</inlineCode>{`标志或`}<inlineCode parentName=\"p\">{`K3S_NODE_NAME`}</inlineCode>{`环境变量来运行 K3s，这样就会传递节点名称来解决这个问题。\nK3s 创建了一个控制器，该控制器为 service load balancer 创建了一个 Pod，这个 Pod 是`}<a parentName=\"p\" {...{\n        \"href\": \"https://kubernetes.io/docs/concepts/services-networking/service/\"\n      }}>{`Service`}</a>{`类型的 Kubernetes 对象。`}</p>\n    <p>{`对于每个 service load balancer，都会创建一个`}<a parentName=\"p\" {...{\n        \"href\": \"https://kubernetes.io/docs/concepts/workloads/controllers/daemonset/\"\n      }}>{`DaemonSet`}</a>{`。 DaemonSet 在每个节点上创建一个前缀为`}<inlineCode parentName=\"p\">{`svc`}</inlineCode>{`的 Pod。`}</p>\n    <p>{`Service LB 控制器会监听其他 Kubernetes Services。当它找到一个 Service 后，它会在所有节点上使用 DaemonSet 为该服务创建一个代理 Pod。这个 Pod 成为其他 Service 的代理，例如，来自节点上 8000 端口的请求可以被路由到端口 8888 上的工作负载。`}</p>\n    <p>{`如果 Service LB 运行在有外部 IP 的节点上，则使用外部 IP。`}</p>\n    <p>{`如果创建多个 Services，则为每个 Service 创建一个单独的 DaemonSet。`}</p>\n    <p>{`只要使用不同的端口，就可以在同一节点上运行多个 Services。`}</p>\n    <p>{`如果您尝试创建一个在 80 端口上监听的 Service LB，Service LB 将尝试在集群中找到 80 端口的空闲主机。如果该端口没有可用的主机，LB 将保持 Pending 状态。`}</p>\n    <h3 {...{\n      \"id\": \"用法\"\n    }}>{`用法`}</h3>\n    <p>{`在 K3s 中创建一个`}<a parentName=\"p\" {...{\n        \"href\": \"https://kubernetes.io/docs/concepts/services-networking/service/#loadbalancer\"\n      }}>{`LoadBalancer 类型的 Service`}</a>{`。`}</p>\n    <h3 {...{\n      \"id\": \"从节点中排除-service-lb\"\n    }}>{`从节点中排除 Service LB`}</h3>\n    <p>{`要排除节点使用 Service LB，请将以下标签添加到不应排除的节点上：`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`svccontroller.k3s.cattle.io/enablelb\n`}</code></pre>\n    <p>{`如果使用标签，则 service load balancer 仅在标记的节点上运行。`}</p>\n    <h3 {...{\n      \"id\": \"禁用-service-lb\"\n    }}>{`禁用 Service LB`}</h3>\n    <p>{`要禁用嵌入式 LB，请使用`}<inlineCode parentName=\"p\">{`--disable servicelb`}</inlineCode>{`选项运行 k3s server。`}</p>\n    <p>{`如果您希望运行其他 LB，例如 MetalLB，这是必需的。`}</p>\n    <h2 {...{\n      \"id\": \"没有主机名的节点\"\n    }}>{`没有主机名的节点`}</h2>\n    <p>{`一些云提供商（如 Linode）会以 \"localhost \"作为主机名创建机器，而其他提供商可能根本没有设置主机名。这可能会导致域名解析的问题。你可以用`}<inlineCode parentName=\"p\">{`--node-name`}</inlineCode>{`标志或`}<inlineCode parentName=\"p\">{`K3S_NODE_NAME`}</inlineCode>{`环境变量来运行 K3s，这样就会传递节点名称来解决这个问题。`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}