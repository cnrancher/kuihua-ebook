8BAAtB,CAAoD,kBAAY,UAAU,CAAC,IAAvB,EAA8B,mBAA9B,CAApD,CAAoH,eAApH,CADF,CAEE,UAAI,UAAU,CAAC,IAAf,EAAsB,SAAtB,CAA+B,kBAAY,UAAU,CAAC,IAAvB,EAA8B,qBAA9B,CAA/B,CAAiG,IAAjG,CAFF,CAFF,CApBF,CA2BE,UAAI,UAAU,CAAC,IAAf,EACE,SAAG,UAAU,CAAC,IAAd,EAAqB,WAArB,CADF,CA3BF,CA8BE,UAAI,UAAU,CAAC,IAAf,EACE,SAAG,UAAU,CAAC,IAAd,EAAqB,gDAArB,CAAqE,kBAAG,UAAU,CAAC,GAAd,EAAsB,CACvF,OAAQ,sEAD+E,CAAtB,EAE9D,OAF8D,CAArE,CAEoB,8BAFpB,CAEkD,kBAAG,UAAU,CAAC,GAAd,EAAsB,CACpE,OAAQ,4DAD4D,CAAtB,EAE3C,MAF2C,CAFlD,CAImB,GAJnB,CADF,CA9BF,CAhHK,CAsJL,SAAQ,CACN,KAAM,mCADA,CAAR,CAEK,qCAFL,CAtJK,CAyJL,aAAK,2EAAL,CAAgF,kBAAG,UAAU,CAAC,GAAd,EAAsB,CAClG,OAAQ,sDAD0F,CAAtB,EAEzE,MAFyE,CAAhF,CAEmB,IAFnB,CAEuB,cAAQ,UAAU,CAAC,GAAnB,EAAyB,IAAzB,CAFvB,CAE+D,iDAF/D,CAEgH,kBAAG,UAAU,CAAC,GAAd,EAAsB,CAClI,OAAQ,oDAD0H,CAAtB,EAEzG,YAFyG,CAFhH,CAIyB,mBAJzB,CAzJK,CAAP,CA+JD,CAED,CACAW,UAAU,CAACG,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '恢复集群',\n\tdescription: '在 RKE 集群中可以很容易地执行 etcd 备份和恢复。etcd 数据库的快照被获取并保存到本地的 etcd 节点或 S3 兼容的目标上。配置 S3 的优点是，即使所有的 etcd 节点都丢失了，因为您的快照保存在远程，您仍然可以用它来恢复集群。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'集群管理员指南',\n\t\t'集群访问控制',\n\t\t'恢复集群'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/cluster-admin/restoring-etcd/_index\",\n  \"title\": \"恢复集群\",\n  \"description\": \"在 RKE 集群中可以很容易地执行 etcd 备份和恢复。etcd 数据库的快照被获取并保存到本地的 etcd 节点或 S3 兼容的目标上。配置 S3 的优点是，即使所有的 etcd 节点都丢失了，因为您的快照保存在远程，您仍然可以用它来恢复集群。\",\n  \"source\": \"@site/docs/rancher2/cluster-admin/restoring-etcd/_index.md\",\n  \"permalink\": \"/docs/rancher2/cluster-admin/restoring-etcd/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/cluster-admin/restoring-etcd/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055\n};\n  /* @jsx mdx */\nimport Tabs from '@theme/Tabs';\nimport TabItem from '@theme/TabItem';\nexport const rightToc = [{\n  value: '查看可用的快照',\n  id: '查看可用的快照',\n  children: []\n}, {\n  value: '从快照中恢复集群',\n  id: '从快照中恢复集群',\n  children: []\n}, {\n  value: '在没有快照的情况下恢复 etcd',\n  id: '在没有快照的情况下恢复-etcd',\n  children: []\n}, {\n  value: '在 Rancher v2.2.0 之前的版本中创建的集群里启用定期快照',\n  id: '在-rancher-v220-之前的版本中创建的集群里启用定期快照',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n    <p><em parentName=\"p\">{`从 v2.2.0 开始可用`}</em></p>\n    <p>{`etcd 数据库简化了`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-provisioning/rke-clusters/_index\"\n      }}>{`RKE 集群`}</a>{`执行 etcd 备份和恢复的过程。etcd 数据库的快照被获取并保存到本地的 etcd 节点或 S3 兼容的目标上，即使所有的 etcd 节点都丢失了，因为您的快照保存在云端，您可以用它恢复集群。`}</p>\n    <p>{`Rancher 建议启用`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/backing-up-etcd/_index\"\n      }}>{`对 etcd 进行定期快照的功能`}</a>{`，但是您也可以轻松的进行`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/backing-up-etcd/_index\"\n      }}>{`一次性快照`}</a>{`。Rancher 允许从`}<a parentName=\"p\" {...{\n        \"href\": \"#%E4%BB%8E%E5%BF%AB%E7%85%A7%E6%81%A2%E5%A4%8D%E9%9B%86%E7%BE%A4\"\n      }}>{`已保存的快照`}</a>{`恢复数据，或者如果您没有任何快照，您仍然可以`}<a parentName=\"p\" {...{\n        \"href\": \"#%E5%9C%A8%E6%B2%A1%E6%9C%89%E5%BF%AB%E7%85%A7%E7%9A%84%E6%83%85%E5%86%B5%E4%B8%8B%E6%81%A2%E5%A4%8D-etcd\"\n      }}>{`恢复 etcd`}</a>{`。`}</p>\n    <p>{`从 Rancher v2.4.0 开始，您还可以还原集群的 Kubernetes 版本和集群配置。`}</p>\n    <h2 {...{\n      \"id\": \"查看可用的快照\"\n    }}>{`查看可用的快照`}</h2>\n    <p>{`在 Rancher UI 中，您可以查看所有可用快照的列表。`}</p>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`在 `}<strong parentName=\"p\">{`全局`}</strong>{` 视图中，导航到要查看快照的集群。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`从导航栏中单击 `}<strong parentName=\"p\">{`工具 > 快照`}</strong>{` 查看保存的快照列表。这些快照包括创建它们的时间戳。`}</p>\n      </li>\n    </ol>\n    <h2 {...{\n      \"id\": \"从快照中恢复集群\"\n    }}>{`从快照中恢复集群`}</h2>\n    <p>{`如果 Kubernetes 集群已损坏，您可以从快照中还原该集群。`}</p>\n    <p>{`在 Rancher v2.4.0 中恢复的功能有所变化。`}</p>\n\n\n    <Tabs defaultValue=\"new\" values={[{\n      label: 'Rancher v2.4.0+',\n      value: 'new'\n    }, {\n      label: 'Rancher v2.4.0 之前的版本',\n      value: 'old'\n    }]} mdxType=\"Tabs\">\n      <TabItem value=\"new\" mdxType=\"TabItem\">\n        <p>{`快照由 etcd 中的集群数据、Kubernetes 版本和`}<inlineCode parentName=\"p\">{`cluster.yml`}</inlineCode>{`形式的集群配置组成。您可以使用快照中的这些组件。选择如何从快照中恢复集群：`}</p>\n        <ul>\n          <li parentName=\"ul\"><strong parentName=\"li\">{`仅恢复 etcd 中的集群数据：`}</strong>{` 此还原类似于在 v2.4.0 之前的 Rancher 中还原快照。`}</li>\n          <li parentName=\"ul\"><strong parentName=\"li\">{`恢复 etcd 和 Kubernetes 版本：`}</strong>{` 如果由于升级 Kubernetes 版本导致集群出现故障，并且您尚未进行任何集群配置更改，则应使用此选项。`}</li>\n          <li parentName=\"ul\"><strong parentName=\"li\">{`恢复 etcd，Kubernetes 版本和集群配置：`}</strong>{` 如果在升级时同时更改了 Kubernetes 版本和集群配置，则应使用此选项。`}</li>\n        </ul>\n        <p>{`回滚到 Kubernetes 之前的版本时，`}<a parentName=\"p\" {...{\n            \"href\": \"/docs/rancher2/cluster-admin/upgrading-kubernetes/_index\"\n          }}>{`升级策略选项`}</a>{`是将不会生效。在将工作节点恢复到较早的 Kubernetes 版本之前，不会对其进行`}<strong parentName=\"p\">{`暂停（Cordon）`}</strong>{`或`}<strong parentName=\"p\">{`驱散（Drain）`}</strong>{`，这是为了尽快将运行状况不佳的集群恢复到健康状态。`}</p>\n        <blockquote>\n          <p parentName=\"blockquote\"><strong parentName=\"p\">{`先决条件：`}</strong>{` 要从 S3 恢复快照，需要将集群配置为`}<a parentName=\"p\" {...{\n              \"href\": \"/docs/rancher2/cluster-admin/backing-up-etcd/_index\"\n            }}>{`在 S3 上进行定期快照`}</a>{`。`}</p>\n        </blockquote>\n        <ol>\n          <li parentName=\"ol\">\n            <p parentName=\"li\">{`在 `}<strong parentName=\"p\">{`全局`}</strong>{` 视图中，导航到要恢复的集群。`}</p>\n          </li>\n          <li parentName=\"ol\">\n            <p parentName=\"li\">{`单击 `}<strong parentName=\"p\">{`垂直省略号 (...)> 恢复`}</strong>{`。`}</p>\n          </li>\n          <li parentName=\"ol\">\n            <p parentName=\"li\">{`从可用快照的下拉菜单中选择要用于恢复集群的快照。`}</p>\n          </li>\n          <li parentName=\"ol\">\n            <p parentName=\"li\">{`在`}<strong parentName=\"p\">{`恢复类型`}</strong>{`字段中，选择上述恢复方式之一。`}</p>\n          </li>\n          <li parentName=\"ol\">\n            <p parentName=\"li\">{`单击`}<strong parentName=\"p\">{`保存`}</strong>{`。`}</p>\n          </li>\n        </ol>\n        <p><strong parentName=\"p\">{`结果：`}</strong>{` 集群将进入`}<inlineCode parentName=\"p\">{`Updating`}</inlineCode>{`状态，从快照恢复`}<inlineCode parentName=\"p\">{`etcd`}</inlineCode>{`节点的过程将启动。当集群回到`}<inlineCode parentName=\"p\">{`active`}</inlineCode>{`状态时，表示集群已经被恢复了。`}</p>\n      </TabItem>\n      <TabItem value=\"old\" mdxType=\"TabItem\">\n        <blockquote>\n          <p parentName=\"blockquote\"><strong parentName=\"p\">{`先决条件：`}</strong></p>\n          <ul parentName=\"blockquote\">\n            <li parentName=\"ul\">{`确保您的 etcd 节点运行状况良好。如果要还原的集群中的 etcd 节点不可用，建议在尝试还原之前从 Rancher 中删除所有 etcd 节点。如果您使用的是通过节点池`}<a parentName=\"li\" {...{\n                \"href\": \"/docs/rancher2/cluster-provisioning/rke-clusters/node-pools/_index\"\n              }}>{`在基础设施中创建节点`}</a>{`的集群，那么新的 etcd 节点将自动创建。对于`}<a parentName=\"li\" {...{\n                \"href\": \"/docs/rancher2/cluster-provisioning/rke-clusters/custom-nodes/_index\"\n              }}>{`自定义集群`}</a>{`，请确保将新的 etcd 节点添加到集群。`}</li>\n            <li parentName=\"ul\">{`要从 S3 恢复快照，需要将集群配置为`}<a parentName=\"li\" {...{\n                \"href\": \"/docs/rancher2/cluster-admin/backing-up-etcd/_index\"\n              }}>{`在 S3 上进行定期快照`}</a></li>\n          </ul>\n        </blockquote>\n        <ol>\n          <li parentName=\"ol\">\n            <p parentName=\"li\">{`在 `}<strong parentName=\"p\">{`全局`}</strong>{` 视图中，导航到要恢复的集群。`}</p>\n          </li>\n          <li parentName=\"ol\">\n            <p parentName=\"li\">{`单击 `}<strong parentName=\"p\">{`垂直省略号 (...) > 恢复`}</strong>{`。`}</p>\n          </li>\n          <li parentName=\"ol\">\n            <p parentName=\"li\">{`从可用快照的下拉菜单中选择要用于恢复集群的快照。`}</p>\n          </li>\n          <li parentName=\"ol\">\n            <p parentName=\"li\">{`单击 `}<strong parentName=\"p\">{`保存`}</strong>{`。`}</p>\n          </li>\n        </ol>\n        <p><strong parentName=\"p\">{`结果：`}</strong>{` 集群将进入`}<inlineCode parentName=\"p\">{`Updating`}</inlineCode>{`状态，从快照恢复`}<inlineCode parentName=\"p\">{`etcd`}</inlineCode>{`节点的过程将启动。当集群回到`}<inlineCode parentName=\"p\">{`active`}</inlineCode>{`状态时，表示集群已经被恢复了。`}</p>\n      </TabItem>\n    </Tabs>\n    <h2 {...{\n      \"id\": \"在没有快照的情况下恢复-etcd\"\n    }}>{`在没有快照的情况下恢复 etcd`}</h2>\n    <p>{`如果 etcd 节点组没有保持法定的成员数（quorum），即超过半数以上的 etcd 节点组成员丢失，Kubernetes 集群将报告失败，因为在 Kubernetes 集群中不能执行任何操作，例如部署工作负载。请查看 Kubernetes 集群中的`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-provisioning/production/_index\"\n      }}>{`etcd 节点数量`}</a>{`的最佳实践。如果您想恢复您的 etcd 节点集，请遵循以下说明：`}</p>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`通过删除所有其他 etcd 节点，在集群中只保留一个 etcd 节点。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`在剩下的 etcd 节点上，运行以下命令：`}</p>\n        <pre parentName=\"li\"><code parentName=\"pre\" {...{\n            \"className\": \"language-bash\"\n          }}>{`$ docker run --rm -v /var/run/docker.sock:/var/run/docker.sock assaflavie/runlike etcd\n`}</code></pre>\n        <p parentName=\"li\">{`此命令输出 etcd 的运行命令，保存此命令供以后使用。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`停止上一步启动的 etcd 容器，将其重命名为 `}<inlineCode parentName=\"p\">{`etcd-old`}</inlineCode>{`。`}</p>\n        <pre parentName=\"li\"><code parentName=\"pre\" {...{\n            \"className\": \"language-bash\"\n          }}>{`$ docker stop etcd\n$ docker rename etcd etcd-old\n`}</code></pre>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`修改步骤 2 中保存的命令`}</p>\n        <ul parentName=\"li\">\n          <li parentName=\"ul\">{`如果您最初拥有一个以上的 etcd 节点，那么您需要将 `}<inlineCode parentName=\"li\">{`--initial-cluster`}</inlineCode>{` 更改为只包含剩余的节点。`}</li>\n          <li parentName=\"ul\">{`在命令末尾添加`}<inlineCode parentName=\"li\">{`--force-new-cluster`}</inlineCode>{` 。`}</li>\n        </ul>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`运行修改后的命令。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`在单个节点启动并运行之后，Rancher 建议向集群添加额外的 etcd 节点。如果您有一个`}<a parentName=\"p\" {...{\n            \"href\": \"/docs/rancher2/cluster-provisioning/rke-clusters/custom-nodes/_index\"\n          }}>{`自定义集群`}</a>{`并且希望重用旧节点，则需要在尝试将它们重新添加回集群之前`}<a parentName=\"p\" {...{\n            \"href\": \"/docs/rancher2/cluster-admin/cleaning-cluster-nodes/_index\"\n          }}>{`清理节点`}</a>{`。`}</p>\n      </li>\n    </ol>\n    <h2 {...{\n      \"id\": \"在-rancher-v220-之前的版本中创建的集群里启用定期快照\"\n    }}>{`在 Rancher v2.2.0 之前的版本中创建的集群里启用定期快照`}</h2>\n    <p>{`如果您有任何在 v2.2.0 之前创建的 Rancher 启动的 Kubernetes 集群（RKE 集群），在升级 Rancher 之后，您必须`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/editing-clusters/_index\"\n      }}>{`编辑集群`}</a>{`并且`}<strong parentName=\"p\">{`保存`}</strong>{`它，以启用更新的快照功能。即使您已经在 v2.2.0 之前创建了快照，也必须执行此步骤，因为在`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/restoring-etcd/_index\"\n      }}>{`通过 UI 还原集群`}</a>{`时，无法使用较早版本中创建的快照。`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}