EAA2B,EAA3B,EAAiC;;;;;;;;;;;;;CAAjC,CAAL,CA/BK,CA6CL,SAAQ,CACN,KAAM,4BADA,CAAR,CAEG,kBAAY,UAAU,CAAC,IAAvB,EAA8B,qBAA9B,CAFH,CAEqE,UAFrE,CA7CK,CAgDL,eAAK,qBAAM,UAAU,CAAC,KAAjB,EAA2B,EAA3B,EAAiC;;;CAAjC,CAAL,CAhDK,CAAP,CAqDD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '外部服务'\n};\nexport const metadata = {\n  \"id\": \"rancher1/infrastructure/cattle/adding-external-services/_index\",\n  \"title\": \"外部服务\",\n  \"description\": \"您可能会有一些部署在 Rancher 之外的服务想要整合进 Rancher。您可以通过添加一个外部服务的功能将它添加到 Rancher 集群中。\",\n  \"source\": \"@site/docs/rancher1/infrastructure/cattle/adding-external-services/_index.md\",\n  \"permalink\": \"/docs/rancher1/infrastructure/cattle/adding-external-services/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher1/infrastructure/cattle/adding-external-services/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar\": \"troubleshooting\",\n  \"previous\": {\n    \"title\": \"系统设置\",\n    \"permalink\": \"/docs/rancher1/configurations/environments/settings/_index\"\n  },\n  \"next\": {\n    \"title\": \"负载均衡\",\n    \"permalink\": \"/docs/rancher1/infrastructure/cattle/adding-load-balancers/_index\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '在 UI 上添加外部服务',\n  id: '在-ui-上添加外部服务',\n  children: []\n}, {\n  value: '使用 Rancher Compose 添加外部服务',\n  id: '使用-rancher-compose-添加外部服务',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p>{`您可能会有一些部署在 Rancher 之外的服务想要整合进 Rancher。您可以通过添加一个外部服务的功能将它添加到 Rancher 集群中。`}</p>\n    <h3 {...{\n      \"id\": \"在-ui-上添加外部服务\"\n    }}>{`在 UI 上添加外部服务`}</h3>\n    <p>{`在您的应用上，您可以通过 `}<strong parentName=\"p\">{`添加服务`}</strong>{` 旁边的下拉菜单按钮添加外部服务。选择 `}<strong parentName=\"p\">{`外部服务`}</strong>{`。 或者您在应用层级的页面查看您的应用，同样存在相同的 `}<strong parentName=\"p\">{`添加服务`}</strong>{` 下拉菜单。`}</p>\n    <p>{`您将需要提供一个外部服务的 `}<strong parentName=\"p\">{`名称`}</strong>{`，如果需要的话，提供这个服务的 `}<strong parentName=\"p\">{`描述`}</strong>{`。`}</p>\n    <p>{`添加您想要的目标。您可以选择外部的 IP 或者域名。最后单击 `}<strong parentName=\"p\">{`添加`}</strong>{`。`}</p>\n    <p>{`外部服务的 IP 和域名会在服务中呈现。和 Rancher 的服务一样，您需要去启动一个外部服务。`}</p>\n    <h4 {...{\n      \"id\": \"添加删除外部服务目标\"\n    }}>{`添加/删除外部服务目标`}</h4>\n    <p>{`在任何时候您都可以编辑您外部服务中的服务目标。在外部服务的下拉菜单中单击 `}<strong parentName=\"p\">{`编辑`}</strong>{`，您可以添加或者移除目标。`}</p>\n    <h3 {...{\n      \"id\": \"使用-rancher-compose-添加外部服务\"\n    }}>{`使用 Rancher Compose 添加外部服务`}</h3>\n    <p>{`在外部服务中，您可以设置外部 IP 地址 `}<strong parentName=\"p\">{`或者`}</strong>{` 域名。`}<inlineCode parentName=\"p\">{`rancher/external-service`}</inlineCode>{` 并不是一个真实的镜像，但在 `}<inlineCode parentName=\"p\">{`docker-compose.yml`}</inlineCode>{` 中是必要的。Rancher 不会为外部服务创建容器。`}</p>\n    <h4 {...{\n      \"id\": \"docker-composeyml例子\"\n    }}><inlineCode parentName=\"h4\">{`docker-compose.yml`}</inlineCode>{`例子`}</h4>\n    <pre><code parentName=\"pre\" {...{}}>{`version: '2'\nservices:\n  db:\n    image: rancher/external-service\n  redis:\n    image: redis\n`}</code></pre>\n    <h4 {...{\n      \"id\": \"rancher-composeyml-使用外部-ip-的例子\"\n    }}><inlineCode parentName=\"h4\">{`rancher-compose.yml`}</inlineCode>{` 使用外部 IP 的例子`}</h4>\n    <pre><code parentName=\"pre\" {...{}}>{`version: '2'\nservices:\n  db:\n    external_ips:\n    - 1.1.1.1\n    - 2.2.2.2\n\n  # Override any service to become an external service\n  redis:\n    image: redis\n    external_ips:\n    - 1.1.1.1\n    - 2.2.2.2\n`}</code></pre>\n    <h4 {...{\n      \"id\": \"rancher-composeyml-使用域名的例子\"\n    }}><inlineCode parentName=\"h4\">{`rancher-compose.yml`}</inlineCode>{` 使用域名的例子`}</h4>\n    <pre><code parentName=\"pre\" {...{}}>{`version: '2'\nservices:\n  db:\n`}</code></pre>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}