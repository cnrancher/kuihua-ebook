UAAU,CAAC,IAAvB,EAA8B,8BAA9B,CAFH,CADF,CAIE,mBAAI,UAAU,CAAC,IAAf,EAAwB,CACtB,QAAS,MADa,CAAxB,EAEK,aAFL,CAJF,CAzCF,CAXF,CA/CK,CAAP,CA8GD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '备份和恢复',\n\tdescription: '3s 的备份和恢复方式由您使用的数据存储类型决定。本文提供了基于外部数据存储和嵌入式 etcd 数据存储*两种存储方式的备份和恢复操作指导，请按需阅读以下章节完成备份和还原K3s。',\n\tkeywords: [\n\t\t'k3s中文文档',\n\t\t'k3s 中文文档',\n\t\t'k3s中文',\n\t\t'k3s 中文',\n\t\t'k3s',\n\t\t'k3s教程',\n\t\t'k3s中国',\n\t\t'rancher',\n\t\t'k3s 中文教程',\n\t\t'备份和恢复'\n\t]\n};\nexport const metadata = {\n  \"id\": \"k3s/backup-restore/_index\",\n  \"title\": \"备份和恢复\",\n  \"description\": \"3s 的备份和恢复方式由您使用的数据存储类型决定。本文提供了基于外部数据存储和嵌入式 etcd 数据存储*两种存储方式的备份和恢复操作指导，请按需阅读以下章节完成备份和还原K3s。\",\n  \"source\": \"@site/docs/k3s/backup-restore/_index.md\",\n  \"permalink\": \"/docs/k3s/backup-restore/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/k3s/backup-restore/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '外部数据存储备份和还原',\n  id: '外部数据存储备份和还原',\n  children: []\n}, {\n  value: '嵌入式 etcd 数据存储备份和还原（实验性）',\n  id: '嵌入式-etcd-数据存储备份和还原（实验性）',\n  children: [{\n    value: '创建快照',\n    id: '创建快照',\n    children: []\n  }, {\n    value: '从快照恢复集群',\n    id: '从快照恢复集群',\n    children: []\n  }, {\n    value: '参数',\n    id: '参数',\n    children: []\n  }]\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p>{`K3s 的备份和恢复方式由您使用的数据存储类型决定。本文提供了基于`}<strong parentName=\"p\">{`外部数据存储`}</strong>{`和`}<strong parentName=\"p\">{`嵌入式 etcd 数据存储`}</strong>{`两种存储方式的备份和恢复操作指导，请按需阅读以下章节完成备份和还原 K3s。`}</p>\n    <h2 {...{\n      \"id\": \"外部数据存储备份和还原\"\n    }}>{`外部数据存储备份和还原`}</h2>\n    <p>{`当使用外部数据存储时，备份和恢复操作是在 K3s 之外处理的。数据库管理员需要对外部数据库进行备份，或者从快照或转储中进行恢复。`}</p>\n    <p>{`我们建议将数据库配置为`}<strong parentName=\"p\">{`执行定期快照`}</strong>{`。`}</p>\n    <p>{`有关获取数据库快照和从快照还原数据库的详细信息，请参阅官方数据库文档：`}</p>\n    <ul>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"https://dev.mysql.com/doc/refman/8.0/en/replication-snapshot-method.html\"\n        }}>{`MySQL 官方文档`}</a></li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"https://www.postgresql.org/docs/8.3/backup-dump.html\"\n        }}>{`PostgreSQL 官方文档`}</a></li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"https://github.com/etcd-io/etcd/blob/master/Documentation/op-guide/recovery.md\"\n        }}>{`etcd 官方文档`}</a></li>\n    </ul>\n    <h2 {...{\n      \"id\": \"嵌入式-etcd-数据存储备份和还原（实验性）\"\n    }}>{`嵌入式 etcd 数据存储备份和还原（实验性）`}</h2>\n    <p><em parentName=\"p\">{`v1.19.1+k3s1 可用`}</em></p>\n    <p>{`在本节中，您将学习如何创建 K3s 集群数据的备份，并从备份中恢复集群。`}</p>\n    <h3 {...{\n      \"id\": \"创建快照\"\n    }}>{`创建快照`}</h3>\n    <p>{`K3s 默认启用快照。快照目录默认为`}<inlineCode parentName=\"p\">{`/server/db/Snapshots`}</inlineCode>{`。要配置快照间隔或保留的快照数量，请参考下文的参数表格。`}</p>\n    <h3 {...{\n      \"id\": \"从快照恢复集群\"\n    }}>{`从快照恢复集群`}</h3>\n    <p>{`当 K3s 从备份中恢复时，旧的数据目录将被移动到`}<inlineCode parentName=\"p\">{`/server/db/etcd-old/`}</inlineCode>{`。然后 K3s 会尝试通过创建一个新的数据目录来恢复快照，然后从一个带有一个 etcd 成员的新 K3s 集群启动 etcd。`}</p>\n    <p>{`要从备份中恢复集群，运行 K3s 时，请使用`}<inlineCode parentName=\"p\">{`--cluster-reset`}</inlineCode>{`选项运行 K3s，同时给出`}<inlineCode parentName=\"p\">{`--cluster-reset-restore-path`}</inlineCode>{`，如下：`}</p>\n    <pre><code parentName=\"pre\" {...{\n        \"className\": \"language-shell\"\n      }}>{`./k3s server \\\\\n  --cluster-reset \\\\\n  --cluster-reset-restore-path=<PATH-TO-SNAPSHOT>\n`}</code></pre>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{`日志中出现一条信息，可以在没有标志的情况下重新启动 K3s。再次启动 K3s，可以成功运行并从指定的快照中恢复。`}</p>\n    <h3 {...{\n      \"id\": \"参数\"\n    }}>{`参数`}</h3>\n    <p>{`这些选项可以通过命令行或`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/k3s/installation/install-options/_index\"\n      }}>{`K3s 配置文件`}</a>{`传递进来，这样可以更容易使用。`}</p>\n    <table>\n      <thead parentName=\"table\">\n        <tr parentName=\"thead\">\n          <th parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`参数`}</th>\n          <th parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`描述`}</th>\n        </tr>\n      </thead>\n      <tbody parentName=\"table\">\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}><inlineCode parentName=\"td\">{`--etcd-disable-snapshots`}</inlineCode></td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`禁用自动 etcd 快照`}</td>\n        </tr>\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}><inlineCode parentName=\"td\">{`--etcd-snapshot-schedule-cron`}</inlineCode></td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`以 Cron 表达式的形式配置触发定时快照的时间点，例如：每 5 小时触发一次`}<inlineCode parentName=\"td\">{`* */5 * * *`}</inlineCode>{`，默认值为每 12 小时触发一次：`}<inlineCode parentName=\"td\">{`0 */12 * * *`}</inlineCode></td>\n        </tr>\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}><inlineCode parentName=\"td\">{`--etcd-snapshot-retention`}</inlineCode></td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`保留的快照数量，默认值为 5。`}</td>\n        </tr>\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}><inlineCode parentName=\"td\">{`--etcd-snapshot-dir`}</inlineCode></td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`保存数据库快照的目录路径。(默认位置：`}<inlineCode parentName=\"td\">{`\\${data-dir}/db/snapshots`}</inlineCode>{`)`}</td>\n        </tr>\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}><inlineCode parentName=\"td\">{`--cluster-reset`}</inlineCode></td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`忘记所有的对等体，成为新集群的唯一成员，也可以通过环境变量`}<inlineCode parentName=\"td\">{`[$K3S_CLUSTER_RESET]`}</inlineCode>{`进行设置。`}</td>\n        </tr>\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}><inlineCode parentName=\"td\">{`--cluster-reset-restore-path`}</inlineCode></td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`要恢复的快照文件的路径`}</td>\n        </tr>\n      </tbody>\n    </table>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}