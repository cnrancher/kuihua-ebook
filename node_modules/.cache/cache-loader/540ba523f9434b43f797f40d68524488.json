m 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: 'F5 启动 WAF 功能',\n\tdescription: 'F5 通过成熟健壮的负载均衡和应用安全保障 Rancher 管平面的高可用性和安全性，也可以通过 SSLO 服务编排组件将流量牵引到外部的 WAF 资源池，实现更好灵活性和扩展性。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'安装指南',\n\t\t'资料、参考和高级选项',\n\t\t'F5 启动 WAF 功能'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/installation/options/F5-WAF/_index\",\n  \"title\": \"F5 启动 WAF 功能\",\n  \"description\": \"F5 通过成熟健壮的负载均衡和应用安全保障 Rancher 管平面的高可用性和安全性，也可以通过 SSLO 服务编排组件将流量牵引到外部的 WAF 资源池，实现更好灵活性和扩展性。\",\n  \"source\": \"@site/docs/rancher2/installation/options/F5-WAF/_index.md\",\n  \"permalink\": \"/docs/rancher2/installation/options/F5-WAF/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/installation/options/F5-WAF/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '概述',\n  id: '概述',\n  children: []\n}, {\n  value: '内置 WAF 实现管理平面安全防护',\n  id: '内置-waf-实现管理平面安全防护',\n  children: [{\n    value: '先决条件',\n    id: '先决条件',\n    children: []\n  }, {\n    value: '配置实现',\n    id: '配置实现',\n    children: []\n  }]\n}, {\n  value: '外部 WAF 实现管理平面安全防护',\n  id: '外部-waf-实现管理平面安全防护',\n  children: [{\n    value: '概述',\n    id: '概述-1',\n    children: []\n  }, {\n    value: '前置条件',\n    id: '前置条件',\n    children: []\n  }, {\n    value: '配置实现',\n    id: '配置实现-1',\n    children: []\n  }]\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <h2 {...{\n      \"id\": \"概述\"\n    }}>{`概述`}</h2>\n    <p>{`F5 通过成熟健壮的负载均衡和应用安全保障 Rancher 管平面的高可用性和安全性，也可以通过 SSLO 服务编排组件将流量牵引到外部的 WAF 资源池，实现更好灵活性和扩展性。`}</p>\n    <h2 {...{\n      \"id\": \"内置-waf-实现管理平面安全防护\"\n    }}>{`内置 WAF 实现管理平面安全防护`}</h2>\n    <h3 {...{\n      \"id\": \"先决条件\"\n    }}>{`先决条件`}</h3>\n    <ul>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`已经部署 F5 BIG-IP LTM，实现了 Rancher 管理平面的负载均衡。SSL 可以终结结在 F5 或原生的 Ingress Controller 上。随着内网安全的挑战不断增大，端到端的 SSL 连接越来越被企业接受，因此`}<strong parentName=\"p\">{`首选的部署模式`}</strong>{`还是终结在管理平面上。`}</p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`F5 设施上有 WAF 许可（ASM 或者 AWAF）。在这种条件下，可以直接启用 F5 设施上的 WAF 模块实现对管理平面的安全防护，提高安全性，简化了部署架构。`}</p>\n      </li>\n    </ul>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/F5-WAF/F5-conditions.jpg\",\n        \"alt\": \"架构图\"\n      }}></img></p>\n    <h3 {...{\n      \"id\": \"配置实现\"\n    }}>{`配置实现`}</h3>\n    <h4 {...{\n      \"id\": \"启用-asm（waf）模块\"\n    }}>{`启用 ASM（WAF）模块`}</h4>\n    <p>{`在 `}<inlineCode parentName=\"p\">{`system > Resource Provisioning`}</inlineCode>{` 界面启用 `}<inlineCode parentName=\"p\">{`ASM`}</inlineCode>{` 模块，单击 `}<inlineCode parentName=\"p\">{`submit`}</inlineCode>{` 提交即可，如下图所示：\n`}<img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/F5-WAF/start-asm-module.jpg\",\n        \"alt\": \"启用 ASM（WAF）模块\"\n      }}></img></p>\n    <h4 {...{\n      \"id\": \"配置-waf-策略\"\n    }}>{`配置 WAF 策略`}</h4>\n    <p>{`启用 ASM 之后，设备将出现 `}<inlineCode parentName=\"p\">{`security > Application Security`}</inlineCode>{` 界面，从界面新建 WAF 策略。`}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/F5-WAF/create-waf-strategy.jpg\",\n        \"alt\": \"启用 ASM（WAF）模块\"\n      }}></img></p>\n    <p>{`策略模板可以使用快速部署（Rapid Deployment Policy），由于本文主要关注部署架构，关于安全策略的细节将不进行详细的描述。`}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/F5-WAF/waf-strategy-template.jpg\",\n        \"alt\": \"策略模板\"\n      }}></img></p>\n    <h4 {...{\n      \"id\": \"绑定到-ltm-虚拟服务器（virtual-server）\"\n    }}>{`绑定到 LTM 虚拟服务器（Virtual Server）`}</h4>\n    <p>{`安全策略的绑定可在建立策略是直接选择，也可在 LTM 的属性中直接关联。创建策略时选择虚拟服务器，如下：`}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/F5-WAF/virtual-server.jpg\",\n        \"alt\": \"绑定到 LTM 虚拟服务器（Virtual Server）\"\n      }}></img></p>\n    <p>{`在虚拟服务器属性中关联：`}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/F5-WAF/virtual-server-binding.jpg\",\n        \"alt\": \"虚拟服务器（Virtual Server）关联安全策略\"\n      }}></img></p>\n    <p>{`完成关联后，系统将自动创建策略将访问流量引导给 ASM 模块进行处理：`}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/F5-WAF/redirect-traffic.jpg\",\n        \"alt\": \"访问流量引导给 ASM 模块\"\n      }}></img></p>\n    <h4 {...{\n      \"id\": \"验证访问\"\n    }}>{`验证访问`}</h4>\n    <p>{`使用浏览器访问集群管理平面，应该能见到 F5 ASM 插入的相关 cookie：`}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/F5-WAF/assert-cookie.jpg\",\n        \"alt\": \"相关cookie\"\n      }}></img></p>\n    <p>{`ASM 的安全策略页开始学习访问流量：`}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/F5-WAF/learn-traffic.jpg\",\n        \"alt\": \"学习访问流量\"\n      }}></img></p>\n    <h2 {...{\n      \"id\": \"外部-waf-实现管理平面安全防护\"\n    }}>{`外部 WAF 实现管理平面安全防护`}</h2>\n    <h3 {...{\n      \"id\": \"概述-1\"\n    }}>{`概述`}</h3>\n    <p>{`很多情况下，企业的安全设施会采用独立部署，通过一定的方式将流量牵引到安全设施进行相应的安全处理。流量牵引的方式很多，包含但不限于下面的方式：`}</p>\n    <table>\n      <thead parentName=\"table\">\n        <tr parentName=\"thead\">\n          <th parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`部署方式`}</th>\n          <th parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`优点`}</th>\n          <th parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`缺点`}</th>\n        </tr>\n      </thead>\n      <tbody parentName=\"table\">\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`负载均衡`}</td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`扩展性强，探活机制完善`}</td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`缺乏全局视野`}</td>\n        </tr>\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`路由设施`}</td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`成熟稳定`}</td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`基于 IP 地址，无服务概念`}</td>\n        </tr>\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`SDN 服务链`}</td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`网络编排，全局视野`}</td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`投入大，成熟度、兼容性不够`}</td>\n        </tr>\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`网络服务编排`}</td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`网络服务编排，动态服务链，扩展性强、探活机制完善`}</td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`局部视野`}</td>\n        </tr>\n      </tbody>\n    </table>\n    <p>{`网络服务编排的方式虽然比较新，但由于融合了负载均衡和服务链的能力，接受度不断提升。这里我们介绍如何利用 F5 的 SSLO 网络服务编排模块将管理平面的访问牵引到外部 WAF 资源池，实现管理平面的安全防护。`}</p>\n    <p>{`采用这种部署模式的优势显而易见：`}</p>\n    <ul>\n      <li parentName=\"ul\">{`独立安全设施，专业分工：交付设施专注交付，安全设施专注安全，边界清晰。`}</li>\n      <li parentName=\"ul\">{`WAF 多活部署：改变传统部署下 WAF 主备模式，ROI 成倍提升。`}</li>\n      <li parentName=\"ul\">{`弹性扩展，灰度升级：融合了负载均衡功能，WAF 可以池化扩展，设备可以灰度升级，流量可以灰度发布。`}</li>\n      <li parentName=\"ul\">{`旁路部署，便于运维：结合 F5 完善的探活机制，可随时隔离故障设备，运维灵活。`}</li>\n    </ul>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/F5-WAF/external.jpg\",\n        \"alt\": \"外部 WAF 架构\"\n      }}></img></p>\n    <h3 {...{\n      \"id\": \"前置条件\"\n    }}>{`前置条件`}</h3>\n    <ul>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`已经部署 F5 BIG-IP LTM 实现了 Rancher 管理平面的负载均衡。SSL 可以端结在 F5 上，也可以端结在原生的 Ingress Controller 上。`}</p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`F5 设备具备 SSLO 网络服务编排软件许可。`}</p>\n      </li>\n    </ul>\n    <h3 {...{\n      \"id\": \"配置实现-1\"\n    }}>{`配置实现`}</h3>\n    <h4 {...{\n      \"id\": \"启用-sslo-网络服务编排模块\"\n    }}>{`启用 SSLO 网络服务编排模块`}</h4>\n    <p>{`首先启用 SSLO 网络服务编排功能。在 `}<inlineCode parentName=\"p\">{`F5 System > Resource Provisioning`}</inlineCode>{` 菜单下，打开 `}<inlineCode parentName=\"p\">{`SSL Orchestrator`}</inlineCode>{` 功能，单击 `}<inlineCode parentName=\"p\">{`Submit`}</inlineCode>{` 提交即可。`}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/F5-WAF/SSLO-module.jpg\",\n        \"alt\": \"启用 SSLO 网络服务编排功能\"\n      }}></img></p>\n    <h4 {...{\n      \"id\": \"配置网络编排策略\"\n    }}>{`配置网络编排策略`}</h4>\n    <p>{`F5 的 SSLO 策略配置完全实现向导化，只需根据流程指引完成配置：`}</p>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`选择部署拓扑。`}</p>\n        <p parentName=\"li\">{`进入配置菜单 `}<inlineCode parentName=\"p\">{`SSL Orchestrator ›› Configuration`}</inlineCode>{`，选择部署拓扑 `}<inlineCode parentName=\"p\">{`“Existing Application”`}</inlineCode>{`，单击 `}<inlineCode parentName=\"p\">{`“Save & Next”`}</inlineCode>{`。`}</p>\n        <p parentName=\"li\"><img parentName=\"p\" {...{\n            \"src\": \"/img/rancher/F5-WAF/SSL-module-topology.jpg\",\n            \"alt\": \"部署拓扑\"\n          }}></img></p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`配置网络服务`}</p>\n        <p parentName=\"li\">{`进入 service 配置界面，选择 `}<inlineCode parentName=\"p\">{`“Generic Inline Layer 3”`}</inlineCode>{`，选择 `}<inlineCode parentName=\"p\">{`“Auto Manage Addresses”`}</inlineCode>{`，系统将给出相应的 IP 地址，VLAN 同样可以选择由系统新建。`}</p>\n        <p parentName=\"li\"><img parentName=\"p\" {...{\n            \"src\": \"/img/rancher/F5-WAF/SSLO-configuration-1.jpg\",\n            \"alt\": \"配置参数\"\n          }}></img></p>\n        <p parentName=\"li\">{`选择 `}<inlineCode parentName=\"p\">{`“Service Down Action”`}</inlineCode>{` 为 `}<inlineCode parentName=\"p\">{`“Ingore”`}</inlineCode>{`，失效自动屏蔽，添加 WAF 设备地址及其探活方式。\n`}<img parentName=\"p\" {...{\n            \"src\": \"/img/rancher/F5-WAF/SSLO-configuration-2.jpg\",\n            \"alt\": \"配置参数2\"\n          }}></img></p>\n        <p parentName=\"li\"><img parentName=\"p\" {...{\n            \"src\": \"/img/rancher/F5-WAF/SSLO-configuration-3.jpg\",\n            \"alt\": \"配置参数3\"\n          }}></img>{`\n保存，下一步进入服务链配置。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`配置服务链，将 步骤 2 中配置的 WAF 服务加入服务链即可。\n`}<img parentName=\"p\" {...{\n            \"src\": \"/img/rancher/F5-WAF/service-chain.jpg\",\n            \"alt\": \"配置参数\"\n          }}></img></p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`配置安全策略，编辑缺省的安全策略，添加服务链。单击保存，进入下一步。`}</p>\n        <p parentName=\"li\"><img parentName=\"p\" {...{\n            \"src\": \"/img/rancher/F5-WAF/security-strategy-1.jpg\",\n            \"alt\": \"配置安全策略1\"\n          }}></img></p>\n        <p parentName=\"li\"><img parentName=\"p\" {...{\n            \"src\": \"/img/rancher/F5-WAF/security-strategy-2.jpg\",\n            \"alt\": \"配置安全策略2\"\n          }}></img></p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`完成所有步骤后，提交部署。`}</p>\n        <p parentName=\"li\"><img parentName=\"p\" {...{\n            \"src\": \"/img/rancher/F5-WAF/submit-deployment.jpg\",\n            \"alt\": \"提交部署\"\n          }}></img></p>\n      </li>\n    </ol>\n    <h4 {...{\n      \"id\": \"绑定到-ltm-虚拟服务器（virtual-server）-1\"\n    }}>{`绑定到 LTM 虚拟服务器（Virtual Server）`}</h4>\n    <p>{`打开菜单 `}<inlineCode parentName=\"p\">{`Local Traffic ›› Virtual Servers : Virtual Server List ›› vs-rancher-443`}</inlineCode>{` 在 `}<inlineCode parentName=\"p\">{`Access Policy`}</inlineCode>{` 中，`}<inlineCode parentName=\"p\">{`Access Profile`}</inlineCode>{` 选择 `}<inlineCode parentName=\"p\">{`“ssloDefault_accessProfile”`}</inlineCode>{`，`}<inlineCode parentName=\"p\">{`Per-Request Ploicy`}</inlineCode>{` 选择 `}<inlineCode parentName=\"p\">{`“ssloP_existing_app_per_req_policy”`}</inlineCode>{` 即可。`}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/F5-WAF/virtual-server-binding-1.png\",\n        \"alt\": \"绑定到 LTM 虚拟服务器\"\n      }}></img></p>\n    <h4 {...{\n      \"id\": \"验证访问-1\"\n    }}>{`验证访问`}</h4>\n    <p>{`从管理客户端发起访问，观察客户端体验是否正常。观察 WAF 设备接收流量是否为明码的解密流量。`}</p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/rancher/F5-WAF/validate-access.png\",\n        \"alt\": \"绑定到 LTM 虚拟服务器\"\n      }}></img></p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}