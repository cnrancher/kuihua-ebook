D,CAFjE,CAI2B,uCAJ3B,CAhCK,CAqCL,SAAQ,CACN,KAAM,OADA,CAAR,CAEK,OAFL,CArCK,CAwCL,aAAG,kBAAG,UAAU,CAAC,GAAd,EAAsB,CACrB,OAAQ,mBADa,CAAtB,EAEI,OAFJ,CAAH,CAEoB,yFAFpB,CAE6G,kBAAG,UAAU,CAAC,GAAd,EAAsB,CAC/H,OAAQ,6BADuH,CAAtB,EAEtG,YAFsG,CAF7G,CAIyB,GAJzB,CAxCK,CA6CL,SAAQ,CACN,KAAM,gBADA,CAAR,CAEK,gBAFL,CA7CK,CAgDL,aAAG,kBAAG,UAAU,CAAC,GAAd,EAAsB,CACrB,OAAQ,iDADa,CAAtB,EAEI,gBAFJ,CAAH,CAE6B,oGAF7B,CAEiI,kBAAG,UAAU,CAAC,GAAd,EAAsB,CACnJ,OAAQ,0DAD2I,CAAtB,EAE1H,gBAF0H,CAFjI,CAI6B,GAJ7B,CAhDK,CAAP,CAsDD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '集群工具',\n\tdescription: 'Kubernetes 只提供了一个创建和管理容器的平台，Rancher 在此基础上集成了通知和告警、日志、监控等工具，让容器的运维和管理变得更加简单。同时，Rancher 也可以与外部服务集成，使集群运行更加高效。Rancher 内置的工具包括以下几类：通知和告警、日志、监控、Istio、OPA Gatekeeper。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'集群管理员指南',\n\t\t'集群工具'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/cluster-admin/tools/_index\",\n  \"title\": \"集群工具\",\n  \"description\": \"Kubernetes 只提供了一个创建和管理容器的平台，Rancher 在此基础上集成了通知和告警、日志、监控等工具，让容器的运维和管理变得更加简单。同时，Rancher 也可以与外部服务集成，使集群运行更加高效。Rancher 内置的工具包括以下几类：通知和告警、日志、监控、Istio、OPA Gatekeeper。\",\n  \"source\": \"@site/docs/rancher2/cluster-admin/tools/_index.md\",\n  \"permalink\": \"/docs/rancher2/cluster-admin/tools/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/cluster-admin/tools/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '通知和告警',\n  id: '通知和告警',\n  children: []\n}, {\n  value: '日志',\n  id: '日志',\n  children: []\n}, {\n  value: '监控',\n  id: '监控',\n  children: []\n}, {\n  value: 'Istio',\n  id: 'istio',\n  children: []\n}, {\n  value: 'OPA Gatekeeper',\n  id: 'opa-gatekeeper',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p>{`Kubernetes 只提供了一个创建和管理容器的平台，Rancher 在此基础上集成了通知和告警、日志、监控等工具，让容器的运维和管理变得更加简单。同时，Rancher 也可以与外部服务集成，使集群运行更加高效。Rancher 内置的工具包括以下几类：`}</p>\n    <h2 {...{\n      \"id\": \"通知和告警\"\n    }}>{`通知和告警`}</h2>\n    <p>{`通知和告警是两个协同工作的功能，向您通知 Rancher 系统中的事件。`}</p>\n    <p><a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/monitoring-alerting/2.0-2.4/notifiers/_index\"\n      }}>{`通知`}</a>{`是通知您告警事件的服务。您可以配置通知接收者，以将告警通知发送给最适合采取措施的人员。目前 Rancher 支持的通知发送方式包括：Slack、电子邮件、PagerDuty、微信、钉钉和 Webhooks。`}</p>\n    <p><a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/monitoring-alerting/2.0-2.4/cluster-alerts/_index\"\n      }}>{`告警`}</a>{`是触发通知的规则。在接收告警之前，必须在 Rancher 中配置一个或多个通知接收者。告警范围可以在集群或项目级别设置。`}</p>\n    <h2 {...{\n      \"id\": \"日志\"\n    }}>{`日志`}</h2>\n    <p>{`日志服务提供了以下功能：`}</p>\n    <ul>\n      <li parentName=\"ul\">{`捕获并分析集群的状态`}</li>\n      <li parentName=\"ul\">{`在您的环境中分析趋势，寻找集群变化的规律`}</li>\n      <li parentName=\"ul\">{`将日志保存到集群外的安全位置`}</li>\n      <li parentName=\"ul\">{`随时了解容器崩溃，Pod 驱逐或节点死亡等事件`}</li>\n      <li parentName=\"ul\">{`更轻松地调试和排除故障`}</li>\n    </ul>\n    <p>{`Rancher 可以与 Elasticsearch、Splunk、Kafka、Syslog 和 Fluentd 集成。`}</p>\n    <p>{`请参考`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/logging/2.0.x-2.4.x/project-logging/_index\"\n      }}>{`集群日志`}</a>{`，获取配置 Rancher 和外部日志服务集成的详细操作步骤。`}</p>\n    <h2 {...{\n      \"id\": \"监控\"\n    }}>{`监控`}</h2>\n    <p><em parentName=\"p\">{`自 v2.2.0 起可用`}</em></p>\n    <p>{`您可以通过领先的开源监控解决方案 `}<a parentName=\"p\" {...{\n        \"href\": \"https://prometheus.io/\"\n      }}>{`Prometheus`}</a>{` 监控集群节点、Kubernetes 系统组件、软件部署的状态和过程。请参考`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/tools/monitoring/_index\"\n      }}>{`Rancher 监控概述`}</a>{`，获取配置 Rancher 和 Prometheus 对接的详细操作步骤。`}</p>\n    <h2 {...{\n      \"id\": \"istio\"\n    }}>{`Istio`}</h2>\n    <p><a parentName=\"p\" {...{\n        \"href\": \"https://istio.io/\"\n      }}>{`Istio`}</a>{` 是一个开源工具，可使 DevOps 团队更轻松地观察，控制，排查故障和在复杂的微服务网络中的保证流量传输。有关如何在 Rancher 中启用 Istio 的详细信息，请参阅`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/istio/_index\"\n      }}>{`Istio 功能介绍`}</a>{`。`}</p>\n    <h2 {...{\n      \"id\": \"opa-gatekeeper\"\n    }}>{`OPA Gatekeeper`}</h2>\n    <p><a parentName=\"p\" {...{\n        \"href\": \"https://github.com/open-policy-agent/gatekeeper\"\n      }}>{`OPA Gatekeeper`}</a>{` 是一个开源项目，它对 OPA 和 Kubernetes 进行了集成，以通过准入控制器 Webhooks 提供策略控制。有关如何在 Rancher 中启用 Gatekeeper 的详细信息，请参阅`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/cluster-admin/tools/opa-gatekeeper/_index\"\n      }}>{`OPA Gatekeeper`}</a>{`。`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}