kD,CAAtB,EAEjC,QAFiC,CAAxC,CAEqB,GAFrB,CAtIK,CAAP,CA0ID,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '手动快速部署',\n\tdescription: '手动快速部署 Rancher Server 包括以下四个步骤。',\n\tkeywords: [\n\t\t'rancher 2.0中文文档',\n\t\t'rancher 2.x 中文文档',\n\t\t'rancher中文',\n\t\t'rancher 2.0中文',\n\t\t'rancher2',\n\t\t'rancher教程',\n\t\t'rancher中国',\n\t\t'rancher 2.0',\n\t\t'rancher2.0 中文教程',\n\t\t'快速入门',\n\t\t'部署Rancher Server',\n\t\t'手动快速部署'\n\t]\n};\nexport const metadata = {\n  \"id\": \"rancher2/quick-start-guide/deployment/quickstart-manual-setup/_index\",\n  \"title\": \"手动快速部署\",\n  \"description\": \"手动快速部署 Rancher Server 包括以下四个步骤。\",\n  \"source\": \"@site/docs/rancher2/quick-start-guide/deployment/quickstart-manual-setup/_index.md\",\n  \"permalink\": \"/docs/rancher2/quick-start-guide/deployment/quickstart-manual-setup/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/rancher2/quick-start-guide/deployment/quickstart-manual-setup/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar\": \"rancher2\",\n  \"previous\": {\n    \"title\": \"概述\",\n    \"permalink\": \"/docs/rancher2/quick-start-guide/deployment/_index\"\n  },\n  \"next\": {\n    \"title\": \"AWS 快速部署\",\n    \"permalink\": \"/docs/rancher2/quick-start-guide/deployment/amazon-aws-qs/_index\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '概述',\n  id: '概述',\n  children: []\n}, {\n  value: '运行 Linux 主机:',\n  id: '运行-linux-主机',\n  children: []\n}, {\n  value: '安装 Rancher 2.x',\n  id: '安装-rancher-2x',\n  children: []\n}, {\n  value: '登录 Rancher 界面并配置初始设置',\n  id: '登录-rancher-界面并配置初始设置',\n  children: []\n}, {\n  value: '创建业务集群',\n  id: '创建业务集群',\n  children: []\n}, {\n  value: '后续操作',\n  id: '后续操作',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <h2 {...{\n      \"id\": \"概述\"\n    }}>{`概述`}</h2>\n    <p>{`手动快速部署 Rancher Server 包括以下四个步骤：`}</p>\n    <ul>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"#%E8%BF%90%E8%A1%8C-linux-%E4%B8%BB%E6%9C%BA\"\n        }}>{`运行 Linux 主机`}</a></li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"#%E5%AE%89%E8%A3%85-rancher-2x\"\n        }}>{`安装 Rancher 2.x`}</a></li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"#%E7%99%BB%E5%BD%95-rancher-%E7%95%8C%E9%9D%A2%E5%B9%B6%E9%85%8D%E7%BD%AE%E5%88%9D%E5%A7%8B%E8%AE%BE%E7%BD%AE\"\n        }}>{`登录 Rancher 2.x 并配置初始设置`}</a></li>\n      <li parentName=\"ul\"><a parentName=\"li\" {...{\n          \"href\": \"#%E5%88%9B%E5%BB%BA%E4%B8%9A%E5%8A%A1%E9%9B%86%E7%BE%A4\"\n        }}>{`创建业务集群`}</a></li>\n    </ul>\n    <h2 {...{\n      \"id\": \"运行-linux-主机\"\n    }}>{`运行 Linux 主机:`}</h2>\n    <p>{`首先，您需要运行一台 Linux 主机，Rancher 支持的 Linux 主机类型包括：`}</p>\n    <ul>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`云端虚拟机（如 Amazon ECS、阿里云 ECS、腾讯云 CVM、华为云 ECS 等）`}</p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`本地数据中心的虚拟机`}</p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`裸金属服务器（BMS）`}</p>\n        <blockquote parentName=\"li\">\n          <p parentName=\"blockquote\"><strong parentName=\"p\">{`说明：`}</strong></p>\n          <p parentName=\"blockquote\">{`如果您使用的是云端虚拟机，请开放 `}<strong parentName=\"p\">{`80 端口`}</strong>{`和 `}<strong parentName=\"p\">{`443 端口`}</strong>{`的流入流量，详情请参考云服务提供商的文档。`}</p>\n          <p parentName=\"blockquote\">{`有关业务集群的端口的详细要求，请参考`}<a parentName=\"p\" {...{\n              \"href\": \"/docs/rancher2/cluster-provisioning/node-requirements/_index\"\n            }}>{`下游集群节点要求`}</a>{`。`}</p>\n        </blockquote>\n        <p parentName=\"li\">{`请按照`}<a parentName=\"p\" {...{\n            \"href\": \"/docs/rancher2/installation/requirements/_index\"\n          }}>{`具体要求`}</a>{`配置 Linux 主机。`}</p>\n      </li>\n    </ul>\n    <h2 {...{\n      \"id\": \"安装-rancher-2x\"\n    }}>{`安装 Rancher 2.x`}</h2>\n    <p>{`首先连接到主机，然后使用 shell 安装 Rancher。`}</p>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`使用 shell 工具（如 PuTTy 或其他连接工具）登录 Linux 主机。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`执行以下命令：\n如果您使用的 Rancher 2.5.x 及更新版本，需要开启特权模式安装 Rancher，请执行以下命令：`}</p>\n        <pre parentName=\"li\"><code parentName=\"pre\" {...{\n            \"className\": \"language-bash\"\n          }}>{`#如果您使用的 Rancher 2.5.x 及更新版本，需要开启特权模式安装 Rancher，请执行以下命令：\ndocker run -d --restart=unless-stopped \\\\\n  -p 80:80 -p 443:443 \\\\\n  --privileged \\\\\n  rancher/rancher:latest\n`}</code></pre>\n        <p parentName=\"li\">{`如果您使用的 Rancher 2.4.x 及之前的版本，请执行以下命令：`}</p>\n        <pre parentName=\"li\"><code parentName=\"pre\" {...{\n            \"className\": \"language-bash\"\n          }}>{`#如果您使用的 Rancher 2.4.x 及之前的版本，请执行以下命令：\nsudo docker run -d --restart=unless-stopped -p 80:80 -p 443:443 rancher/rancher\n`}</code></pre>\n      </li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{` Rancher 已经安装在了 Linux 主机上。`}</p>\n    <h2 {...{\n      \"id\": \"登录-rancher-界面并配置初始设置\"\n    }}>{`登录 Rancher 界面并配置初始设置`}</h2>\n    <p>{`您需要先登录 Rancher，然后再开始使用 Rancher。登录以后，您需要完成一些一次性的配置。`}</p>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`打开浏览器，输入主机的 IP 地址：`}<inlineCode parentName=\"p\">{`https://<SERVER_IP>`}</inlineCode></p>\n        <p parentName=\"li\">{`请使用真实的主机 IP 地址替换 `}<inlineCode parentName=\"p\">{`<SERVER_IP>`}</inlineCode>{` 。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`首次登录时，请按照页面提示设置登录密码。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`设置 `}<strong parentName=\"p\">{`Rancher Server URL`}</strong>{`。URL 既可以是一个 IP 地址，也可以是一个主机名称。请确保您在集群内添加的每个节点都可以连接到这个 URL。如果您使用的是主机名称，请保证主机名称可以被节点的 DNS 服务器成功解析。`}</p>\n      </li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{`完成 Rancher 管理员用户的密码设置和访问地址设置。下次使用 Rancher 时，可以输入 IP 地址或主机地址访问 Rancher 界面，然后输入管理员用户名`}<inlineCode parentName=\"p\">{`admin`}</inlineCode>{`和您设置的密码登录 Rancher 界面。`}</p>\n    <h2 {...{\n      \"id\": \"创建业务集群\"\n    }}>{`创建业务集群`}</h2>\n    <p>{`完成安装和登录 Rancher 的步骤之后，您现在可以参考以下步骤，在 Rancher 中创建第一个 Kubernetes 集群。`}</p>\n    <p>{`在这个任务中，您可以使用`}<strong parentName=\"p\">{`自定义集群`}</strong>{`选项，使用的`}<strong parentName=\"p\">{`任意`}</strong>{` Linux 主机（云主机、虚拟机或裸金属服务器）创建集群。`}</p>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`访问`}<strong parentName=\"p\">{`集群`}</strong>{`页面，单击`}<strong parentName=\"p\">{`添加集群`}</strong>{`。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`选择`}<strong parentName=\"p\">{`自定义`}</strong>{`选项。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`输入`}<strong parentName=\"p\">{`集群名称`}</strong>{`。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`跳过`}<strong parentName=\"p\">{`集群角色`}</strong>{`和`}<strong parentName=\"p\">{`集群选项`}</strong>{`。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`单击`}<strong parentName=\"p\">{`下一步`}</strong>{`。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`勾选`}<strong parentName=\"p\">{`主机选项 - 角色选择`}</strong>{`中的所有角色： `}<strong parentName=\"p\">{`Etcd`}</strong>{`、 `}<strong parentName=\"p\">{`Control`}</strong>{` 和 `}<strong parentName=\"p\">{`Worker`}</strong>{`。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\"><strong parentName=\"p\">{`可选：`}</strong>{` Rancher 会自动探查用于 Rancher 通信和集群通信的 IP 地址。您可以通过`}<strong parentName=\"p\">{`主机选项 > 显示高级选项`}</strong>{`中的`}<inlineCode parentName=\"p\">{`公网地址`}</inlineCode>{`和`}<inlineCode parentName=\"p\">{`内网地址`}</inlineCode>{`指定 IP 地址。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`跳过`}<strong parentName=\"p\">{`主机标签`}</strong>{`参数，因为对快速入门来说，这部分的参数不太重要。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`复制代码框中的命令。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`登录您的 Linux 主机，打开命令行工具，粘贴命令，单击回车键运命令。`}</p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`运行完成后，回到 Rancher 界面，单击`}<strong parentName=\"p\">{`完成`}</strong>{`。`}</p>\n      </li>\n    </ol>\n    <p><strong parentName=\"p\">{`结果：`}</strong>{` 在 Rancher 中创建了一个 Kubernetes 集群。`}</p>\n    <h2 {...{\n      \"id\": \"后续操作\"\n    }}>{`后续操作`}</h2>\n    <p>{`创建了集群后，您可以使用 Rancher 部署工作负载，详情请参考`}<a parentName=\"p\" {...{\n        \"href\": \"/docs/rancher2/quick-start-guide/workload/_index\"\n      }}>{`部署工作负载`}</a>{`。`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}