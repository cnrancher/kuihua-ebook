UAAU,CAAC,YAAd,EAA6B,iCAA7B,CAA8D,kBAAY,UAAU,CAAC,GAAvB,EAA6B,SAA7B,CAA9D,CAAmH,gBAAnH,CADF,CA9IK,CAiJL,aAAK,gCAAL,CAjJK,CAkJL,SAAQ,CACN,KAAM,aADA,CAAR,CAEK,aAFL,CAlJK,CAqJL,aAAK;8CAAL,CArJK,CAuJL,aAAK,MAAL,CAAW,kBAAY,UAAU,CAAC,GAAvB,EAA6B,qDAA7B,CAAX,CAvJK,CAwJL,aAAK,oBAAL,CAxJK,CAyJL,aAAK,MAAL,CAAW,kBAAY,UAAU,CAAC,GAAvB,EAA6B,mEAA7B,CAAX,CAzJK,CA0JL,aAAK,kBAAL,CA1JK,CA2JL,aAAK,MAAL,CAAW,kBAAY,UAAU,CAAC,GAAvB,EAA6B,8CAA7B,CAAX,CA3JK,CAAP,CA6JD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: '私有镜像仓库配置参考',\n\tdescription: '您可以配置 Containerd 连接到私有镜像仓库，并使用它们在节点上拉取私有镜像。',\n\tkeywords: [\n\t\t'k3s中文文档',\n\t\t'k3s 中文文档',\n\t\t'k3s中文',\n\t\t'k3s 中文',\n\t\t'k3s',\n\t\t'k3s教程',\n\t\t'k3s中国',\n\t\t'rancher',\n\t\t'k3s 中文教程',\n\t\t'私有镜像仓库配置参考'\n\t]\n};\nexport const metadata = {\n  \"id\": \"k3s/installation/private-registry/_index\",\n  \"title\": \"私有镜像仓库配置参考\",\n  \"description\": \"您可以配置 Containerd 连接到私有镜像仓库，并使用它们在节点上拉取私有镜像。\",\n  \"source\": \"@site/docs/k3s/installation/private-registry/_index.md\",\n  \"permalink\": \"/docs/k3s/installation/private-registry/_index\",\n  \"editUrl\": \"https://github.com/cnrancher/docs-rancher2/edit/master/docs/k3s/installation/private-registry/_index.md\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '镜像仓库配置文件',\n  id: '镜像仓库配置文件',\n  children: [{\n    value: 'Mirrors',\n    id: 'mirrors',\n    children: []\n  }, {\n    value: 'Configs',\n    id: 'configs',\n    children: []\n  }, {\n    value: '使用 TLS',\n    id: '使用-tls',\n    children: []\n  }, {\n    value: '不使用 TLS',\n    id: '不使用-tls',\n    children: []\n  }]\n}, {\n  value: '添加镜像到私有镜像仓库',\n  id: '添加镜像到私有镜像仓库',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p><em parentName=\"p\">{`v1.0.0 开始可用`}</em></p>\n    <p>{`可以配置 Containerd 连接到私有镜像仓库，并使用它们在节点上拉取私有镜像。`}</p>\n    <p>{`启动时，K3s 会检查`}<inlineCode parentName=\"p\">{`/etc/rancher/k3s/`}</inlineCode>{`中是否存在`}<inlineCode parentName=\"p\">{`registries.yaml`}</inlineCode>{`文件，并指示 containerd 使用文件中定义的镜像仓库。如果你想使用一个私有的镜像仓库，那么你需要在每个使用镜像仓库的节点上以 root 身份创建这个文件。`}</p>\n    <p>{`请注意，server 节点默认是可以调度的。如果你没有在 server 节点上设置污点，那么将在它们上运行工作负载，请确保在每个 server 节点上创建`}<inlineCode parentName=\"p\">{`registries.yaml`}</inlineCode>{`文件。`}</p>\n    <p>{`Containerd 中的配置可以用于通过 TLS 连接到私有镜像仓库，也可以与启用验证的镜像仓库连接。下一节将解释`}<inlineCode parentName=\"p\">{`registries.yaml`}</inlineCode>{`文件，并给出在 K3s 中使用私有镜像仓库配置的不同例子。`}</p>\n    <h2 {...{\n      \"id\": \"镜像仓库配置文件\"\n    }}>{`镜像仓库配置文件`}</h2>\n    <p>{`该文件由两大部分组成：`}</p>\n    <ul>\n      <li parentName=\"ul\">{`mirrors`}</li>\n      <li parentName=\"ul\">{`configs`}</li>\n    </ul>\n    <h3 {...{\n      \"id\": \"mirrors\"\n    }}>{`Mirrors`}</h3>\n    <p>{`Mirrors 是一个用于定义专用镜像仓库的名称和 endpoint 的指令，例如。`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`mirrors:\n  mycustomreg.com:\n    endpoint:\n      - \"https://mycustomreg.com:5000\"\n`}</code></pre>\n    <p>{`每个 mirror 必须有一个名称和一组 endpoint。当从镜像仓库中拉取镜像时，containerd 会逐一尝试这些 endpoint URL，并使用第一个可用的 endpoint。`}</p>\n    <h3 {...{\n      \"id\": \"configs\"\n    }}>{`Configs`}</h3>\n    <p>{`Configs 部分定义了每个 mirror 的 TLS 和证书配置。对于每个 mirror，你可以定义`}<inlineCode parentName=\"p\">{`auth`}</inlineCode>{`和/或`}<inlineCode parentName=\"p\">{`tls`}</inlineCode>{`。TLS 部分包括：`}</p>\n    <table>\n      <thead parentName=\"table\">\n        <tr parentName=\"thead\">\n          <th parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`指令`}</th>\n          <th parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`描述`}</th>\n        </tr>\n      </thead>\n      <tbody parentName=\"table\">\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}><inlineCode parentName=\"td\">{`cert_file`}</inlineCode></td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`用来与镜像仓库进行验证的客户证书路径`}</td>\n        </tr>\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}><inlineCode parentName=\"td\">{`key_file`}</inlineCode></td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`用来验证镜像仓库的客户端密钥路径`}</td>\n        </tr>\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}><inlineCode parentName=\"td\">{`ca_file`}</inlineCode></td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`定义用于验证镜像仓库服务器证书文件的 CA 证书路径`}</td>\n        </tr>\n        <tr parentName=\"tbody\">\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}><inlineCode parentName=\"td\">{`insecure_skip_verify`}</inlineCode></td>\n          <td parentName=\"tr\" {...{\n            \"align\": \"left\"\n          }}>{`定义是否应跳过注册表的TLS验证的布尔值`}</td>\n        </tr>\n      </tbody>\n    </table>\n    <p>{`凭证由用户名/密码或认证 token 组成：`}</p>\n    <ul>\n      <li parentName=\"ul\">{`username： 镜像仓库身份验证的用户名`}</li>\n      <li parentName=\"ul\">{`password： 镜像仓库身份验证的用户密码`}</li>\n      <li parentName=\"ul\">{`auth： 镜像仓库 auth 的认证 token`}</li>\n    </ul>\n    <p>{`以下是在不同模式下使用私有镜像仓库的基本例子：`}</p>\n    <h3 {...{\n      \"id\": \"使用-tls\"\n    }}>{`使用 TLS`}</h3>\n    <p>{`下面的例子展示了当你使用 TLS 时，如何在每个节点上配置`}<inlineCode parentName=\"p\">{`/etc/rancher/k3s/registries.yaml`}</inlineCode>{`。`}</p>\n    <h4 {...{\n      \"id\": \"有认证\"\n    }}>{`有认证`}</h4>\n    <pre><code parentName=\"pre\" {...{}}>{`mirrors:\n  docker.io:\n    endpoint:\n      - \"https://mycustomreg.com:5000\"\nconfigs:\n  \"mycustomreg:5000\":\n    auth:\n      username: xxxxxx # 这是私有镜像仓库的用户名\n      password: xxxxxx # 这是私有镜像仓库的密码\n    tls:\n      cert_file: # 镜像仓库中使用的cert文件的路径。\n      key_file:  # 镜像仓库中使用的key文件的路径。\n      ca_file:   # 镜像仓库中使用的ca文件的路径。\n`}</code></pre>\n    <h4 {...{\n      \"id\": \"无认证\"\n    }}>{`无认证`}</h4>\n    <pre><code parentName=\"pre\" {...{}}>{`mirrors:\n  docker.io:\n    endpoint:\n      - \"https://mycustomreg.com:5000\"\nconfigs:\n  \"mycustomreg:5000\":\n    tls:\n      cert_file: # 镜像仓库中使用的cert文件的路径。\n      key_file:  # 镜像仓库中使用的key文件的路径。\n      ca_file:   # 镜像仓库中使用的ca文件的路径。\n`}</code></pre>\n    <h3 {...{\n      \"id\": \"不使用-tls\"\n    }}>{`不使用 TLS`}</h3>\n    <p>{`下面的例子展示了当你不使用 TLS 时，如何在每个节点上配置`}<inlineCode parentName=\"p\">{`/etc/rancher/k3s/registries.yaml`}</inlineCode>{`。`}</p>\n    <h4 {...{\n      \"id\": \"有认证-1\"\n    }}>{`有认证`}</h4>\n    <pre><code parentName=\"pre\" {...{}}>{`mirrors:\n  docker.io:\n    endpoint:\n      - \"http://mycustomreg.com:5000\"\nconfigs:\n  \"mycustomreg:5000\":\n    auth:\n      username: xxxxxx # 这是私有镜像仓库的用户名\n      password: xxxxxx # 这是私有镜像仓库的密码\n`}</code></pre>\n    <h4 {...{\n      \"id\": \"无认证-1\"\n    }}>{`无认证`}</h4>\n    <pre><code parentName=\"pre\" {...{}}>{`mirrors:\n  docker.io:\n    endpoint:\n      - \"http://mycustomreg.com:5000\"\n`}</code></pre>\n    <blockquote>\n      <p parentName=\"blockquote\">{`在没有 TLS 通信的情况下，需要为 endpoints 指定`}<inlineCode parentName=\"p\">{`http://`}</inlineCode>{`，否则将默认为 https。`}</p>\n    </blockquote>\n    <p>{`为了使镜像仓库更改生效，你需要重新启动每个节点上的 K3s。`}</p>\n    <h2 {...{\n      \"id\": \"添加镜像到私有镜像仓库\"\n    }}>{`添加镜像到私有镜像仓库`}</h2>\n    <p>{`首先，从 GitHub 上获取你正在使用的版本的 k3s-images.txt 文件。\n从 docker.io 中拉取 k3s-images.txt 文件中列出的 K3s 镜像。`}</p>\n    <p>{`示例： `}<inlineCode parentName=\"p\">{`docker pull docker.io/rancher/coredns-coredns:1.6.3`}</inlineCode></p>\n    <p>{`然后，将镜像重新标记成私有镜像仓库。`}</p>\n    <p>{`示例： `}<inlineCode parentName=\"p\">{`docker tag coredns-coredns:1.6.3 mycustomreg:5000/coredns-coredns`}</inlineCode></p>\n    <p>{`最后，将镜像推送到私有镜像仓库。`}</p>\n    <p>{`示例： `}<inlineCode parentName=\"p\">{`docker push mycustomreg:5000/coredns-coredns`}</inlineCode></p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}